                                                        x:Name="AvoidShapesCheckBoxSimplified"
                                                        helpers:ThemeHelper.FocusVisualMargin="0"/>
                                                <CheckBox
                                                        Content="{telerik:LocalizableResource Key=DiagramRibbon_AvoidConnectionOverlap}"

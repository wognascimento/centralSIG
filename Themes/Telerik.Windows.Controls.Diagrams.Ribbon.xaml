<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:ribbon="clr-namespace:Telerik.Windows.Controls.Diagrams.Ribbon;assembly=Telerik.Windows.Controls.Diagrams.Ribbon"
        xmlns:localization="clr-namespace:Telerik.Windows.Controls.Localization;assembly=Telerik.Windows.Controls"
        xmlns:telerik1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
        xmlns:ribbonPrimitives="clr-namespace:Telerik.Windows.Controls.RibbonView;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:telerikRibbon="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.RibbonView"
        xmlns:diagram="clr-namespace:Telerik.Windows.Controls.Diagrams;assembly=Telerik.Windows.Controls.Diagrams"
        xmlns:extensions="clr-namespace:Telerik.Windows.Controls.Diagrams.Extensions;assembly=Telerik.Windows.Controls.Diagrams.Extensions"
        xmlns:telerikNavigation="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Navigation"
        xmlns:helpers="clr-namespace:Telerik.Windows.Controls.Theming.Helpers;assembly=Telerik.Windows.Controls"
        xmlns:telerikInput="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Input"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:declaratives="clr-namespace:Telerik.Windows.Diagrams.Core;assembly=Telerik.Windows.Diagrams.Core">
    <ResourceDictionary.MergedDictionaries>
        <telerik:Windows11ResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <FontFamily x:Key="TelerikWebUI">/Telerik.Windows.Controls;component/Themes/Fonts/old/TelerikWebUI.ttf#TelerikWebUI</FontFamily>
    <sys:Double x:Key="DiagramRibbonNumericUpDownWidth">110</sys:Double>
    <telerik:IconSources x:Key="IconPaths"
            LightBasePath="/Telerik.Windows.Controls.Diagrams.Ribbon;component/Images/Light/"
            ModernBasePath="/Telerik.Windows.Controls.Diagrams.Ribbon;component/Images/Light/"
            DarkBasePath="/Telerik.Windows.Controls.Diagrams.Ribbon;component/Images/Dark/"/>
    <ribbon:PercentageConverter x:Key="PercentConverter"/>
    <ribbon:NullToBooleanConverter x:Key="NullToBooleanConverter"/>
    <telerik:VisibilityToBooleanConverter x:Key="VisibilityToBooleanConverter"/>
    <telerik:InvertedBooleanConverter x:Key="InvertedBooleanConverter"/>
    <telerik:BrushToColorConverter x:Key="BrushToColorConverter"/>
    <telerik:LocalizationManager x:Key="LocalizationManagerProvider"/>
    <localization:CultureToLocalizationStringConverter x:Key="CultureToStringConverter"/>
    <ribbon:ActiveToolToBooleanConverter x:Key="ActiveToolToBooleanConverter"/>
    <DataTemplate x:Key="UndoItemTemplate">
        <TextBlock Margin="3" Text="{Binding Name}"/>
    </DataTemplate>
    <DataTemplate x:Key="ConnectionBridgeTemplate">
        <StackPanel Orientation="Horizontal">
            <Border Background="Transparent" Margin="2">
                <Image Width="16" Height="16" Source="{Binding IconPath}"/>
            </Border>
            <TextBlock Margin="3" Text="{Binding BridgeType}"/>
        </StackPanel>
    </DataTemplate>
    <Style x:Key="DiagramRibbonTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="{telerik:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="DiagramRibbonSeparatorStyle" TargetType="telerik:Separator">
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="Width" Value="1"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:Separator">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" Value="{telerik:Windows11Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadDiagramRibbonStyle" TargetType="ribbon:RadDiagramRibbon">
        <Setter Property="ribbonPrimitives:KeyTipService.IsKeyTipsEnabled" Value="True"/>
        <Setter Property="LayoutModeButtonContent" Value="{Binding Manager.Culture, Source={StaticResource LocalizationManagerProvider}, Converter={StaticResource CultureToStringConverter}, ConverterParameter=RibbonViewLayoutModeButtonContent}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ribbon:RadDiagramRibbon">
                    <telerikRibbon:RadRibbonView x:Name="PART_RibbonView"
                            ribbonPrimitives:KeyTipService.IsKeyTipsEnabled="{TemplateBinding ribbonPrimitives:KeyTipService.IsKeyTipsEnabled}"
                            IsEnabled="{Binding Diagram, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource NullToBooleanConverter}}"
                            MinimizeButtonVisibility="Visible"
                            TitleBarVisibility="{TemplateBinding TitleBarVisibility}"
                            BackstageClippingElement="{TemplateBinding BackstageClippingElement}"
                            Backstage="{TemplateBinding Backstage}"
                            QuickAccessToolBar="{TemplateBinding QuickAccessToolBar}"
                            QuickAccessToolBarPosition="{TemplateBinding QuickAccessToolBarPosition}"
                            ApplicationButtonContent="{TemplateBinding ApplicationButtonContent}"
                            ApplicationButtonImageSource="{TemplateBinding ApplicationButtonImageSource}"
                            CollapseThresholdSize="300, 100"
                            LayoutMode="{TemplateBinding LayoutMode}"
                            LayoutModeButtonContent="{TemplateBinding LayoutModeButtonContent}"
                            ShowLayoutModeButton="{TemplateBinding ShowLayoutModeButton}">
                        <telerikRibbon:RadRibbonTab Header="{telerik:LocalizableResource Key=DiagramRibbon_HomeTab}" IsSelected="True" ribbonPrimitives:KeyTipService.AccessText="H">
                            <telerikRibbon:RadRibbonGroup
                                    Header="{telerik:LocalizableResource Key=DiagramRibbon_General}"
                                    x:Name="generalGroup"
                                    telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_GeneralDescription}"
                                    telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_General}"
                                    Icon="{telerik:IconResource IconRelativePath=16/CollapsedGroupIcons/home_general.png, IconSources={StaticResource IconPaths}}">
                                <telerikRibbon:RadRibbonButton x:Name="NewButton"
                                        Command="diagram:DiagramCommands.Clear"
                                        ribbonPrimitives:KeyTipService.AccessText="N"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        LargeImage="{telerik:IconResource IconRelativePath=new.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_New}"/>
                                <telerikRibbon:RadRibbonButton x:Name="OpenButton"
                                        Command="diagram:DiagramCommands.Open"
                                        ribbonPrimitives:KeyTipService.AccessText="O"
                                        CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                        LargeImage="{telerik:IconResource IconRelativePath=open.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Open}"/>
                                <telerikRibbon:RadRibbonButton x:Name="SaveButton"
                                        Command="diagram:DiagramCommands.Save"
                                        ribbonPrimitives:KeyTipService.AccessText="S"
                                        CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                        LargeImage="{telerik:IconResource IconRelativePath=save.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Save}"/>
                                <telerik:RadCollapsiblePanel x:Name="undoRedoPanel">
                                    <telerikRibbon:RadRibbonSplitButton
                                            Command="diagram:DiagramCommands.Undo"
                                            x:Name="UndoButton"
                                            ribbonPrimitives:KeyTipService.AccessText="U"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            CollapseToSmall="WhenGroupIsSmall"
                                            LargeImage="{telerik:IconResource IconRelativePath=Undo.png, IconSources={StaticResource IconPaths}}"
                                            Size="Medium"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_Undo}">
                                        <telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                            <telerik:RadListBox
                                                    MaxHeight="500"
                                                    BorderThickness="0"
                                                    ItemsSource="{Binding Diagram.UndoRedoService.UndoStack, RelativeSource={RelativeSource TemplatedParent}}"
                                                    ItemTemplate="{StaticResource UndoItemTemplate}"
                                                    x:Name="UndoStackListBox"/>
                                        </telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                    </telerikRibbon:RadRibbonSplitButton>
                                    <telerikRibbon:RadRibbonSplitButton
                                            Command="diagram:DiagramCommands.Redo"
                                            x:Name="RedoButton"
                                            ribbonPrimitives:KeyTipService.AccessText="R"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            CollapseToSmall="WhenGroupIsSmall"
                                            LargeImage="{telerik:IconResource IconRelativePath=Redo.png, IconSources={StaticResource IconPaths}}"
                                            Size="Medium"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_Redo}">
                                        <telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                            <telerik:RadListBox
                                                    MaxHeight="500"
                                                    BorderThickness="0"
                                                    ItemsSource="{Binding Diagram.UndoRedoService.RedoStack, RelativeSource={RelativeSource TemplatedParent}}"
                                                    ItemTemplate="{StaticResource UndoItemTemplate}"
                                                    x:Name="RedoStackListBox"/>
                                        </telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                    </telerikRibbon:RadRibbonSplitButton>
                                </telerik:RadCollapsiblePanel>
                            </telerikRibbon:RadRibbonGroup>
                            <telerikRibbon:RadRibbonGroup
                                    Header="{telerik:LocalizableResource Key=DiagramRibbon_Clipboard}"
                                    ribbonPrimitives:KeyTipService.AccessText="FO"
                                    x:Name="clipBoardGroup"
                                    Icon="{telerik:IconResource IconRelativePath=16/CollapsedGroupIcons/home_clipboard.png, IconSources={StaticResource IconPaths}}"
                                    telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_ClipboardDescription}"
                                    telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Clipboard}">
                                <telerikRibbon:RadRibbonButton x:Name="PasteButton"
                                        Command="diagram:DiagramCommands.Paste"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        LargeImage="{telerik:IconResource IconRelativePath=paste.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        SmallImage="{telerik:IconResource IconRelativePath=paste16.png, IconSources={StaticResource IconPaths}}"
                                        ribbonPrimitives:KeyTipService.AccessText="V"
                                        telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_PasteDescription}"
                                        telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Paste}"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Paste}"/>
                                <telerik:RadCollapsiblePanel x:Name="cutCopyDeletePanel">
                                    <telerikRibbon:RadRibbonButton x:Name="CutButton"
                                            Command="diagram:DiagramCommands.Cut"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            CollapseToSmall="WhenGroupIsSmall"
                                            SmallImage="{telerik:IconResource IconRelativePath=cut.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="X"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_CutDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Cut}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_Cut}"/>
                                    <telerikRibbon:RadRibbonButton x:Name="CopyButton"
                                            Command="diagram:DiagramCommands.Copy"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            CollapseToSmall="WhenGroupIsSmall"
                                            SmallImage="{telerik:IconResource IconRelativePath=copy.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="C"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_CopyDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Copy}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_Copy}"/>
                                    <telerikRibbon:RadRibbonButton x:Name="DeleteButton"
                                            Command="diagram:DiagramCommands.Delete"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            CollapseToSmall="WhenGroupIsSmall"
                                            SmallImage="{telerik:IconResource IconRelativePath=delete_disable.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="D"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_DeleteDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Delete}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_Delete}"/>
                                </telerik:RadCollapsiblePanel>
                            </telerikRibbon:RadRibbonGroup>
                            <telerikRibbon:RadRibbonGroup
                                    Header="{telerik:LocalizableResource Key=DiagramRibbon_Tools}"
                                    ribbonPrimitives:KeyTipService.AccessText="TO"
                                    x:Name="toolsGroup"
                                    Icon="{telerik:IconResource IconRelativePath=16/CollapsedGroupIcons/home_tools.png, IconSources={StaticResource IconPaths}}"
                                    telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_ToolsDescription}"
                                    telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Tools}">
                                <telerik:RadCollapsiblePanel x:Name="toolsPanel">
                                    <telerikRibbon:RadRibbonRadioButton x:Name="TextButton"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Text'}"
                                            SmallImage="{telerik:IconResource IconRelativePath=Text.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="T"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_TextToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_TextToolScreenTip}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_TextTool}"/>
                                    <telerikRibbon:RadRibbonRadioButton x:Name="PathButton"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Path'}"
                                            SmallImage="{telerik:IconResource IconRelativePath=path_tool.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="P1"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_PathToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_PathToolScreenTip}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_PathTool}"/>
                                    <telerikRibbon:RadRibbonRadioButton x:Name="PencilButton"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Pencil'}"
                                            SmallImage="{telerik:IconResource IconRelativePath=pencil_tool.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="P2"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_PencilToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_PencilToolScreenTip}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_PencilTool}"/>
                                    <telerikRibbon:RadRibbonRadioButton x:Name="PointerButton"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Pointer'}"
                                            SmallImage="{telerik:IconResource IconRelativePath=Pointer.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="P3"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_PointerToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_PointerToolScreenTip}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_PointerTool}"/>
                                    <telerikRibbon:RadRibbonRadioButton x:Name="ConnectionButton"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Connector'}"
                                            SmallImage="{telerik:IconResource IconRelativePath=Connection.png, IconSources={StaticResource IconPaths}}"
                                            ribbonPrimitives:KeyTipService.AccessText="CN"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_ConnectorToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_ConnectorToolScreenTip}"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_ConnectorTool}"/>
                                    <telerikRibbon:RadRibbonSplitButton
                                            Size="Medium"
                                            IsToggle="True"
                                            x:Name="shapeToolButton"
                                            Validation.ErrorTemplate="{x:Null}"
                                            IsChecked="{Binding ActiveToolType, RelativeSource={RelativeSource Templatedparent}, Mode=TwoWay, Converter={StaticResource ActiveToolToBooleanConverter}, ConverterParameter='Shape'}"
                                            ribbonPrimitives:KeyTipService.AccessText="ST"
                                            telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_ShapeToolDescription}"
                                            telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_ShapeToolScreenTip}">
                                        <Image Width="16" Height="16" x:Name="shapeToolImage" Source="{telerik:IconResource IconRelativePath=16/rectangle.png, IconSources={StaticResource IconPaths}}"/>
                                        <telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                            <telerik:RadListBox x:Name="shapeToolListBox" SelectedIndex="{Binding SelectedShapeToolIndex, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                                <telerik:RadListBoxItem>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Border Background="Transparent" Margin="0 2">
                                                            <Image Width="16" Height="16" Source="{telerik:IconResource IconRelativePath=16/rectangle.png, IconSources={StaticResource IconPaths}}" Margin="2 0"/>
                                                        </Border>
                                                        <TextBlock Margin="3" Text="{telerik:LocalizableResource Key=DiagramRibbon_RectangleShape}"/>
                                                    </StackPanel>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Border Background="Transparent" Margin="0 2">
                                                            <Image Width="16" Height="16" Source="{telerik:IconResource IconRelativePath=16/ellipse.png, IconSources={StaticResource IconPaths}}" Margin="2 0"/>
                                                        </Border>
                                                        <TextBlock Margin="3" Text="{telerik:LocalizableResource Key=DiagramRibbon_EllipseShape}"/>
                                                    </StackPanel>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Border Background="Transparent" Margin="0 2">
                                                            <Image Width="16" Height="16" Source="{telerik:IconResource IconRelativePath=16/triangle.png, IconSources={StaticResource IconPaths}}" Margin="2 0"/>
                                                        </Border>
                                                        <TextBlock Margin="3" Text="{telerik:LocalizableResource Key=DiagramRibbon_TriangleShape}"/>
                                                    </StackPanel>
                                                </telerik:RadListBoxItem>
                                            </telerik:RadListBox>
                                        </telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                    </telerikRibbon:RadRibbonSplitButton>
                                </telerik:RadCollapsiblePanel>
                            </telerikRibbon:RadRibbonGroup>
                            <telerikRibbon:RadRibbonGroup Header="{telerik:LocalizableResource Key=DiagramRibbon_Export}" x:Name="exportGroup" Icon="{telerik:IconResource IconRelativePath=16/CollapsedGroupIcons/home_export.png, IconSources={StaticResource IconPaths}}">
                                <telerikRibbon:RadRibbonButton
                                        Command="diagram:DiagramCommands.Export"
                                        CommandParameter="Png"
                                        x:Name="exportButton"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        LargeImage="{telerik:IconResource IconRelativePath=32/export.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        ribbonPrimitives:KeyTipService.AccessText="Ex"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_ExportToImage}"/>
                                <telerikRibbon:RadRibbonButton x:Name="ExportToHtmlButton"
                                        LargeImage="{telerik:IconResource IconRelativePath=32/exportHtml.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        ribbonPrimitives:KeyTipService.AccessText="EHt"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_ExportToHtml}"
                                        AutomationProperties.Name="{telerik:LocalizableResource Key=DiagramRibbon_ExportToHtml}"/>
                                <telerikRibbon:RadRibbonButton
                                        Command="extensions:DiagramExtensionCommands.Print"
                                        x:Name="printButton"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        ribbonPrimitives:KeyTipService.AccessText="Pr"
                                        LargeImage="{telerik:IconResource IconRelativePath=32/print.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Print}"/>
                            </telerikRibbon:RadRibbonGroup>
                            <telerikRibbon:RadRibbonGroup Header="{telerik:LocalizableResource Key=DiagramRibbon_Arrange}" x:Name="arrangeGroup" Icon="{telerik:IconResource IconRelativePath=16/CollapsedGroupIcons/home_arrange.png, IconSources={StaticResource IconPaths}}">
                                <telerikRibbon:RadRibbonButton x:Name="GroupButton"
                                        Command="diagram:DiagramCommands.Group"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        LargeImage="{telerik:IconResource IconRelativePath=Group.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        ribbonPrimitives:KeyTipService.AccessText="Gr"
                                        telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_GroupDescription}"
                                        telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Group}"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Group}"/>
                                <telerikRibbon:RadRibbonButton x:Name="UngroupButton"
                                        Command="diagram:DiagramCommands.Ungroup"
                                        CommandTarget="{TemplateBinding Diagram}"
                                        LargeImage="{telerik:IconResource IconRelativePath=Ungroup.png, IconSources={StaticResource IconPaths}}"
                                        Size="Large"
                                        ribbonPrimitives:KeyTipService.AccessText="Ugr"
                                        telerik:ScreenTip.Description="{telerik:LocalizableResource Key=DiagramRibbon_UngroupDescription}"
                                        telerik:ScreenTip.Title="{telerik:LocalizableResource Key=DiagramRibbon_Ungroup}"
                                        Text="{telerik:LocalizableResource Key=DiagramRibbon_Ungroup}"/>
                                <telerik:Separator Style="{StaticResource DiagramRibbonSeparatorStyle}"/>
                                <StackPanel VerticalAlignment="Center">
                                    <telerikRibbon:RadRibbonSplitButton
                                            Margin="3"
                                            VerticalContentAlignment="Center"
                                            HorizontalContentAlignment="Left"
                                            x:Name="bringForwardSplitButton"
                                            ribbonPrimitives:KeyTipService.AccessText="BF"
                                            Text="{telerik:LocalizableResource Key=DiagramRibbon_BringForward}"
                                            Command="diagram:DiagramCommands.BringForward"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            SmallImage="{telerik:IconResource IconRelativePath=16/Arrange/bring_forward.png, IconSources={StaticResource IconPaths}}">
                                        <telerikRibbon:RadRibbonSplitButton.DropDownContent>
                                            <telerikNavigation:RadContextMenu BorderThickness="0" x:Name="bringForwardMenu">
                                                <telerikNavigation:RadMenuItem x:Name="bringForwardMenuItem"
                                                        Header="{telerik:LocalizableResource Key=DiagramRibbon_BringForward}"
                                                        ribbonPrimitives:KeyTipService.AccessText="BF"
                                                        Command="diagram:DiagramCommands.BringForward"
                                                        CommandTarget="{TemplateBinding Diagram}">
                                                    <telerikNavigation:RadMenuItem.Icon>
                                                        <Image Source="{telerik:IconResource IconRelativePath=16/Arrange/bring_forward.png, IconSources={StaticResource IconPaths}}" Stretch="None"/>
                                                    </telerikNavigation:RadMenuItem.Icon>
                                                </telerikNavigation:RadMenuItem>
                                                <telerikNavigation:RadMenuItem x:Name="bringToFrontMenuItem"
                                                        Header="{telerik:LocalizableResource Key=DiagramRibbon_BringToFront}"
                                                        ribbonPrimitives:KeyTipService.AccessText="BT"
                                                        Command="diagram:DiagramCommands.BringToFront"
                                                        CommandTarget="{TemplateBinding Diagram}">
                                                    <telerikNavigation:RadMenuItem.Icon>
                                                        <Image Source="{telerik:IconResource IconRelativePath=16/Arrange/bring_to_front.png, IconSources={StaticResource IconPaths}}" Stretch="None"/>
                                                    </telerikNavigation:RadMenuItem.Icon>
                                                </telerikNavigation:RadMenuItem>
                                                <telerikNavigation:RadMenuItem IsSeparator="True"/>
                                                <telerikNavigation:RadMenuItem x:Name="sendBackwardMenuItem"
                                                        Header="{telerik:LocalizableResource Key=DiagramRibbon_SendBackward}"
                                                        ribbonPrimitives:KeyTipService.AccessText="SB"
                                                        Command="diagram:DiagramCommands.SendBackward"
                                                        CommandTarget="{TemplateBinding Diagram}">

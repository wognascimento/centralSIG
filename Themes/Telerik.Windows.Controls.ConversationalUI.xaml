    <Style x:Key="RepeatButtonLineBaseStyle" TargetType="RepeatButton" BasedOn="{StaticResource RepeatButtonStyle}">
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="Transparent"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="Transparent"/>

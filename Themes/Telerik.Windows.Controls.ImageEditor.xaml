                                </Grid>
                            </ScrollViewer>
                        </Grid>
                        <Border x:Name="ImageEditorBorderVisual"
                                Grid.Column="1"
                                Background="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid Margin="{TemplateBinding Padding}">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>

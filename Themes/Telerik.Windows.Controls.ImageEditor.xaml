<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:imageEditor="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.ImageEditor"
        xmlns:telerik1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
        xmlns:toolsUI="clr-namespace:Telerik.Windows.Media.Imaging.Tools.UI;assembly=Telerik.Windows.Controls.ImageEditor"
        xmlns:mat="clr-namespace:Telerik.Windows.Controls.MaterialControls;assembly=Telerik.Windows.Controls"
        xmlns:helpers="clr-namespace:Telerik.Windows.Controls.Theming.Helpers;assembly=Telerik.Windows.Controls"
        xmlns:animation="clr-namespace:Telerik.Windows.Controls.Animation;assembly=Telerik.Windows.Controls"
        xmlns:telerikPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:imaging="clr-namespace:Telerik.Windows.Media.Imaging;assembly=Telerik.Windows.Controls.ImageEditor"
        xmlns:imagingExt="clr-namespace:Telerik.Windows.Media.Imaging.Extensions;assembly=Telerik.Windows.Controls.ImageEditor">
    <ResourceDictionary.MergedDictionaries>
        <telerik:Windows11ResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <telerik1:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <FontFamily x:Key="TelerikWebUI">/Telerik.Windows.Controls;component/Themes/Fonts/old/TelerikWebUI.ttf#TelerikWebUI</FontFamily>
    <sys:String x:Key="GlyphArrowChevronDown"></sys:String>
    <sys:String x:Key="GlyphUndo"></sys:String>
    <sys:String x:Key="GlyphRedo"></sys:String>
    <sys:String x:Key="GlyphSave"></sys:String>
    <sys:String x:Key="GlyphPlus"></sys:String>
    <sys:String x:Key="GlyphMinus"></sys:String>
    <sys:String x:Key="GlyphPan"></sys:String>
    <sys:String x:Key="GlyphThreeStateNull"></sys:String>
    <sys:String x:Key="GlyphImageResize"></sys:String>
    <sys:String x:Key="GlyphCrop"></sys:String>
    <sys:String x:Key="GlyphMirror"></sys:String>
    <sys:String x:Key="GlyphFlipHorizontal"></sys:String>
    <sys:String x:Key="GlyphFlipVertical"></sys:String>
    <sys:String x:Key="GlyphRotate"></sys:String>
    <sys:String x:Key="GlyphPalette"></sys:String>
    <sys:String x:Key="GlyphBrightnessContrast"></sys:String>
    <sys:String x:Key="GlyphSaturation"></sys:String>
    <sys:String x:Key="GlyphGreyscale"></sys:String>
    <sys:String x:Key="GlyphBlur"></sys:String>
    <sys:String x:Key="GlyphSharpen"></sys:String>
    <sys:String x:Key="GlyphShape"></sys:String>
    <sys:String x:Key="GlyphRoundCorners"></sys:String>
    <sys:String x:Key="GlyphBold"></sys:String>
    <sys:String x:Key="GlyphItalic"></sys:String>
    <sys:String x:Key="GlyphUnderline"></sys:String>
    <sys:String x:Key="GlyphEditTools"></sys:String>
    <sys:String x:Key="GlyphFolderOpen"></sys:String>
    <Style x:Key="FocusVisualBorderStyle" TargetType="Border">
        <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBackgroundBrush)}"/>
        <Setter Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
        <Setter Property="BorderThickness" Value="{telerik1:Windows11Resource ResourceKey=FocusThickness}"/>
        <Setter Property="SnapsToDevicePixels" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SnapsToDevicePixels}"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Opacity" Value="0"/>
    </Style>
    <Style TargetType="imageEditor:RadImageEditor" x:Key="RadImageEditorStyle">
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Padding" Value="10"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="imageEditor:RadImageEditor">
                    <Grid x:Name="LayoutRoot">
                        <ScrollViewer x:Name="ScrollViewer" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderBrush="{x:Null}" Background="{TemplateBinding Background}">
                            <Grid x:Name="ScrollContent">
                                <Grid x:Name="ImageView">
                                    <Grid Background="{TemplateBinding Background}" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Image x:Name="MainImage"/>
                                        <Grid x:Name="PreviewPanel"/>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </ScrollViewer>
                        <Grid>
                            <toolsUI:InlineToolSettingsPane x:Name="ToolSettingsPane" Visibility="Collapsed" MinWidth="270" Padding="16 16 13 16"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="toolsUI:InlineToolSettingsPane" x:Key="InlineToolSettingsPaneStyle">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="toolsUI:InlineToolSettingsPane">
                    <Grid>
                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}"/>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Cursor="Hand">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <ContentPresenter/>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1" Margin="20 2">
                                    <telerik:RadButton Name="ButtonOK" Margin="0 0 10 0" MinWidth="118" Content="{telerik:LocalizableResource Key=Ok}"/>
                                    <telerik:RadButton Name="ButtonCancel" Margin="0 0 10 0" MinWidth="118" Content="{telerik:LocalizableResource Key=Cancel}"/>
                                    <telerik:RadButton Name="ButtonReset" MinWidth="118" Content="{telerik:LocalizableResource Key=Reset}"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="toolsUI:ToolSettingsPanel" x:Key="ToolSettingsPanelStyle">
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="toolsUI:ToolSettingsPanel">
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0" Background="{TemplateBinding Background}" Margin="0 1 1 0">
                            <ContentPresenter Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </ScrollViewer>
                        <Grid VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Row="1" Margin="16 0 16 16">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <telerik:RadButton
                                    Visibility="Collapsed"
                                    Margin="0 0 2 0"
                                    x:Name="PART_CommitSelectionButton"
                                    MinWidth="118"
                                    Content="{telerik:LocalizableResource Key=ImageEditor_SelectionTool_CommitSelection}"
                                    Command="{Binding Path=Commands.CommitSelection, ElementName=ImageEditor}"/>
                            <telerik:RadButton x:Name="ButtonOK" Grid.Column="1" Visibility="Collapsed" MinWidth="118" Margin="2 0" Content="{telerik:LocalizableResource Key=Ok}"/>
                            <telerik:RadButton x:Name="ButtonCancel" Grid.Column="2" Visibility="Collapsed" MinWidth="118" Margin="2 0" Content="{telerik:LocalizableResource Key=Cancel}"/>
                            <telerik:RadButton x:Name="ButtonReset" Grid.Column="2" MinWidth="118" Margin="2 0 0 0" Content="{telerik:LocalizableResource Key=Reset}"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="Thumb" x:Key="ThumbStyleEmpty">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Rectangle Fill="Black"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="Thumb" x:Key="IntersectionThumbStyle">
        <Setter Property="Panel.ZIndex" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Rectangle Fill="{telerik:Windows11Resource ResourceKey=AccentBrush}" Width="10" Height="10"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="Thumb" x:Key="BridgeThumbStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Rectangle Fill="{telerik:Windows11Resource ResourceKey=AccentBrush}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="toolsUI:CropAdorner" x:Key="CropAdornerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="toolsUI:CropAdorner">
                    <Grid x:Name="MainGrid">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Black" Opacity="0.3" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3"/>
                        <Rectangle Fill="Black" Opacity="0.3" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="1"/>
                        <Rectangle Fill="Black" Opacity="0.3" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3"/>
                        <Rectangle Fill="Black" Opacity="0.3" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="1"/>
                        <Thumb x:Name="ThumbC" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Style="{StaticResource ThumbStyleEmpty}" Opacity="0"/>
                        <Thumb x:Name="ThumbL"
                                Width="16"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="-10 0 0 0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Stretch"
                                Style="{StaticResource ThumbStyleEmpty}"
                                Opacity="0"
                                Cursor="SizeWE"/>
                        <Thumb x:Name="ThumbR"
                                Width="16"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="0 0 -10 0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Stretch"
                                Style="{StaticResource ThumbStyleEmpty}"
                                Opacity="0"
                                Cursor="SizeWE"/>
                        <Thumb x:Name="ThumbT"
                                Height="16"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="0 -10 0 0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Top"
                                Style="{StaticResource ThumbStyleEmpty}"
                                Opacity="0"
                                Cursor="SizeNS"/>
                        <Thumb x:Name="ThumbB"
                                Height="16"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="0 0 0 -10"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource ThumbStyleEmpty}"
                                Opacity="0"
                                Cursor="SizeNS"/>
                        <Thumb x:Name="ThumbTL"
                                Grid.Row="1"
                                Grid.Column="1"
                                Cursor="SizeNWSE"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Style="{StaticResource IntersectionThumbStyle}"
                                Margin="-5 -5 0 0"/>
                        <Thumb x:Name="ThumbTR"
                                Grid.Row="1"
                                Grid.Column="1"
                                Cursor="SizeNESW"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Style="{StaticResource IntersectionThumbStyle}"
                                Margin="0 -5 -5 0"/>
                        <Thumb x:Name="ThumbBR"
                                Grid.Row="1"
                                Grid.Column="1"
                                Cursor="SizeNWSE"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource IntersectionThumbStyle}"
                                Margin="0 0 -5 -5"/>
                        <Thumb x:Name="ThumbBL"
                                Grid.Row="1"
                                Grid.Column="1"
                                Cursor="SizeNESW"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource IntersectionThumbStyle}"
                                Margin="-5 0 0 -5"/>
                        <Thumb x:Name="ThumbBridgeL" Grid.Row="1" Grid.Column="1" Width="1" HorizontalAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource BridgeThumbStyle}"/>
                        <Thumb x:Name="ThumbBridgeT" Grid.Row="1" Grid.Column="1" Height="1" HorizontalAlignment="Stretch" VerticalAlignment="Top" Style="{StaticResource BridgeThumbStyle}"/>
                        <Thumb x:Name="ThumbBridgeR" Grid.Row="1" Grid.Column="1" Width="1" HorizontalAlignment="Right" VerticalAlignment="Stretch" Style="{StaticResource BridgeThumbStyle}"/>
                        <Thumb x:Name="ThumbBridgeB" Grid.Row="1" Grid.Column="1" Height="1" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Style="{StaticResource BridgeThumbStyle}"/>
                        <Canvas Grid.Row="1" Grid.Column="1" Margin="-2 -23 0 0">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock x:Name="CropSizeLabel" Foreground="{telerik:Windows11Resource ResourceKey=AccentControlForegroundBrush}" IsHitTestVisible="False"/>
                                <TextBlock Text="px" Margin="2 0 0 0" Foreground="{telerik:Windows11Resource ResourceKey=AccentControlForegroundBrush}" IsHitTestVisible="False"/>
                            </StackPanel>
                        </Canvas>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="ImageEditorToggleButtonTemplate" TargetType="telerik:RadToggleButton">
        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
        </Border>
    </ControlTemplate>
    <Style x:Key="EmptyFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Rectangle Stroke="Transparent" StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ImageEditorExpanderStyle" TargetType="telerik:RadExpander">
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="FontWeight" Value="SemiBold"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="#00FFFFFF"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0 1"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalHeaderAlignment" Value="Center"/>
        <Setter Property="ExpandDirection" Value="Down"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Expand" Direction="In" TargetElementName="Content"/>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Collapse" Direction="Out" TargetElementName="Content"/>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadExpander">
                    <Grid HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="ExpandStateGroup">
                                <VisualState x:Name="Expanded">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="arrow" Storyboard.TargetProperty="(FrameworkElement.RenderTransform).Angle" To="180" Duration="0:0:0.2"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Collapsed">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="arrow" Storyboard.TargetProperty="(FrameworkElement.RenderTransform).Angle" To="0" Duration="0:0:0.2"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid Background="{TemplateBinding Background}">
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="row0" Height="Auto" MinHeight="26"/>
                                <RowDefinition x:Name="row1" Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition x:Name="col0" Width="Auto"/>
                                <ColumnDefinition x:Name="col1" Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <telerik:RadToggleButton x:Name="ImageEditorHeaderButton"
                                    TabIndex="{TemplateBinding TabIndex}"
                                    HorizontalAlignment="{TemplateBinding HorizontalHeaderAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalHeaderAlignment}"
                                    HorizontalContentAlignment="{TemplateBinding HorizontalHeaderAlignment}"
                                    VerticalContentAlignment="{TemplateBinding VerticalHeaderAlignment}"
                                    Background="Transparent"
                                    Template="{StaticResource ImageEditorToggleButtonTemplate}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    IsChecked="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                    ClickMode="{TemplateBinding ClickMode}"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontStyle="{TemplateBinding FontStyle}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontStretch="{TemplateBinding FontStretch}"
                                    Foreground="{TemplateBinding Foreground}"
                                    FocusVisualStyle="{StaticResource EmptyFocusVisual}">
                                <Grid x:Name="HeaderPanel" Background="Transparent">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <telerikPrimitives:LayoutTransformControl x:Name="HeaderContentTransform" Grid.Column="0" Grid.Row="0" RenderTransformOrigin=".5 .5" Margin="16 0 0 0">
                                        <ContentControl x:Name="HeaderContent"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Stretch"
                                                DataContext="{TemplateBinding Header}"
                                                Content="{Binding}"
                                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                FontFamily="{TemplateBinding FontFamily}"
                                                FontSize="{TemplateBinding FontSize}"
                                                Foreground="{TemplateBinding Foreground}"/>
                                    </telerikPrimitives:LayoutTransformControl>
                                    <Grid Grid.Column="1" Grid.Row="0" x:Name="arrowPanel" Margin="12">
                                        <Border x:Name="arrowBorder" Width="22" Height="22" CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}">
                                            <telerik:RadGlyph x:Name="arrow"
                                                    Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                                                    FontSize="12"
                                                    Glyph="{StaticResource GlyphArrowChevronDown}"
                                                    RenderTransformOrigin="0.5 0.5">
                                                <telerik:RadGlyph.RenderTransform>
                                                    <RotateTransform Angle="0"/>
                                                </telerik:RadGlyph.RenderTransform>
                                            </telerik:RadGlyph>
                                        </Border>
                                    </Grid>
                                </Grid>
                            </telerik:RadToggleButton>
                            <ContentPresenter x:Name="Content"
                                    Margin="{TemplateBinding Padding}"
                                    Visibility="Collapsed"
                                    Content="{TemplateBinding Content}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="HeaderContent" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="ImageEditorHeaderButton" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                        </Trigger>
                        <Trigger SourceName="ImageEditorHeaderButton" Property="IsMouseOver" Value="True">
                            <Setter TargetName="arrowBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush)}"/>
                        </Trigger>
                        <Trigger SourceName="ImageEditorHeaderButton" Property="IsPressed" Value="True">
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                            <Setter TargetName="arrowBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBackgroundBrush)}"/>
                        </Trigger>
                        <Trigger Property="ResultOrientation" Value="Vertical">
                            <Setter TargetName="HeaderContentTransform" Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="90"/>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="HeaderContentTransform" Property="Grid.Row" Value="1"/>
                        </Trigger>
                        <Trigger Property="ResultOrientation" Value="Horizontal">
                            <Setter TargetName="HeaderContentTransform" Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="0"/>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="HeaderContentTransform" Property="Grid.Column" Value="0"/>
                        </Trigger>
                        <Trigger Property="ExpandDirection" Value="Down">
                            <Setter TargetName="col0" Property="Width" Value="*"/>
                            <Setter TargetName="row1" Property="Height" Value="*"/>
                            <Setter TargetName="Content" Property="Grid.Row" Value="1"/>
                        </Trigger>
                        <Trigger Property="ExpandDirection" Value="Up">
                            <Setter TargetName="col0" Property="Width" Value="*"/>
                            <Setter TargetName="row0" Property="Height" Value="*"/>
                            <Setter TargetName="ImageEditorHeaderButton" Property="Grid.Row" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="IncreaseHandleBaseStyle" TargetType="RepeatButton">
        <Setter Property="FontFamily" Value="{x:Static telerik:RadGlyph.TelerikFluentIconsFontFamily}"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid>
                        <telerik:RadGlyph
                                Foreground="{TemplateBinding Foreground}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Glyph="{TemplateBinding Content}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                        </Trigger>

                                        IsBackgroundVisible="False"
                                        AutomationProperties.Name="Save"
                                        CommandParameter="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SaveCommandParameter, Mode=TwoWay}"
                                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsSaveButtonVisible, Converter={StaticResource BooleanToVisibilityConverter}}"
                                        MinWidth="32"
                                        MinHeight="32"
                                        Foreground="{telerik1:Windows11Resource ResourceKey=IconBrush}"
                                        BorderBrush="Transparent"
                                        Background="Transparent"
                                        Padding="0">
                                    <ToolTipService.ToolTip>
                                        <TextBlock Text="{telerik:LocalizableResource Key=ImageEditor_Save}"/>

                                        VerticalAlignment="Top"/>
                                <imageEditor:RadImageEditor
                                        Grid.Row="1"
                                        x:Name="ImageEditor"
                                        AutomationProperties.Name="ImageEditor"
                                        ToolSettingsContainer="{Binding ElementName=SettingsPanel}"
                                        VerticalAlignment="Stretch"
                                        HorizontalAlignment="Stretch"
                                        Margin="0 10 0 0"
                                        Image="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image, Mode=OneWay}"
                                        IsReadOnly="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsReadOnly, Mode=TwoWay}"/>
                                <imaging:ZoomController Grid.Row="2" ImageEditor="{Binding ElementName=ImageEditor}" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 0 0"/>
                            </Grid>
                        </Border>
                        <Grid Grid.Column="2">
                            <toolsUI:ToolSettingsPanel x:Name="SettingsPanel" Visibility="Collapsed" MinWidth="270" Padding="16"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="ImageEditorImagePreviewText" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="ImageEditor" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBackgroundBrush)}"/>
                            <Setter TargetName="OpenButton" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="SaveButton" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="UndoButton" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="RedoButton" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="toolsUI:NumericPropertyEditor" x:Key="NumericPropertyEditorStyle">
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="Padding" Value="0 8 0 0"/>

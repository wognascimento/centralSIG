                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid>
                            <TextBlock x:Name="selectionToolHelperTextBlock" Text="{telerik:LocalizableResource Key=ImageEditor_Selection}" Visibility="Collapsed"/>

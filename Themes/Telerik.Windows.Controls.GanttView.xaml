    <ControlTemplate x:Key="DependencyIndicatorTemplate" TargetType="gantt1:DependencyIndicator">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:Windows11Resource ResourceKey=AccentBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik1:Windows11Resource ResourceKey=FocusBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="DragOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:Windows11Resource ResourceKey=AccentBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik1:Windows11Resource ResourceKey=FocusBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="StrokeThickness" To="2"/>
                            <DoubleAnimation Duration="0:0:0.1" To="1.2" Storyboard.TargetName="EllipseScale" Storyboard.TargetProperty="ScaleX"/>
                            <DoubleAnimation Duration="0:0:0.1" To="1.2" Storyboard.TargetName="EllipseScale" Storyboard.TargetProperty="ScaleY"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Visual" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>-1 0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Ellipse x:Name="Visual"
                    StrokeThickness="1"
                    Stroke="{TemplateBinding BorderBrush}"
                    Fill="{TemplateBinding Background}"
                    Cursor="Hand"
                    VerticalAlignment="Center"
                    RenderTransformOrigin="0.5 0.5"
                    Width="12"
                    Height="12">
                <Ellipse.RenderTransform>
                    <ScaleTransform x:Name="EllipseScale"/>
                </Ellipse.RenderTransform>
                <telerik:ToolTipManager.ToolTip>
                    <ToolTip ContentTemplate="{StaticResource DragRelationTooltipTemplate}"/>
                </telerik:ToolTipManager.ToolTip>
            </Ellipse>
        </Grid>
    </ControlTemplate>

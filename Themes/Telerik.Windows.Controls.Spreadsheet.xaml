                                <Grid Margin="5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel Margin="5" Grid.ColumnSpan="3">
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_Colors" Margin="5 0 0 5" Style="{StaticResource HeaderTextBlockStyle}"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_Color1" Foreground="{TemplateBinding Foreground}" Margin="5"/>
                                        <spreadsheetControls:ColorSelectorDropDown x:Name="Color1Selector"
                                                SelectedColor="{Binding Path=Color1, Mode=TwoWay}"
                                                HeaderPaletteItemsSource="{Binding Path=ColorPalette.HeaderPaletteItemsSource}"
                                                MainPaletteItemsSource="{Binding Path=ColorPalette.MainPaletteItemsSource}"
                                                MainPaletteOrientation="{Binding Path=ColorPalette.MainPaletteOrientation}"
                                                HorizontalAlignment="Left"
                                                Width="122"
                                                Margin="5 0 0 0"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_Color2" Foreground="{TemplateBinding Foreground}" Margin="5"/>
                                        <spreadsheetControls:ColorSelectorDropDown x:Name="Color2Selector"
                                                SelectedColor="{Binding Path=Color2, Mode=TwoWay}"
                                                HeaderPaletteItemsSource="{Binding Path=ColorPalette.HeaderPaletteItemsSource}"
                                                MainPaletteItemsSource="{Binding Path=ColorPalette.MainPaletteItemsSource}"
                                                MainPaletteOrientation="{Binding Path=ColorPalette.MainPaletteOrientation}"
                                                HorizontalAlignment="Left"
                                                Width="122"
                                                Margin="5 0 0 0"/>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" Margin="5">
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_ShadingStyles" Style="{StaticResource HeaderTextBlockStyle}" Margin="0 0 0 10"/>
                                        <Border BorderThickness="1" Padding="5" BorderBrush="{TemplateBinding BorderBrush}">

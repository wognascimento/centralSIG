                                                <telerik:RadMenuItem.Icon>
                                                    <Image Source="{telerik:IconResource IconRelativePath=SheetSelector/deleteSheet.png, IconSources={StaticResource IconPaths}}" Stretch="None"/>
                                                </telerik:RadMenuItem.Icon>
                                            </telerik:RadMenuItem>
                                            <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_SheetSelector_Rename" Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RenameSheetCommand, Mode=OneWay}"/>
                                            <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_SheetSelector_TabColor">
                                                <telerik:RadMenuGroupItem BorderThickness="0" Padding="0">
                                                    <telerik:RadColorSelector x:Name="PART_ColorSelector"
                                                            HeaderPaletteItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RadSpreadsheet.ColorPalette.HeaderPaletteItemsSource}"
                                                            MainPaletteItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RadSpreadsheet.ColorPalette.MainPaletteItemsSource}"
                                                            MainPaletteOrientation="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RadSpreadsheet.ColorPalette.MainPaletteOrientation}"
                                                            RecentColorsVisibility="Visible"
                                                            SelectedColor="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ActiveTabItemColor, Mode=TwoWay}"
                                                            Margin="-2"/>
                                                </telerik:RadMenuGroupItem>
                                            </telerik:RadMenuItem>
                                            <telerik:RadMenuSeparatorItem/>
                                            <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Hide" Command="{Binding Path=HideSheet.Command, Mode=OneWay}" IsEnabled="{Binding Path=HideSheet.IsEnabled, Mode=OneWay}"/>
                                            <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Unhide_Ellipsis" Command="{Binding Path=ShowUnhideSheetDialog.Command, Mode=OneWay}" IsEnabled="{Binding Path=ShowUnhideSheetDialog.IsEnabled, Mode=OneWay}"/>
                                        </telerik:RadContextMenu>
                                    </telerik:RadContextMenu.ContextMenu>
                                </StackPanel>
                            </ScrollViewer>
                        </StackPanel>
                        <Canvas x:Name="PART_ImagesContainer" Margin="0 -50 0 0">
                            <Image x:Name="PART_PageIcon"
                                    Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/SheetSelector/dragDropSheetPage.png"
                                    Visibility="Collapsed"
                                    Canvas.Left="20"
                                    Canvas.ZIndex="1000"/>
                            <Image x:Name="PART_Arrow"
                                    Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/SheetSelector/dragDropSheetArrow.png"
                                    Visibility="Collapsed"
                                    UseLayoutRounding="True"
                                    SnapsToDevicePixels="True"/>
                        </Canvas>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PrintPreviewControlStyle" TargetType="spreadsheetControls:PrintPreviewControl">

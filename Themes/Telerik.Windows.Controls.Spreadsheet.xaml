                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ManageConditionalFormattingDialogContentStyle" TargetType="dialogs:ManageConditionalFormattingDialogContent">
        <Setter Property="telerik:LocalizationManager.ResourceKey" Value="Spreadsheet_ConditionalFormatting_ManageRulesDialog_ConditionalFormattingRulesManager"/>
        <Setter Property="Background" Value="{telerik:Windows11Resource ResourceKey=OverlayBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Width" Value="800"/>
        <Setter Property="MinHeight" Value="400"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:ManageConditionalFormattingDialogContent">
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Horizontal" Margin="16 16 16 0">
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_ShowFormattingRulesFor" VerticalAlignment="Center"/>
                            <telerik:RadComboBox
                                    ItemsSource="{Binding ConditionalFormattingSources}"
                                    SelectedValue="{Binding SelectedConditionalFormattingSource}"
                                    DisplayMemberPath="DisplayName"
                                    SelectedValuePath="Value"/>
                        </StackPanel>
                        <Border Grid.Row="1">
                            <Grid Margin="{StaticResource DialogsInnerContentMargin}">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <StackPanel Orientation="Horizontal">
                                    <telerik:RadButton MinWidth="100" Margin="1 5 2 5" Command="{Binding NewCommand}" Visibility="Collapsed">
                                        <telerik:RadButton.Content>
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Source="{telerik:IconResource IconRelativePath=16/ConditionalFormatting/newRule.png, IconSources={StaticResource IconPaths}}"/>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_NewRule" Margin="5 0 2 1"/>
                                            </StackPanel>
                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>
                                    <telerik:RadButton MinWidth="100" Margin="1 5 2 5" Command="{Binding EditCommand}">
                                        <telerik:RadButton.Content>
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Source="{telerik:IconResource IconRelativePath=16/ConditionalFormatting/editRule.png, IconSources={StaticResource IconPaths}}"/>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_EditRule" Margin="5 0 2 1"/>
                                            </StackPanel>
                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>
                                    <telerik:RadButton MinWidth="100" Margin="1 5 2 5" Command="{Binding DeleteCommand}">
                                        <telerik:RadButton.Content>
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Source="{telerik:IconResource IconRelativePath=16/ConditionalFormatting/deleteRule.png, IconSources={StaticResource IconPaths}}"/>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_DeleteRule" Margin="5 0 2 1"/>
                                            </StackPanel>
                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>
                                    <telerik:RadButton MinWidth="120" Margin="1 5 2 5" Command="{Binding DuplicateCommand}">
                                        <telerik:RadButton.Content>
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Source="{telerik:IconResource IconRelativePath=16/ConditionalFormatting/duplicateRule.png, IconSources={StaticResource IconPaths}}"/>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_DuplicateRule" Margin="5 0 2 1"/>
                                            </StackPanel>
                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>
                                    <telerik:RadButton MinWidth="32" Margin="2 5" Command="{Binding MoveUpCommand}">

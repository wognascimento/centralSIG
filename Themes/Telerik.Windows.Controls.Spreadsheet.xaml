                                    IsTabStop="False"
                                    Focusable="False"
                                    Margin="8 -3 0 0">
                                <StackPanel Orientation="Horizontal" Name="PART_SheetTabItemsContainer" Height="36" VerticalAlignment="Top" Margin="0 -3 0 0">
                                    <telerik:RadContextMenu.ContextMenu>
                                        <telerik:RadContextMenu x:Name="PART_ContextMenu" InheritDataContext="True">

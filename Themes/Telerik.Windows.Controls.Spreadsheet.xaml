                                            <telerik:RadListBox x:Name="BorderStyleSelector" ScrollViewer.VerticalScrollBarVisibility="Disabled" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Height="329">
                                                <telerik:EventToCommandBehavior.EventBindings>
                                                    <telerik:EventBinding

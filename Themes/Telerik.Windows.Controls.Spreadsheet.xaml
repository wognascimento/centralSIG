                                    CanUserFreezeColumns="False"
                                    ShowGroupPanel="False"
                                    VerticalAlignment="Top"
                                    MaxWidth="548"
                                    MaxHeight="277"
                                    Margin="1"
                                    BorderThickness="1">
                                <grid:RadGridView.Columns>

                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>
                                    <telerik:RadButton MinWidth="32" Margin="2 5" Command="{Binding MoveDownCommand}">
                                        <telerik:RadButton.Content>
                                            <telerik:RadGlyph Foreground="{telerik:Windows11Resource ResourceKey=IconBrush}" Glyph="{StaticResource GlyphArrowChevronDown}"/>
                                        </telerik:RadButton.Content>
                                    </telerik:RadButton>

        <Setter Property="MinHeight" Value="490"/>
        <Setter Property="WindowStartupLocation" Value="CenterScreen"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:FillEffectsDialogContent">
                    <Grid Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Rectangle Fill="{TemplateBinding Background}"/>
                        <telerik:RadTabControl BorderThickness="0" Background="{TemplateBinding Background}">

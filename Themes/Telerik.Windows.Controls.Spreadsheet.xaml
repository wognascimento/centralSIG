            <Setter.Value>
                <ControlTemplate TargetType="{x:Type dialogs:DataBarDialogContent}">
                    <Grid Width="400" Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Grid.Row="0">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Negative bar fill color" Grid.Column="0"/>
                                <Border Height="1" BorderThickness="1" BorderBrush="Gray" Grid.Column="1"/>
                            </Grid>
                            <StackPanel Orientation="Horizontal">
                                <RadioButton Content="Fill color:" GroupName="negFill" x:Name="NegFillButton" IsChecked="{Binding UseNegativeFill}"/>
                                <telerik:RadColorPicker IsEnabled="{Binding ElementName=NegFillButton, Path=IsChecked}" SelectedColor="{Binding NegativeFillColor}"/>
                            </StackPanel>
                            <RadioButton Content="Apply same fill color as positive bar" GroupName="negFill" IsChecked="{Binding UsePositiveFill}"/>
                        </StackPanel>
                        <StackPanel Grid.Row="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Negative bar border color"/>
                                <Border Height="1" BorderThickness="1" BorderBrush="Gray" Grid.Column="1"/>
                            </Grid>
                            <StackPanel Orientation="Horizontal">
                                <RadioButton Content="Border color:" GroupName="negBorder" x:Name="NegBorderButton" IsChecked="{Binding UseNegativeBorder}"/>
                                <telerik:RadColorPicker IsEnabled="{Binding ElementName=NegBorderButton, Path=IsChecked}" SelectedColor="{Binding NegativeBorderColor}"/>
                            </StackPanel>
                            <RadioButton Content="Apply same border color as positive bar" GroupName="negBorder" IsChecked="{Binding UsePositiveBorder}"/>
                        </StackPanel>
                        <StackPanel Grid.Row="2">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Axis settings"/>
                                <Border Height="1" BorderThickness="1" BorderBrush="Gray" Grid.Column="1"/>
                            </Grid>
                            <TextBlock Text="Select axis position in cell to change the appearence of bars for negative values"/>
                            <RadioButton Content="Automatic (display at variable position based on negative values)" GroupName="axis" IsChecked="{Binding AutomaticAxis}"/>
                            <RadioButton Content="Cell midpoint" GroupName="axis" IsChecked="{Binding MidpointAxis}"/>
                            <RadioButton Content="None (show negative value bars in same direction as positive)" GroupName="axis" IsChecked="{Binding NoAxis}"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Axis color:"/>
                                <telerik:RadColorPicker SelectedColor="{Binding AxisColor}" IsEnabled="{Binding HasAxis}"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Grid.Row="3" HorizontalAlignment="Right">
                            <telerik:RadButton x:Name="PART_ButtonOK" Content="Ok" Width="70" Margin="1 1 10 1"/>
                            <telerik:RadButton x:Name="PART_ButtonCancel" Content="Cancel" Width="70" Margin="1 1 10 1"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="{x:Type dialogs:ValueView}" x:Key="ValueViewStyle">

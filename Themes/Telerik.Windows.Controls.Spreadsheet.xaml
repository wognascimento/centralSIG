                                                    <StackPanel Orientation="Horizontal">
                                                        <CheckBox IsChecked="{Binding IsChecked, Mode=TwoWay}" Content="{Binding Name}"/>
                                                    </StackPanel>
                                                </HierarchicalDataTemplate>
                                            </telerik:RadTreeView.ItemTemplate>
                                        </telerik:RadTreeView>
                                        <StackPanel
                                                Grid.Column="1"
                                                Grid.Row="1"
                                                Orientation="Horizontal"
                                                Margin="8 8 8 0"
                                                Visibility="{Binding Path=FilteringPopupTreeViewViewModel.TreeIsOversize, Converter={StaticResource BoolToVisibilityValueConverter}}">

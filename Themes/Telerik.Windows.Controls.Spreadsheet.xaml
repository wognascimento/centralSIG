                                                    telerik:LocalizationManager.ResourceKey="Spreadsheet_SheetSelector_Insert"
                                                    Command="{Binding Path=InsertSheet.Command, Mode=OneWay}"
                                                    CommandParameter="Worksheet"
                                                    IsEnabled="{Binding Path=InsertSheet.IsEnabled, Mode=TwoWay}"/>

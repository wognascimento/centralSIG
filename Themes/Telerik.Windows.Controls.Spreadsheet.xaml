    <DataTemplate x:Key="CommentEntryItemTemplate">
        <Border x:Name="BorderVisual" BorderBrush="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}" BorderThickness="0 1 0 0">
            <Grid x:Name="LayoutRootGrid">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="44"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Center">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Column="0" CornerRadius="14" Width="28" Height="28" Margin="8 4" Background="{telerik:Windows11Resource ResourceKey=DisabledBackgroundBrush}">
                        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Text="{Binding AuthorInitials}"/>
                    </Border>
                    <TextBlock Grid.Column="1" Text="{Binding AuthorName}" Margin="0 10"/>
                    <TextBlock
                            Grid.Column="2"
                            Visibility="{Binding IsCommentSpecific, Converter={StaticResource BoolToVisibilityValueConverter}}"
                            Margin="5 10"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Right"
                            Text="{Binding CellName}"/>
                    <Grid Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0 0 10 0">
                        <telerik:RadDropDownButton x:Name="OptionsButton"
                                IsOpen="{Binding IsOptionsDropDownOpen, Mode=TwoWay}"
                                DropDownIndicatorVisibility="Collapsed"
                                AutoOpenDelay="0:0:0.0"
                                Style="{StaticResource CommentOptionsButtonStyle}">
                            <telerik:RadDropDownButton.Visibility>
                                <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                    <Binding Path="IsCommentSpecific"/>
                                    <Binding Path="IsCommentResolved" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    <Binding Path="IsInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    <Binding Path="IsAnotherCommentReplyInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    <Binding Path="IsCommentInCreationState" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    <Binding Path="AreAllButtonsCollapsed" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    <Binding Path="HasAnotherPendingComment" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                </MultiBinding>
                            </telerik:RadDropDownButton.Visibility>
                            <telerik:RadGlyph Glyph="{StaticResource GlyphMoreVertical}" Foreground="{Binding ElementName=OptionsButton, Path=Foreground}"/>
                            <telerik:RadDropDownButton.DropDownContent>
                                <telerik:RadMenu Orientation="Vertical" BorderThickness="0" Background="{x:Null}" Padding="0">
                                    <telerik:RadMenuItem Header="Delete Thread" Command="{Binding DeleteThreadCommand}"/>
                                    <telerik:RadMenuItem Header="Resolve Thread" Command="{Binding ResolveThreadCommand}"/>
                                </telerik:RadMenu>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                        <telerik:RadButton x:Name="DeleteThreadButton" Command="{Binding CancelClickCommand}" Style="{StaticResource CommentButtonBaseStyle}" MinWidth="0">
                            <telerik:RadButton.Visibility>
                                <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                    <Binding Path="IsCommentSpecific"/>
                                    <Binding Path="IsCommentInCreationState"/>
                                </MultiBinding>
                            </telerik:RadButton.Visibility>
                            <telerik:RadGlyph Glyph="{StaticResource GlyphClose}"/>
                        </telerik:RadButton>
                    </Grid>
                </Grid>
                <Grid Grid.Row="1" Grid.Column="1" Margin="0 0 10 0" Visibility="{Binding IsCommentInCreationState, Converter={StaticResource InversedBoolToVisibilityValueConverter}}">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Margin="0 0 0 5" HorizontalAlignment="Stretch">
                        <TextBox x:Name="PART_CommentTextBox"
                                Text="{Binding Text, UpdateSourceTrigger=LostFocus}"
                                TextWrapping="Wrap"
                                AcceptsReturn="True"
                                MaxLines="5"
                                IsReadOnly="{Binding IsReadOnly}"/>
                        <Grid Visibility="{Binding ElementName=PART_CommentTextBox, Path=IsReadOnly, Converter={StaticResource BooleanToVisibilityConverter}}">
                            <TextBlock x:Name="PreviewTb"
                                    Text="{Binding ElementName=PART_CommentTextBox, Path=Text}"
                                    TextWrapping="Wrap"
                                    TextTrimming="CharacterEllipsis"
                                    Width="{Binding ElementName=PART_CommentTextBox, Path=ActualWidth}"
                                    Height="{Binding ElementName=PART_CommentTextBox, Path=ActualHeight}"/>
                            <telerik:RadToggleButton x:Name="ExpandButton" MinHeight="0" MinWidth="0" Cursor="IBeam">
                                <telerik:RadToggleButton.Template>
                                    <ControlTemplate TargetType="telerik:RadToggleButton">
                                        <Border Background="Transparent"/>
                                    </ControlTemplate>
                                </telerik:RadToggleButton.Template>
                            </telerik:RadToggleButton>
                        </Grid>
                    </Grid>
                    <TextBlock Grid.Row="1" Text="{Binding CreationDate}" Margin="0 0 0 10" Visibility="{Binding IsInEditMode, Converter={StaticResource InversedBoolToVisibilityValueConverter}}"/>
                    <Grid x:Name="ActionButtonsPanel"
                            Grid.Row="2"
                            HorizontalAlignment="Right"
                            Margin="0 0 0 5"
                            Visibility="{Binding IsCommentResolved, Converter={StaticResource InversedBoolToVisibilityValueConverter}}"
                            Opacity="0">
                        <StackPanel Orientation="Horizontal">
                            <telerik:RadButton x:Name="EditButton"
                                    MinWidth="{StaticResource CommentActionButtonMinWidth}"
                                    IsBackgroundVisible="False"
                                    Command="{Binding EditClickCommand}"
                                    Content="Edit"
                                    Margin="0 0 4 0"
                                    Cursor="Hand">
                                <telerik:RadButton.Visibility>
                                    <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                        <Binding Path="IsInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsCommentResolved" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsAnotherCommentReplyInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="AreAllButtonsCollapsed" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="HasAnotherPendingComment" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    </MultiBinding>
                                </telerik:RadButton.Visibility>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="DeleteButton"
                                    MinWidth="{StaticResource CommentActionButtonMinWidth}"
                                    IsBackgroundVisible="False"
                                    Command="{Binding DeleteThreadCommand}"
                                    Content="Delete"
                                    Cursor="Hand">
                                <telerik:RadButton.Visibility>
                                    <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                        <Binding Path="IsReplySpecific"/>
                                        <Binding Path="IsInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsCommentResolved" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsAnotherCommentReplyInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="AreAllButtonsCollapsed" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="HasAnotherPendingComment" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    </MultiBinding>
                                </telerik:RadButton.Visibility>
                            </telerik:RadButton>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <telerik:RadButton x:Name="SaveButton"
                                    MinWidth="{StaticResource CommentActionButtonMinWidth}"
                                    IsBackgroundVisible="False"
                                    Command="{Binding SaveClickCommand}"
                                    Content="Save"
                                    Margin="0 0 4 0"
                                    Cursor="Hand">
                                <telerik:RadButton.Visibility>
                                    <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                        <Binding Path="IsAnotherCommentReplyInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsInEditMode"/>
                                        <Binding Path="AreAllButtonsCollapsed" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    </MultiBinding>
                                </telerik:RadButton.Visibility>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="CancelButton"
                                    MinWidth="{StaticResource CommentActionButtonMinWidth}"
                                    IsBackgroundVisible="False"
                                    Command="{Binding CancelClickCommand}"
                                    Content="Cancel"
                                    Cursor="Hand">
                                <telerik:RadButton.Visibility>
                                    <MultiBinding Converter="{StaticResource BoolToVisibilityMultipleValuesConverter}">
                                        <Binding Path="IsAnotherCommentReplyInEditMode" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                        <Binding Path="IsInEditMode"/>
                                        <Binding Path="AreAllButtonsCollapsed" Converter="{StaticResource BooleanToInversedBooleanConverter}"/>
                                    </MultiBinding>
                                </telerik:RadButton.Visibility>
                            </telerik:RadButton>
                        </StackPanel>
                    </Grid>
                </Grid>
            </Grid>
        </Border>
        <DataTemplate.Triggers>

            <!-- Opening comment -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=IsComment}" Value="True"/>
                    <Condition Binding="{Binding Path=IsCommentResolved}" Value="False"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:Windows11Resource ResourceKey=SecondaryBackgroundBrush}"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=IsComment}" Value="True"/>
                    <Condition Binding="{Binding Path=IsCommentResolved}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Opacity" Value="0.3"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=IsComment}" Value="False"/>
                    <Condition Binding="{Binding Path=IsCommentResolved}" Value="False"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:Windows11Resource ResourceKey=OverlayBrush}"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=IsComment}" Value="False"/>
                    <Condition Binding="{Binding Path=IsCommentResolved}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Opacity" Value="0.3"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsCommentResolved}" Value="True"/>
                    <Condition Binding="{Binding IsCommentVisible}" Value="False"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="OptionsButton" Property="IsOpen" Value="False"/>
            </MultiDataTrigger>
            <Trigger SourceName="PART_CommentTextBox" Property="IsReadOnly" Value="True">
                <Setter TargetName="PART_CommentTextBox" Property="Visibility" Value="Hidden"/>
            </Trigger>
            <Trigger SourceName="ExpandButton" Property="IsChecked" Value="True">
                <Setter TargetName="PreviewTb" Property="Height" Value="Auto"/>
                <Setter TargetName="ExpandButton" Property="IsHitTestVisible" Value="False"/>
            </Trigger>
            <Trigger SourceName="OptionsButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="OptionsButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="OptionsButton" Property="Width" Value="0"/>
            </Trigger>
            <Trigger SourceName="DeleteThreadButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="DeleteThreadButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="DeleteThreadButton" Property="Width" Value="0"/>
            </Trigger>
            <Trigger SourceName="EditButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="EditButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="EditButton" Property="Width" Value="0"/>
            </Trigger>
            <Trigger SourceName="DeleteButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="DeleteButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="DeleteButton" Property="Width" Value="0"/>
            </Trigger>
            <Trigger SourceName="SaveButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="SaveButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="SaveButton" Property="Width" Value="0"/>
            </Trigger>
            <Trigger SourceName="CancelButton" Property="Visibility" Value="Hidden">
                <Setter TargetName="CancelButton" Property="MinWidth" Value="0"/>
                <Setter TargetName="CancelButton" Property="Width" Value="0"/>
            </Trigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsMouseOver}" Value="True">
                <Setter TargetName="ActionButtonsPanel" Property="Opacity" Value="1"/>
            </DataTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <Style x:Key="CommentControlStyle" TargetType="spreadsheetControls:CommentControl">
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows11Resource ResourceKey=OverlayBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:CommentControl">
                    <Grid>
                        <Polygon x:Name="PurpleTriangle"
                                Points="{Binding PurpleTrianglePoints}"
                                Fill="{StaticResource CommentIndicatorBrush}"
                                Visibility="{Binding IsPurpleTriangleVisible, Converter={StaticResource BoolToVisibilityValueConverter}}"/>
                        <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid Visibility="{Binding IsCommentVisible, Converter={StaticResource BoolToVisibilityValueConverter}}" Width="{StaticResource CommentMinWidth}" MinHeight="{StaticResource CommentMinHeight}">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <ScrollViewer x:Name="CommentEntriesScrollViewer"
                                        MaxHeight="{StaticResource CommentListMaxHeight}"
                                        Background="{x:Null}"
                                        BorderThickness="0"
                                        Padding="0"
                                        Margin="0"
                                        HorizontalScrollBarVisibility="Disabled">
                                    <Grid>
                                        <ItemsControl
                                                ItemsSource="{Binding CommentEntriesViewModels}"
                                                ItemTemplate="{StaticResource CommentEntryItemTemplate}"
                                                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                Margin="0 -1 0 0"/>
                                    </Grid>
                                </ScrollViewer>
                                <Border Grid.Row="1" BorderThickness="0 1 0 0" BorderBrush="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}">
                                    <Grid Margin="10">
                                        <Grid Visibility="{Binding IsResolved, Converter={StaticResource InversedBoolToVisibilityValueConverter}}">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <TextBox
                                                    Name="PART_ReplyTextBox"
                                                    Grid.Row="0"
                                                    Text="{Binding EnteredText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextWrapping="Wrap"
                                                    AcceptsReturn="True"
                                                    MaxLines="5"
                                                    IsEnabled="{Binding IsReplyTextBoxEnabled}">
                                                <telerik:EventToCommandBehavior.EventBindings>
                                                    <telerik:EventBinding Command="{Binding ReplyTextBoxGotFocusCommand}" EventName="GotFocus"/>
                                                    <telerik:EventBinding Command="{Binding ReplyTextBoxLostFocusCommand}" EventName="LostFocus"/>
                                                </telerik:EventToCommandBehavior.EventBindings>
                                            </TextBox>
                                            <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="0 10 0 0" Visibility="{Binding AreReplyButtonsEnabled, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                                <telerik:RadButton x:Name="SendButton" Style="{StaticResource CommentReplyButtonStyle}" Command="{Binding SendClickCommand}" IsEnabled="{Binding HasAnyEnteredText}" Margin="0 0 4 0">
                                                    <Path
                                                            Fill="{Binding ElementName=SendButton, Path=Foreground}"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Data="M0.9,9.0 L18,9.0 0,16.5 z M0,0 L18,7.4 0.9,7.4 z"
                                                            Width="14"
                                                            Height="11"
                                                            Stretch="Fill"/>
                                                </telerik:RadButton>
                                                <telerik:RadButton x:Name="CancelButton" Style="{StaticResource CommentReplyButtonStyle}" Command="{Binding CancelClickCommand}">
                                                    <telerik:RadGlyph Glyph="{StaticResource GlyphClose}"/>
                                                </telerik:RadButton>
                                            </StackPanel>
                                        </Grid>
                                        <Grid Visibility="{Binding IsResolved, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <telerik:RadButton Content="Reopen thread" MinWidth="100" Command="{Binding ReopenThreadClickCommand}" IsBackgroundVisible="False" Cursor="Hand"/>
                                                <telerik:RadButton Content="Delete thread" MinWidth="100" Command="{Binding DeleteThreadClickCommand}" IsBackgroundVisible="False" Cursor="Hand"/>
                                            </StackPanel>
                                        </Grid>
                                    </Grid>
                                </Border>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding IsCommentVisible}" Value="False">
                            <Setter Property="BorderThickness" Value="0"/>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

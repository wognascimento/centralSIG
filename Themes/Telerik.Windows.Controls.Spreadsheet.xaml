                                                <Button
                                                        Content="{Binding Path=FoundCell.CellIndex, Converter={StaticResource CellIndexToNameConverter}}"
                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=grid:RadGridView} , Path=DataContext.GoToCellCommand}"
                                                        CommandParameter="{Binding Path=FoundCell}"
                                                        Style="{StaticResource LinkButtonStyle}"/>
                                            </DataTemplate>
                                        </grid:GridViewColumn.CellTemplate>
                                    </grid:GridViewColumn>
                                    <grid:GridViewColumn MinWidth="75">
                                        <grid:GridViewColumn.Header>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_Value"/>

                                <telerik:RadGlyph Glyph="{StaticResource GlyphArrowBackward}"/>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_GoToPreviousButton" Style="{StaticResource SheetSelectionRadButtonStyle}">
                                <telerik:RadGlyph Glyph="{StaticResource GlyphArrow60Left}"/>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_GoToNextButton" Style="{StaticResource SheetSelectionRadButtonStyle}">
                                <telerik:RadGlyph Glyph="{StaticResource GlyphArrow60Right}"/>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_GoToLastButton" Style="{StaticResource SheetSelectionRadButtonStyle}">

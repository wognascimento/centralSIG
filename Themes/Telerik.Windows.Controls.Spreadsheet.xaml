                                        <telerik:GridViewDataColumn IsSortable="False" IsFilterable="False" DataMemberBinding="{Binding Description}" Width="*">
                                            <telerik:GridViewDataColumn.Header>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_Rule"/>
                                            </telerik:GridViewDataColumn.Header>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewImageColumn ImageStretch="None" DataMemberBinding="{Binding FormatImage}" Width="150">
                                            <telerik:GridViewImageColumn.Header>
                                                <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ConditionalFormatting_ManageRulesDialog_Format"/>
                                            </telerik:GridViewImageColumn.Header>
                                        </telerik:GridViewImageColumn>
                                        <telerik:GridViewDataColumn IsReadOnly="True" Width="*">
                                            <telerik:GridViewDataColumn.Header>

                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_BorderMenu_MoreBorders"
                                        Command="{Binding Path=ShowFormatCellsDialog.Command}"
                                        IsEnabled="{Binding Path=ShowFormatCellsDialog.IsEnabled}"
                                        CommandParameter="Border">
                                    <telerik:RadMenuItem.Icon>
                                        <Image Source="{telerik:IconResource IconRelativePath=16/borders.png, IconSources={StaticResource IconPaths}}" Width="16"/>

                                                <RadioButton x:Name="HorizontalGradient"
                                                        GroupName="1"
                                                        IsChecked="{Binding Path=SelectedShadingStyle, Mode=TwoWay, Converter={StaticResource ShadingStyleToBooleanConverter}, ConverterParameter=Horizontal}"
                                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_Horizontal"
                                                        Margin="0 0 0 2"/>
                                                <RadioButton x:Name="VerticalGradient"
                                                        GroupName="2"
                                                        IsChecked="{Binding Path=SelectedShadingStyle, Mode=TwoWay, Converter={StaticResource ShadingStyleToBooleanConverter}, ConverterParameter=Vertical}"
                                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_Vertical"
                                                        Margin="0 0 0 2"/>
                                                <RadioButton x:Name="DiagonalUpGradient"
                                                        GroupName="3"
                                                        IsChecked="{Binding Path=SelectedShadingStyle, Mode=TwoWay, Converter={StaticResource ShadingStyleToBooleanConverter}, ConverterParameter=DiagonalUp}"
                                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_DiagonalUp"
                                                        Margin="0 0 0 2"/>
                                                <RadioButton x:Name="DiagonalDownGradient"
                                                        GroupName="4"
                                                        IsChecked="{Binding Path=SelectedShadingStyle, Mode=TwoWay, Converter={StaticResource ShadingStyleToBooleanConverter}, ConverterParameter=DiagonalDown}"
                                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_DiagonalDown"
                                                        Margin="0 0 0 2"/>
                                                <RadioButton x:Name="FromCornerGradient"
                                                        GroupName="5"
                                                        IsChecked="{Binding Path=SelectedShadingStyle, Mode=TwoWay, Converter={StaticResource ShadingStyleToBooleanConverter}, ConverterParameter=FromCorner}"
                                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FillEffectsDialog_FromCorner"
                                                        Margin="0 0 0 2"/>

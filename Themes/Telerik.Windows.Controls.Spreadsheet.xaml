                                                <telerik:RadListBoxItem Tag="Thin" IsSelected="True">
                                                    <Line Style="{StaticResource LineStyle}" Height="1"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="Hair">
                                                    <Line Style="{StaticResource LineStyle}" Height="1" StrokeDashArray="1.5 1.5"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="Dotted">
                                                    <Line Style="{StaticResource LineStyle}" Height="1" StrokeDashArray="2 2"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="Dashed">
                                                    <Line Style="{StaticResource LineStyle}" Height="1" StrokeDashArray="3 1"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="DashDot">
                                                    <Line Style="{StaticResource LineStyle}" Height="1" StrokeDashArray="8 2 2 2"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="DashDotDot">
                                                    <Line Style="{StaticResource LineStyle}" Height="1" StrokeDashArray="8 2 2 2 2 2"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="Medium">
                                                    <Line Style="{StaticResource LineStyle}" StrokeThickness="2"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="MediumDashed">
                                                    <Line Style="{StaticResource LineStyle}" StrokeThickness="2" StrokeDashArray="2 1"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="MediumDashed">
                                                    <Line Style="{StaticResource LineStyle}" StrokeThickness="2" StrokeDashArray="4 1 1 1"/>
                                                </telerik:RadListBoxItem>
                                                <telerik:RadListBoxItem Tag="MediumDashDotDot">
                                                    <Line Style="{StaticResource LineStyle}" StrokeThickness="2" StrokeDashArray="4 1 1 1 1 1"/>
                                                </telerik:RadListBoxItem>

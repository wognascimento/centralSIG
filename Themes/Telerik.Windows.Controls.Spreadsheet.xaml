                        </telerik:RadSplitButton.DropDownContent>
                    </telerik:RadSplitButton>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="AdornerControlStyle" TargetType="spreadsheetControls:AdornerControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid x:Name="AdornerRoot">
                        <Grid x:Name="gridControl" RenderTransformOrigin="0.5,0.5" ShowGridLines="False" Background="Transparent">
                            <Grid.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHorizontallyFlipped, Converter={StaticResource BoolToIntegerConverter}}" ScaleY="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsVerticallyFlipped, Converter={StaticResource BoolToIntegerConverter}}"/>
                                    <RotateTransform Angle="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RotationAngle}"/>
                                </TransformGroup>
                            </Grid.RenderTransform>
                            <Grid.Resources>
                                <Style TargetType="Ellipse" x:Key="ResizeStyle">
                                    <Setter Property="Margin" Value="-7"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
                                </Style>
                                <Style TargetType="Rectangle" x:Key="ResizeNoEdgeStyle">
                                    <Setter Property="Margin" Value="-7"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
                                </Style>
                                <Style TargetType="Ellipse" x:Key="RotateStyle">
                                    <Setter Property="Margin" Value="-30"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0"/>
                                <RowDefinition/>
                                <RowDefinition Height="0"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0"/>
                            </Grid.ColumnDefinitions>
                            <Image x:Name="ImagePreview" Stretch="Fill" Grid.Row="1" Grid.Column="1" Visibility="Collapsed" Opacity="1"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Width="1"
                                    Fill="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    Grid.RowSpan="3"
                                    HorizontalAlignment="Left"
                                    Grid.Column="1"
                                    VerticalAlignment="Stretch"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Width="1"
                                    Fill="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    Grid.RowSpan="3"
                                    HorizontalAlignment="Right"
                                    Grid.Column="1"
                                    VerticalAlignment="Stretch"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Height="1"
                                    Fill="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Stretch"
                                    Grid.Row="1"
                                    VerticalAlignment="Top"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Height="1"
                                    Fill="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    Stroke="Black"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Stretch"
                                    Grid.Row="1"
                                    VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="rotateLine"
                                    Visibility="{Binding Path=IsRotationEnabled, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    StrokeThickness="0"
                                    Height="24"
                                    Width="1"
                                    Fill="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Center"
                                    Grid.Row="1"
                                    VerticalAlignment="Top"
                                    Margin="0 -20 0 0"/>
                            <Ellipse x:Name="Rotate"
                                    Visibility="{Binding Path=IsRotationEnabled, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    Style="{StaticResource RotateStyle}"
                                    Stroke="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Grid.ColumnSpan="3"
                                    Grid.RowSpan="3"/>
                            <Ellipse x:Name="Resize00" Style="{StaticResource ResizeStyle}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize10" Style="{StaticResource ResizeStyle}" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize01" Style="{StaticResource ResizeStyle}" Grid.Row="2" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
                            <Ellipse x:Name="Resize11" Style="{StaticResource ResizeStyle}" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="ResizeTop" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Top" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeBottom" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Bottom" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeLeft" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeRight" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Right" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FunctionsMenuStyle" TargetType="spreadsheetControls:FunctionsMenu">
        <Setter Property="FontFamily" Value="{telerik:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:FunctionsMenu">
                    <telerik:RadContextMenu BorderThickness="0" Padding="0" mat:MaterialAssist.IsShadowDisabled="True">
                        <telerik:RadMenuGroupItem
                                Background="Transparent"
                                ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Items}"
                                DataContext="{Binding}"
                                IsEnabled="{Binding Path=ApplyFunction.IsEnabled}"
                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                MaxHeight="500"
                                MinWidth="120"
                                StaysOpenOnClick="False">
                            <telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                                <spreadsheetUtilities:SingleTemplateSelector>
                                    <spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                        <DataTemplate>
                                            <telerik:RadMenuItem
                                                    Header="{Binding Name}"
                                                    CommandParameter="{Binding Name}"
                                                    Command="{Binding Path=DataContext.ApplyFunction.Command, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}"
                                                    IsEnabled="{Binding Path=DataContext.ApplyFunction.IsEnabled, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}">
                                                <ToolTipService.ToolTip>
                                                    <ToolTip MinHeight="50">
                                                        <StackPanel>
                                                            <TextBlock Text="{Binding Name}" FontWeight="Medium" FontSize="{telerik:Windows11Resource ResourceKey=FontSizeS}"/>
                                                            <TextBlock Text="{Binding Path=FunctionInfo.Description}" TextWrapping="Wrap" MaxWidth="200" Margin="0 8 0 0" FontSize="{telerik:Windows11Resource ResourceKey=FontSizeS}"/>
                                                        </StackPanel>
                                                    </ToolTip>
                                                </ToolTipService.ToolTip>
                                            </telerik:RadMenuItem>
                                        </DataTemplate>
                                    </spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                </spreadsheetUtilities:SingleTemplateSelector>
                            </telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                        </telerik:RadMenuGroupItem>
                        <telerik:RadMenuSeparatorItem/>
                        <telerik:RadMenuItem
                                telerik:LocalizationManager.ResourceKey="Spreadsheet_FunctionsMenu_InsertFunction"
                                IsEnabled="{Binding Path=ShowInsertFunctionDialog.IsEnabled}"
                                Command="{Binding Path=ShowInsertFunctionDialog.Command}"
                                CommandParameter="{Binding Path=Category, RelativeSource={RelativeSource TemplatedParent}}">
                            <telerik:RadMenuItem.Icon>
                                <Border>
                                    <telerik:RadGlyph Glyph="{StaticResource GlyphFormulaFx}" Foreground="{telerik:Windows11Resource ResourceKey=IconBrush}"/>
                                </Border>
                            </telerik:RadMenuItem.Icon>
                        </telerik:RadMenuItem>
                    </telerik:RadContextMenu>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FilteringPopupControlStyle" TargetType="spreadsheetControls:FilteringPopupControl">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <telerik:RadDropDownButton x:Name="PART_DropDownButton"
                            Command="{Binding InitializePopupCommand}"
                            IsOpen="{Binding Path=IsPopupOpened, Mode=TwoWay}"
                            IsEnabled="{Binding Path=CanFilter}"
                            IsTabStop="False"
                            Padding="0"
                            Margin="0 -1 2 0"
                            IsBackgroundVisible="True"
                            MinHeight="0"
                            helpers:ThemeHelper.FocusVisualMargin="0"
                            VerticalAlignment="Center"
                            DropDownIndicatorVisibility="Collapsed"
                            Focusable="False">
                        <telerik:RadDropDownButton.Content>
                            <StackPanel>
                                <telerik:RadGlyph x:Name="SortIcon"
                                        Height="15"
                                        Foreground="{telerik:Windows11Resource ResourceKey=IconBrush}"
                                        Glyph="{StaticResource GlyphArrow60Down}"
                                        Visibility="{Binding Path=FilterIsApplied, Converter={StaticResource InversedBoolToVisibilityValueConverter}}"/>
                                <telerik:RadGlyph x:Name="FilterIcon"
                                        Height="15"
                                        Foreground="{telerik:Windows11Resource ResourceKey=IconBrush}"
                                        Glyph="{StaticResource GlyphFilter}"
                                        Visibility="{Binding Path=FilterIsApplied, Converter={StaticResource BoolToVisibilityValueConverter}}"/>
                            </StackPanel>
                        </telerik:RadDropDownButton.Content>
                        <telerik:RadDropDownButton.DropDownContent>
                            <telerik:RadContextMenu DataContext="{Binding ElementName=PART_DropDownButton, Path=DataContext}" BorderThickness="0" Margin="0">
                                <telerik:RadMenuItem Command="{Binding ElementName=PART_DropDownButton, Path=DataContext.SortCommand}" CommandParameter="Ascending" telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_SortSmallestToLargest">
                                    <telerik:RadMenuItem.Icon>
                                        <Image Source="{telerik:IconResource IconRelativePath=16/sortAscending.png, IconSources={StaticResource IconPaths}}" Width="16"/>
                                    </telerik:RadMenuItem.Icon>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuItem Command="{Binding ElementName=PART_DropDownButton, Path=DataContext.SortCommand}" CommandParameter="Descending" telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_SortLargestToSmallest">
                                    <telerik:RadMenuItem.Icon>
                                        <Image Source="{telerik:IconResource IconRelativePath=16/sortDescending.png, IconSources={StaticResource IconPaths}}" Width="16"/>
                                    </telerik:RadMenuItem.Icon>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_SortByColor" DataContext="{Binding ElementName=PART_DropDownButton, Path=DataContext}">
                                    <telerik:RadMenuGroupItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_SortByCellColor" Visibility="{Binding Path=RangeContainsFillColors, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                        <telerik:RadListBox ItemsSource="{Binding Path=FillColorsItemsSource}" SelectedIndex="{Binding Path=SelectedSortFillColorIndex, Mode=TwoWay}" BorderThickness="0">
                                            <telerik:RadListBox.ItemTemplateSelector>
                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                        <DataTemplate>
                                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_ColorSelectorNoFill" HorizontalAlignment="Center"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                        <DataTemplate>
                                                            <spreadsheetControls:PatternFillControl
                                                                    Width="50"
                                                                    Height="16"
                                                                    ColorScheme="{Binding ElementName=PART_DropDownButton, Path=DataContext.ColorScheme}"
                                                                    PatternType="{Binding Path=PatternType}"
                                                                    PatternColor="{Binding Path=., Converter={StaticResource PatternFillToPatternColorConverter}}"
                                                                    BackgroundColor="{Binding Path=., Converter={StaticResource PatternFillToBackgroundColorConverter}}"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.GradientFillTemplate>
                                                        <DataTemplate>
                                                            <spreadsheetControls:GradientFillControl
                                                                    MaxWidth="50"
                                                                    MaxHeight="16"
                                                                    ColorScheme="{Binding ElementName=PART_DropDownButton, Path=DataContext.ColorScheme}"
                                                                    GradientType="{Binding Path=GradientType}"
                                                                    Color1="{Binding Path=Color1}"
                                                                    Color2="{Binding Path=Color2}"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.GradientFillTemplate>
                                                </spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                            </telerik:RadListBox.ItemTemplateSelector>
                                        </telerik:RadListBox>
                                    </telerik:RadMenuGroupItem>
                                    <telerik:RadMenuGroupItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_SortByFontColor" Visibility="{Binding Path=RangeContainsFontColors, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                        <telerik:RadListBox ItemsSource="{Binding Path=ForeColorsItemsSource}" SelectedIndex="{Binding Path=SelectedSortForeColorIndex, Mode=TwoWay}" BorderThickness="0">
                                            <telerik:RadListBox.ItemTemplateSelector>
                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                        <DataTemplate>
                                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_ColorSelectorAutomatic" HorizontalAlignment="Center"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                        <DataTemplate>
                                                            <spreadsheetControls:PatternFillControl
                                                                    Width="50"
                                                                    Height="16"
                                                                    ColorScheme="{Binding ElementName=PART_DropDownButton, Path=DataContext.ColorScheme}"
                                                                    PatternType="{Binding Path=PatternType}"
                                                                    PatternColor="{Binding Path=., Converter={StaticResource PatternFillToPatternColorConverter}}"
                                                                    BackgroundColor="{Binding Path=., Converter={StaticResource PatternFillToBackgroundColorConverter}}"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                </spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                            </telerik:RadListBox.ItemTemplateSelector>
                                        </telerik:RadListBox>
                                    </telerik:RadMenuGroupItem>
                                    <telerik:RadMenuItem Command="{Binding Path=SortCommand}" telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_CustomSort">
                                        <telerik:RadMenuItem.Icon>
                                            <Image Source="{telerik:IconResource IconRelativePath=16/customSort.png, IconSources={StaticResource IconPaths}}" Width="16"/>
                                        </telerik:RadMenuItem.Icon>
                                    </telerik:RadMenuItem>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuSeparatorItem/>
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_ClearFilter" DataContext="{Binding ElementName=PART_DropDownButton, Path=DataContext}" Command="{Binding Path=ClearFilter}">
                                    <telerik:RadMenuItem.Icon>
                                        <Image Source="{telerik:IconResource IconRelativePath=16/filterClear.png, IconSources={StaticResource IconPaths}}" Width="16"/>
                                    </telerik:RadMenuItem.Icon>
                                </telerik:RadMenuItem>
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_FilterByColor" DataContext="{Binding ElementName=PART_DropDownButton, Path=DataContext}" IsEnabled="{Binding Path=RangeContainsColors, Mode=TwoWay}">
                                    <telerik:RadMenuItem.IsChecked>
                                        <Binding Path="Filter" Converter="{StaticResource FilterToVisibilityConverter}">
                                            <Binding.ConverterParameter>
                                                <spreadsheetControls:FilteringPopupConverterParameter IsColorFilter="True"/>
                                            </Binding.ConverterParameter>
                                        </Binding>
                                    </telerik:RadMenuItem.IsChecked>
                                    <telerik:RadMenuGroupItem telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_FilterByCellColor" Visibility="{Binding Path=RangeContainsFillColors, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                        <telerik:RadListBox
                                                ItemsSource="{Binding Path=FillColorsItemsSource}"
                                                SelectedIndex="{Binding Path=SelectedFilterFillColorIndex, Mode=TwoWay}"
                                                SelectedItem="{Binding Path=SelectedFilterFillColor, Mode=TwoWay}"
                                                BorderThickness="0">
                                            <telerik:RadListBox.ItemTemplateSelector>
                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                        <DataTemplate>
                                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FilteringPopup_ColorSelectorNoFill" HorizontalAlignment="Center"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                        <DataTemplate>
                                                            <spreadsheetControls:PatternFillControl
                                                                    Width="50"
                                                                    Height="16"
                                                                    ColorScheme="{Binding ElementName=PART_DropDownButton, Path=DataContext.ColorScheme}"
                                                                    PatternType="{Binding Path=PatternType}"
                                                                    PatternColor="{Binding Path=., Converter={StaticResource PatternFillToPatternColorConverter}}"
                                                                    BackgroundColor="{Binding Path=., Converter={StaticResource PatternFillToBackgroundColorConverter}}"/>
                                                        </DataTemplate>
                                                    </spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                    <spreadsheetUtilities:PatternGradientFillTemplateSelector.GradientFillTemplate>
                                                        <DataTemplate>
                                                            <spreadsheetControls:GradientFillControl

                                                Grid.Row="1"
                                                Grid.Column="0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Stretch="None"/>
                                        <StackPanel Grid.Column="2">
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_DataValidationDialog_Title" Margin="{StaticResource GeneralLabelOffset}"/>

    <Style x:Key="CommentsTaskPaneItemStyle" TargetType="spreadsheetControls:CommentsTaskPaneItem">
        <Setter Property="Background" Value="{telerik:Windows11Resource ResourceKey=OverlayBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 0 10"/>
        <Setter Property="MinWidth" Value="{StaticResource CommentMinWidth}"/>
        <Setter Property="MinHeight" Value="{StaticResource TaskPaneCommentMinHeight}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:CommentsTaskPaneItem">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid MinWidth="{TemplateBinding MinWidth}" MinHeight="{TemplateBinding MinHeight}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <ItemsControl
                                    Grid.Row="0"
                                    Margin="0 -1 0 0"
                                    ItemsSource="{Binding CommentEntriesViewModels}"
                                    ItemTemplate="{StaticResource CommentEntryItemTemplate}"
                                    ScrollViewer.HorizontalScrollBarVisibility="Disabled"/>
                            <Border Grid.Row="1" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0 1 0 0">
                                <Grid Margin="10">
                                    <Grid Visibility="{Binding IsResolved, Converter={StaticResource InversedBoolToVisibilityValueConverter}}">
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBox
                                                Name="PART_ReplyTextBox"
                                                Grid.Row="0"
                                                Text="{Binding EnteredText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                TextWrapping="Wrap"
                                                AcceptsReturn="True"
                                                MaxLines="5"
                                                IsEnabled="{Binding IsReplyTextBoxEnabled}">
                                            <telerik:EventToCommandBehavior.EventBindings>
                                                <telerik:EventBinding Command="{Binding ReplyTextBoxGotFocusCommand}" EventName="GotFocus"/>
                                                <telerik:EventBinding Command="{Binding ReplyTextBoxLostFocusCommand}" EventName="LostFocus"/>
                                            </telerik:EventToCommandBehavior.EventBindings>
                                        </TextBox>
                                        <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="0 10 0 0" Visibility="{Binding AreReplyButtonsEnabled, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                            <telerik:RadButton x:Name="SendButton"
                                                    Style="{StaticResource CommentReplyButtonStyle}"
                                                    Padding="0"
                                                    MinWidth="32"
                                                    Command="{Binding SendClickCommand}"
                                                    IsEnabled="{Binding IsAnyEnteredText}"
                                                    Margin="0 0 4 0">
                                                <Path
                                                        Fill="{Binding ElementName=SendButton, Path=Foreground}"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Data="M0.9,9.0 L18,9.0 0,16.5 z M0,0 L18,7.4 0.9,7.4 z"
                                                        Width="14"
                                                        Height="11"
                                                        Stretch="Fill"/>
                                            </telerik:RadButton>
                                            <telerik:RadButton x:Name="CancelButton" Style="{StaticResource CommentReplyButtonStyle}" Padding="0" MinWidth="32" Command="{Binding CancelClickCommand}">
                                                <telerik:RadGlyph Glyph="{StaticResource GlyphClose}"/>
                                            </telerik:RadButton>
                                        </StackPanel>
                                    </Grid>
                                    <Grid Visibility="{Binding IsResolved, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <telerik:RadButton Content="Reopen thread" MinWidth="100" Command="{Binding ReopenThreadClickCommand}" IsBackgroundVisible="False" Cursor="Hand"/>
                                            <telerik:RadButton Content="Delete thread" MinWidth="100" Command="{Binding DeleteThreadClickCommand}" IsBackgroundVisible="False" Cursor="Hand"/>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </Border>
                            <Grid Grid.Row="2" Margin="10 0 10 10" Visibility="{Binding IsCommentPending, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                <telerik:Label
                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_CommentsTaskPane_PostComment"
                                        Foreground="{StaticResource CommentInfoMessageBrush}"
                                        VerticalAlignment="Center"
                                        Padding="0"/>
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TaskPaneStyle" TargetType="spreadsheetControls:TaskPane">

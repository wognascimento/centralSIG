                                    <ContentControl.ContentTemplateSelector>
                                        <dialogs:ContainsViewDataTemplateSelector>
                                            <dialogs:ContainsViewDataTemplateSelector.CellValueTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition/>
                                                        </Grid.ColumnDefinitions>
                                                        <telerik:RadComboBox
                                                                Margin="0 3 5 0"
                                                                VerticalAlignment="Center"
                                                                SelectedValue="{Binding SelectedRuleType, Mode=TwoWay}"
                                                                ItemsSource="{Binding RuleTypes}"
                                                                DisplayMemberPath="DisplayName"
                                                                SelectedValuePath="Value"
                                                                Width="120"
                                                                helpers:ThemeHelper.FocusVisualMargin="0"/>
                                                        <ContentControl Grid.Column="1" Content="{Binding SelectedFormatViewModel}">
                                                            <ContentControl.ContentTemplateSelector>
                                                                <dialogs:CellValueDataTemplateSelector>
                                                                    <dialogs:CellValueDataTemplateSelector.SingleArgumentTemplate>
                                                                        <DataTemplate>
                                                                            <Grid>
                                                                                <TextBox Text="{Binding Argument}" Margin="0 3 5 0" VerticalAlignment="Center"/>
                                                                            </Grid>
                                                                        </DataTemplate>
                                                                    </dialogs:CellValueDataTemplateSelector.SingleArgumentTemplate>
                                                                    <dialogs:CellValueDataTemplateSelector.DualArgumentTemplate>
                                                                        <DataTemplate>
                                                                            <Grid>
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition/>
                                                                                    <ColumnDefinition Width="Auto"/>
                                                                                    <ColumnDefinition/>
                                                                                </Grid.ColumnDefinitions>
                                                                                <TextBox Text="{Binding Argument1}" Margin="0 3 5 0" Grid.Column="0" VerticalAlignment="Center"/>
                                                                                <TextBlock Text="and" Margin="0 3 5 0" Grid.Column="1" VerticalAlignment="Center"/>
                                                                                <TextBox Text="{Binding Argument2}" Margin="0 3 5 0" Grid.Column="2" VerticalAlignment="Center"/>
                                                                            </Grid>
                                                                        </DataTemplate>
                                                                    </dialogs:CellValueDataTemplateSelector.DualArgumentTemplate>
                                                                </dialogs:CellValueDataTemplateSelector>
                                                            </ContentControl.ContentTemplateSelector>
                                                        </ContentControl>
                                                    </Grid>
                                                </DataTemplate>
                                            </dialogs:ContainsViewDataTemplateSelector.CellValueTemplate>
                                            <dialogs:ContainsViewDataTemplateSelector.DateTemplate>
                                                <DataTemplate>
                                                    <telerik:RadComboBox
                                                            HorizontalAlignment="Left"
                                                            Width="120"
                                                            VerticalAlignment="Center"
                                                            Margin="0 3 5 0"
                                                            SelectedValue="{Binding SelectedPeriod}"
                                                            ItemsSource="{Binding Periods}"
                                                            SelectedValuePath="Value"
                                                            DisplayMemberPath="DisplayName"
                                                            helpers:ThemeHelper.FocusVisualMargin="0"/>
                                                </DataTemplate>
                                            </dialogs:ContainsViewDataTemplateSelector.DateTemplate>
                                            <dialogs:ContainsViewDataTemplateSelector.BlankTemplate>
                                                <DataTemplate/>
                                            </dialogs:ContainsViewDataTemplateSelector.BlankTemplate>
                                            <dialogs:ContainsViewDataTemplateSelector.TextTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition/>
                                                        </Grid.ColumnDefinitions>
                                                        <telerik:RadComboBox
                                                                Width="120"
                                                                VerticalAlignment="Center"
                                                                Margin="0 3 5 0"
                                                                SelectedValue="{Binding SelectedOperator}"
                                                                ItemsSource="{Binding Operators}"
                                                                SelectedValuePath="Value"
                                                                DisplayMemberPath="DisplayName"
                                                                helpers:ThemeHelper.FocusVisualMargin="0"/>
                                                        <TextBox Margin="0 3 5 0" Text="{Binding Text, Mode=TwoWay}" Grid.Column="1" VerticalAlignment="Center"/>
                                                    </Grid>
                                                </DataTemplate>
                                            </dialogs:ContainsViewDataTemplateSelector.TextTemplate>
                                        </dialogs:ContainsViewDataTemplateSelector>
                                    </ContentControl.ContentTemplateSelector>
                                </ContentControl>
                            </Grid>
                        </Grid>
                        <ContentControl Content="{Binding Format}" ContentTemplate="{StaticResource FormatTemplate}" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="1"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

                            <Grid Grid.Row="1" Margin="0 5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadComboBox
                                        VerticalAlignment="Center"
                                        Margin="0 3 5 0"
                                        SelectedValue="{Binding SelectedRuleType, Mode=TwoWay}"

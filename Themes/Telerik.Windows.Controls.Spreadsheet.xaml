                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                    </telerik:RadGridView.Columns>
                                </telerik:RadGridView>
                            </Grid>
                        </Border>
                        <Border x:Name="HorizontalRule"
                                Grid.Row="2"
                                VerticalAlignment="Top"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                Background="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                BorderThickness="0 1 0 0"
                                MinHeight="64">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="{StaticResource DialogsFooterMargin}">
                                <telerik:RadButton x:Name="PART_ButtonOK" telerik:LocalizationManager.ResourceKey="Ok" MinWidth="112" Margin="{StaticResource DialogsFooterButtonMargin}"/>
                                <telerik:RadButton x:Name="PART_ButtonCancel" telerik:LocalizationManager.ResourceKey="Cancel" MinWidth="112" Margin="{StaticResource DialogsFooterButtonMargin}"/>
                                <telerik:RadButton x:Name="PART_ButtonApply"
                                        telerik:LocalizationManager.ResourceKey="Apply"
                                        MinWidth="112"
                                        Margin="{StaticResource DialogsFooterButtonMargin}"
                                        IsEnabled="{Binding HasChanges}"/>
                            </StackPanel>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

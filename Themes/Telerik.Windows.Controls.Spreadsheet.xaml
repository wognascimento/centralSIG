    <Style x:Key="BorderTabContentStyle" TargetType="dialogs:BorderTabContent">
        <Setter Property="FontFamily" Value="{telerik:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Width" Value="534"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:BorderTabContent">
                    <Grid Background="{TemplateBinding Background}" Margin="8" VerticalAlignment="Top">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" MinWidth="150"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <dialogs:BorderStyleSelector x:Name="TableBorderSelector" CurrentBorder="{Binding Path=CurrentBorder, Mode=TwoWay}" ColorPalette="{Binding Path=ColorPalette}"/>
                        <dialogs:BorderSelector x:Name="BorderSelector"
                                Grid.Column="1"
                                UseLayoutRounding="True"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Stretch"
                                LeftBorder="{Binding Path=LeftBorder, Mode=TwoWay}"
                                TopBorder="{Binding Path=TopBorder, Mode=TwoWay}"
                                RightBorder="{Binding Path=RightBorder, Mode=TwoWay}"
                                BottomBorder="{Binding Path=BottomBorder, Mode=TwoWay}"
                                InsideHorizontalBorder="{Binding Path=InsideHorizontalBorder, Mode=TwoWay}"
                                InsideVerticalBorder="{Binding Path=InsideVerticalBorder, Mode=TwoWay}"
                                DiagonalDownBorder="{Binding Path=DiagonalDownBorder, Mode=TwoWay}"
                                DiagonalUpBorder="{Binding Path=DiagonalUpBorder, Mode=TwoWay}"
                                SelectedColumnsCount="{Binding Path=SelectedColumnsCount}"
                                SelectedRowsCount="{Binding Path=SelectedRowsCount}"
                                CurrentBorder="{Binding Path=CurrentBorder}"
                                ColorPalette="{Binding Path=ColorPalette}"
                                Margin="16 0 0 0"/>
                        <TextBlock
                                Grid.Row="1"
                                Grid.ColumnSpan="2"
                                TextWrapping="Wrap"
                                telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Border_Info"
                                Margin="{StaticResource GeneralLabelOffset}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PatternFillControlsBaseStyle" TargetType="spreadsheetControls:PatternFillControl">
        <Setter Property="Padding" Value="2 1"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:PatternFillControl">
                    <Border MinHeight="22" BorderThickness="1" BorderBrush="Transparent" Background="{Binding ElementName=Container, Path=Background}">
                        <Canvas x:Name="Container"
                                Margin="{TemplateBinding Padding}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

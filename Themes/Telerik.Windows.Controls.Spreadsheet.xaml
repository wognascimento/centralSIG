                                        <StackPanel Grid.Row="1" Grid.Column="2">
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_DataValidationDialog_ErrorMessage" Margin="{StaticResource GeneralLabelOffset}"/>
                                            <TextBox
                                                    Text="{Binding Path=ErrorAlertContent, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    AutomationProperties.AutomationId="errorAlertContent"
                                                    VerticalScrollBarVisibility="Visible"
                                                    TextWrapping="Wrap"
                                                    MaxLength="235"
                                                    Height="100"
                                                    VerticalContentAlignment="Top"
                                                    HorizontalContentAlignment="Stretch"
                                                    IsEnabled="{Binding Path=ShowErrorAlertAfterInvalidDataIsEntered}"/>
                                        </StackPanel>
                                    </Grid>
                                </StackPanel>
                            </telerik:RadTabItem>
                        </telerik:RadTabControl>
                        <Border
                                Grid.Row="1"
                                Background="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                BorderThickness="0 1 0 0"
                                MinHeight="64">
                            <Grid Margin="{StaticResource DialogsFooterMargin}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

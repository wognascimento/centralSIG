                        <Trigger SourceName="PART_signaturePanel_closeButton" Property="IsPressed" Value="True">
                            <Setter TargetName="CloseIcon" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

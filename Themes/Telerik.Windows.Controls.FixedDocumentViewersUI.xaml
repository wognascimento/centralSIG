                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="8 0">
                                <Image Source="/Telerik.Windows.Controls.FixedDocumentViewers;component/Images/Modern/signature16x16_valid.png" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SignatureGeneralStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Valid}" Stretch="None"/>
                                <Image Source="/Telerik.Windows.Controls.FixedDocumentViewers;component/Images/Modern/signature16x16_invalid.png" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SignatureGeneralStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Invalid}" Stretch="None"/>
                                <Image Source="/Telerik.Windows.Controls.FixedDocumentViewers;component/Images/Modern/signature16x16_error.png" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SignatureGeneralStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Unknown}" Stretch="None"/>

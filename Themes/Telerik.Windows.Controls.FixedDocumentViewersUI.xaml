                            <Setter TargetName="GlyphReset" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="GlyphReset" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                        <Trigger SourceName="PART_PrintButton" Property="IsEnabled" Value="False">
                            <Setter TargetName="GlyphPrint" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="GlyphPrint" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                        <Trigger SourceName="PART_SaveButton" Property="IsEnabled" Value="False">
                            <Setter TargetName="GlyphSave" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="GlyphSave" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                        <Trigger SourceName="PART_OpenButton" Property="IsEnabled" Value="False">
                            <Setter TargetName="GlyphFolderOpen" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="GlyphFolderOpen" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>

                                Focusable="false"
                                HorizontalAlignment="Right">
                            <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronRight}" Foreground="{Binding (TextElement.Foreground), RelativeSource={RelativeSource AncestorType=ContentPresenter}}"/>

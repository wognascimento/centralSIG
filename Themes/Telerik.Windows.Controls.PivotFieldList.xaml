                            </ContentControl>
                            <Border
                                    Grid.Row="1"
                                    BorderThickness="0 1 0 0"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    Margin="0 8 0 0"
                                    Background="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"

<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        mc:Ignorable="c"
        xmlns:fieldList="clr-namespace:Telerik.Windows.Controls.FieldList;assembly=Telerik.Windows.Controls.PivotFieldList"
        xmlns:core="clr-namespace:System;assembly=mscorlib"
        xmlns:helpers="clr-namespace:Telerik.Windows.Controls.Theming.Helpers;assembly=Telerik.Windows.Controls"
        xmlns:telerik1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
        xmlns:pivot="http://schemas.telerik.com/2008/xaml/presentation/pivot"
        xmlns:local="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.PivotFieldList"
        xmlns:tree="clr-namespace:Telerik.Windows.Controls.Pivot;assembly=Telerik.Windows.Controls.Pivot"
        xmlns:editing="clr-namespace:Telerik.Windows.Controls.FieldList.Editing;assembly=Telerik.Windows.Controls.PivotFieldList">
    <ResourceDictionary.MergedDictionaries>
        <telerik:Windows11ResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <telerik1:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik1:ThicknessToOrientedThicknessConverter x:Key="ThicknessToOrientedThicknessConverter"/>
    <telerik1:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <telerik1:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <Geometry x:Key="ArrowDown7x4">M0,0L7,0 3.5,4z</Geometry>
    <FontFamily x:Key="TelerikWebUI">/Telerik.Windows.Controls;component/Themes/Fonts/old/TelerikWebUI.ttf#TelerikWebUI</FontFamily>
    <core:String x:Key="GlyphArrowChevronUp"></core:String>
    <core:String x:Key="GlyphArrowChevronRight"></core:String>
    <core:String x:Key="GlyphArrowChevronDown"></core:String>
    <core:String x:Key="GlyphArrowChevronLeft"></core:String>
    <core:String x:Key="GlyphClose"></core:String>
    <core:String x:Key="GlyphFilter"></core:String>
    <core:String x:Key="GlyphZoom"></core:String>
    <core:String x:Key="GlyphThumbnailsUp"></core:String>
    <core:String x:Key="GlyphThumbnailsLeft"></core:String>
    <core:String x:Key="GlyphFullScreen"></core:String>
    <core:String x:Key="GlyphSum"></core:String>
    <core:String x:Key="GlyphFolderOpen"></core:String>
    <core:String x:Key="GlyphFieldsMore"></core:String>
    <editing:BooleanToSortOrderConverter x:Key="BooleanToSortOrderConverter"/>
    <editing:ComparisonToVisibleConverter x:Key="ComparisonToVisibleConverter"/>
    <editing:IntervalComparisonToVisibilityConverter x:Key="IntervalComparisonToVisibilityConverter"/>
    <editing:NamedToStringConverter x:Key="NamedToStringConverter"/>
    <editing:Top10FilterTypeToVisibilityConverter x:Key="Top10FilterTypeToVisibilityConverter"/>
    <MenuScrollingVisibilityConverter x:Key="PivotMenuScrollingVisibilityConverter"/>
    <Style x:Key="TextBlockTitleStyle" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="{telerik:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>
    <fieldList:FieldDragActionToSourceConverter x:Key="IconConverter"
            ColumnSource="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/VisualStudio2013/DropOnColumns.png"
            FilterSource="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/VisualStudio2013/DropOnFilters.png"
            RemoveSource="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/VisualStudio2013/Remove.png"
            RowSource="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/VisualStudio2013/DropOnRows.png"
            ValueSource="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/VisualStudio2013/DropOnValues.png"/>
    <fieldList:FieldListContextMenuBehavior x:Key="FieldListContextMenuBehavior"/>
    <fieldList:WindowDialogHostingBehavior x:Key="WindowDialogHostingBehavior"/>
    <fieldList:InlineFieldListContextMenuBehavior x:Key="InlineFieldListContextMenuBehavior"/>
    <fieldList:EditorsBehavior x:Key="EditorsBehavior">
        <fieldList:EditorsBehavior.Editors>
            <fieldList:SetSumAggregateEditor/>
            <fieldList:SetAverageAggregateEditor/>
            <fieldList:SetCountAggregateEditor/>
            <fieldList:SummarizationEditor/>
            <fieldList:LabelFilterEditor/>
            <fieldList:ValueFilterEditor/>
            <fieldList:Top10FilterEditor/>
            <fieldList:ClearFilterEditor/>
            <fieldList:SortingEditor/>
            <fieldList:SetIndexTotalFormatEditor/>
            <fieldList:SetPercentOfGrandTotalFormatEditor/>
            <fieldList:TotalFormatEditor/>
            <fieldList:ClearTotalFormatEditor/>

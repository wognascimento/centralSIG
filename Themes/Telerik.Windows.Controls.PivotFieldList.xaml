    <ControlTemplate x:Key="PivotClearFilterButtonTemplate" TargetType="telerik:RadButton">
        <Border x:Name="BorderVisual"
                BorderThickness="{TemplateBinding BorderThickness}"
                BorderBrush="{TemplateBinding BorderBrush}"
                Background="{TemplateBinding Background}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <ContentPresenter x:Name="Content"
                    TextBlock.Foreground="{TemplateBinding Foreground}"
                    Margin="{TemplateBinding Padding}"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                    ContentStringFormat="{TemplateBinding ContentStringFormat}"
                    RecognizesAccessKey="True"/>
        </Border>
    </ControlTemplate>
    <Style x:Key="PivotClearFilterButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=IconBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource PivotClearFilterButtonTemplate}"/>
        <Setter Property="Padding" Value="5 0"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="{telerik:Windows11Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="PivotItemsFilteringControlStyle" TargetType="editing:PivotItemsFilteringControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:PivotItemsFilteringControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <telerik:GroupBox Grid.Row="0" telerik:LocalizationManager.ResourceKey="PivotFieldList_SelectItem">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <telerik:RadWatermarkTextBox x:Name="SearchBox"
                                        Grid.Row="0"
                                        WatermarkContent="{telerik:LocalizableResource Key=PivotFieldList_Search}"
                                        CurrentText="{Binding SearchText, Mode=TwoWay}"
                                        Margin="5">
                                    <telerik:RadWatermarkTextBox.AdditionalContent>
                                        <Grid>
                                            <telerik:RadButton x:Name="ClearButton"
                                                    Command="{x:Static telerik:RadWatermarkTextBoxCommands.Clear}"
                                                    CommandTarget="{Binding ElementName=SearchBox}"
                                                    Style="{StaticResource PivotClearFilterButtonStyle}"
                                                    Visibility="{Binding SearchText, Converter={StaticResource NullToVisibilityConverter}}">
                                                <telerik:RadGlyph Glyph="{StaticResource GlyphClose}"/>
                                            </telerik:RadButton>
                                            <telerik:RadGlyph x:Name="SearchIcon"
                                                    Glyph="{StaticResource GlyphZoom}"
                                                    Foreground="{Binding ElementName=ClearButton, Path=Foreground}"
                                                    Margin="{Binding ElementName=ClearButton, Path=Padding}"/>
                                        </Grid>
                                    </telerik:RadWatermarkTextBox.AdditionalContent>
                                </telerik:RadWatermarkTextBox>
                                <ItemsControl x:Name="PART_DistinctFilter" Grid.Row="1" ItemsSource="{Binding Selection, FallbackValue={x:Null}}" Margin="5">
                                    <ItemsControl.Template>
                                        <ControlTemplate TargetType="ItemsControl">
                                            <ScrollViewer BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" CanContentScroll="True">
                                                <ItemsPresenter/>
                                            </ScrollViewer>
                                        </ControlTemplate>
                                    </ItemsControl.Template>
                                    <ItemsControl.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <VirtualizingStackPanel/>
                                        </ItemsPanelTemplate>
                                    </ItemsControl.ItemsPanel>
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate>
                                            <CheckBox
                                                    Content="{Binding FallbackValue={x:Null}}"
                                                    ContentTemplate="{StaticResource NamedObjectTemplate}"
                                                    Margin="0 0 0 5"
                                                    IsChecked="{Binding IsSelected, Mode=TwoWay, FallbackValue={x:Null}}"/>
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>
                                <Border x:Name="NoResultsBorder"
                                        Grid.Row="1"
                                        Margin="{Binding ElementName=PART_DistinctFilter, Path=Margin}"
                                        Background="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadWindow}}, Path=Background}"
                                        Visibility="Collapsed">
                                    <TextBlock telerik:LocalizationManager.ResourceKey="PivotFieldList_NoResults" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0 20 0 0"/>
                                </Border>
                            </Grid>
                        </telerik:GroupBox>
                        <telerik:GroupBox Grid.Row="1" telerik:LocalizationManager.ResourceKey="PivotFieldList_ItemFilterConditionCaption">
                            <ContentPresenter x:Name="PART_OtherFilterPlaceholder"/>
                        </telerik:GroupBox>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger SourceName="ClearButton" Property="Visibility" Value="Visible">
                            <Setter TargetName="SearchIcon" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding SearchText, Converter={StaticResource NullToVisibilityConverter}}" Value="Visible"/>
                                <Condition Binding="{Binding SelectionView.Count}" Value="1"/>
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="NoResultsBorder" Property="Visibility" Value="Visible"/>
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="OperatorValueFilterControlStyle" TargetType="editing:OperatorValueFilterControl">

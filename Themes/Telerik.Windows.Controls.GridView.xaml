            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True"/>
                    <Condition Property="DisplayVisualCues" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_CellBorder" Property="Background" Value="{Binding MouseOverBackground, RelativeSource={RelativeSource TemplatedParent}}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True"/>
                    <Condition Property="DisplayVisualCues" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_CellBorder" Property="Background" Value="{Binding SelectedBackground, RelativeSource={RelativeSource TemplatedParent}}"/>
            </MultiTrigger>
            <MultiTrigger>

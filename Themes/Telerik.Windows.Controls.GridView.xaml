    <ControlTemplate x:Key="GridViewGroupPanelCellTemplate" TargetType="grid:GridViewGroupPanelCell">
        <Grid UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="00:00:00.2" To="MouseOver"/>
                        <VisualTransition GeneratedDuration="00:00:00.2" To="Normal"/>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames
                                    BeginTime="00:00:00"
                                    Duration="00:00:00.00"
                                    Storyboard.TargetName="ContentElement"
                                    Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
                                <EasingDoubleKeyFrame KeyTime="00:00:0.150" Value="-4"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="OuterBorder"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                    UseLayoutRounding="True"/>
            <Grid x:Name="SortIndicatorGrid" Grid.Column="1" Background="#01FFFFFF" VerticalAlignment="Top" HorizontalAlignment="Center" Opacity="0">
                <telerik:RadGlyph x:Name="PART_SortIndicator"
                        IsHitTestVisible="False"
                        Glyph="{StaticResource GlyphArrowChevronDown}"
                        Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                        FontSize="12"/>
            </Grid>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentControl x:Name="ContentElement"
                        Content="{TemplateBinding Content}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                        Margin="{TemplateBinding Padding}"
                        Foreground="{TemplateBinding Foreground}"
                        IsTabStop="{TemplateBinding IsTabStop}">
                    <ContentControl.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </ContentControl.RenderTransform>
                </ContentControl>
                <Button x:Name="PART_CloseButton"
                        Grid.Column="1"
                        AutomationProperties.Name="close"
                        Style="{StaticResource ButtonCloseStyle}"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Right"
                        Margin="0 0 8 0"
                        Opacity="0"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="PART_CloseButton" Property="Opacity" Value="1"/>
                <Setter TargetName="OuterBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush)}"/>
                <Setter TargetName="OuterBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
            </Trigger>
            <Trigger SourceName="SortIndicatorGrid" Property="IsMouseOver" Value="True">
                <Setter TargetName="PART_SortIndicator" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=AccentMouseOverBrush}"/>
            </Trigger>
            <DataTrigger Binding="{Binding Path=SortDirection}" Value="Ascending">
                <Setter Property="Opacity" TargetName="SortIndicatorGrid" Value="1"/>
                <Setter TargetName="PART_SortIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronUp}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SortDirection}" Value="Descending">
                <Setter Property="Opacity" TargetName="SortIndicatorGrid" Value="1"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SortDirection}" Value="{x:Null}">
                <Setter Property="Opacity" TargetName="SortIndicatorGrid" Value="0"/>
            </DataTrigger>
            <Trigger SourceName="PART_CloseButton" Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
                <Setter TargetName="OuterBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBackgroundBrush)}"/>
                <Setter TargetName="OuterBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="PART_SortIndicator" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="PART_CloseButton" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="ContentElement" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="PART_SortIndicator" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="PART_CloseButton" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="OuterBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBackgroundBrush)}"/>
                <Setter TargetName="OuterBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

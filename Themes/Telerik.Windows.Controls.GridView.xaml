    <ControlTemplate x:Key="GridViewValidationToolTipTemplate" TargetType="ToolTip">
        <Grid x:Name="Root" Opacity="0" Background="Transparent" RenderTransformOrigin="0 0" Margin="1 0 0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25"/>
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Border
                    Grid.Column="1"
                    Padding="5"
                    MinHeight="32"
                    Background="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                <ItemsControl ItemsSource="{TemplateBinding Content}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <TextBlock
                                    MaxWidth="250"
                                    Foreground="{telerik1:Windows11Resource ResourceKey=AccentControlForegroundBrush}"
                                    FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"
                                    FontFamily="{telerik1:Windows11Resource ResourceKey=FontFamily}"
                                    VerticalAlignment="Center"
                                    Text="{Binding}"
                                    TextWrapping="Wrap"/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="OpenAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="CloseAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="-25" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter Property="Opacity" Value="1" TargetName="Root"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="False">
                <Setter Property="Opacity" Value="0" TargetName="Root"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

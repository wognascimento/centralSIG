    <ControlTemplate x:Key="GridViewEditorPresenterValidationToolTipTemplate" TargetType="ToolTip">
        <Grid x:Name="Root" Opacity="0" Background="Transparent" RenderTransformOrigin="0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25"/>
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Border Padding="8 0" Grid.Column="1" MinHeight="32" Background="{telerik1:Windows11Resource ResourceKey=ValidationBrush}">
                <TextBlock
                        MaxWidth="250"
                        FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"
                        FontFamily="{telerik1:Windows11Resource ResourceKey=FontFamily}"
                        Foreground="{telerik1:Windows11Resource ResourceKey=AccentControlForegroundBrush}"
                        VerticalAlignment="Center"
                        Text="{TemplateBinding Content}"
                        TextWrapping="Wrap"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="OpenAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="CloseAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="-25" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter TargetName="Root" Property="Opacity" Value="1"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=InputBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinHeight" Value="32"/>
        <Setter Property="Padding" Value="8 0 0 0"/>
        <Setter Property="DropDownMinWidth" Value="125"/>
        <Setter Property="DropDownMinHeight" Value="100"/>
        <Setter Property="DropDownHeight" Value="330"/>
        <Setter Property="DropDownMaxHeight" Value="500"/>
        <Setter Property="DropDownMaxWidth" Value="1000"/>
        <Setter Property="EditorMinWidth" Value="30"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource StaticValidationTooltipTemplate}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadMultiColumnComboBox">
                    <Grid Margin="{TemplateBinding helpers:ThemeHelper.FocusVisualMargin}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid MinHeight="{TemplateBinding MinHeight}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="BackgroundVisual"
                                    Grid.ColumnSpan="2"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Background="{TemplateBinding Background}"
                                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                            <mccb:SearchAutoCompleteBox x:Name="PART_SearchAutoCompleteBox"
                                    Grid.Column="0"
                                    MinHeight="0"
                                    helpers:ThemeHelper.CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=LeftTopBottom}"
                                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent},Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopBottom}"
                                    BorderBrush="{x:Null}"
                                    Padding="{TemplateBinding Padding}"
                                    AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                    SelectionOnFocus="{TemplateBinding SelectionOnFocus}"
                                    SelectionBoxesVisibility="{TemplateBinding SelectionBoxesVisibility}"
                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                    DeferTextChange="{TemplateBinding DeferTextChange}"
                                    NullText="{TemplateBinding NullText}"
                                    SelectedItems="{TemplateBinding SelectedItems}"
                                    EditorMinWidth="{TemplateBinding EditorMinWidth}"
                                    SelectionBoxTemplate="{TemplateBinding SelectionBoxTemplate}"
                                    SelectionBoxTemplateSelector="{TemplateBinding SelectionBoxTemplateSelector}"
                                    SelectionBoxStyle="{TemplateBinding SelectionBoxStyle}"
                                    SelectionBoxStyleSelector="{TemplateBinding SelectionBoxStyleSelector}"
                                    AutoCompleteProvider="{TemplateBinding AutoCompleteProvider}"/>
                            <telerik:RadDropDownButton x:Name="PART_DropDownButton"
                                    Grid.Column="1"
                                    KeepOpen="{TemplateBinding KeepDropDownOpen}"
                                    PopupPlacementTarget="{Binding ElementName=BackgroundVisual}"
                                    DropDownWidth="{TemplateBinding DropDownWidth}"
                                    DropDownHeight="{TemplateBinding DropDownHeight}"
                                    DropDownMinWidth="{TemplateBinding DropDownMinWidth}"
                                    DropDownMinHeight="{TemplateBinding DropDownMinHeight}"
                                    DropDownMaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    DropDownMaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    DropDownPlacement="{TemplateBinding DropDownPlacement}"
                                    Margin="0 0 5 0"
                                    BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                    BorderThickness="0"
                                    helpers:ThemeHelper.FocusBrush="{x:Null}"
                                    helpers:ThemeHelper.FocusBackgroundBrush="{x:Null}"
                                    helpers:ThemeHelper.FocusVisualMargin="0"
                                    helpers:ThemeHelper.MouseOverBackgroundBrush="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"
                                    helpers:ThemeHelper.PressedBackgroundBrush="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"
                                    MinHeight="0"
                                    Width="22"
                                    Height="22"
                                    HorizontalContentAlignment="Center"
                                    Background="Transparent"
                                    Padding="0"
                                    IsTabStop="False"
                                    DropDownIndicatorVisibility="Collapsed"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                <telerik:RadDropDownButton.Content>
                                    <telerik:RadGlyph x:Name="GlyphClose"
                                            FontSize="{telerik1:Windows11Resource ResourceKey=FontSizeS}"
                                            Glyph="{StaticResource GlyphArrowChevronDown}"
                                            Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                </telerik:RadDropDownButton.Content>
                                <telerik:RadDropDownButton.DropDownContent>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <mat:Shadow
                                                Grid.RowSpan="2"
                                                Grid.ColumnSpan="2"
                                                Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                                                ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}"
                                                CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
                                        <ContentControl Content="{TemplateBinding ItemsSourceProvider}" Grid.ColumnSpan="2" Grid.RowSpan="1">
                                            <ContentControl.ContentTemplate>
                                                <DataTemplate>
                                                    <controls:RadGridView
                                                            GroupRenderMode="Flat"
                                                            CanUserSearchInHiddenColumns="{Binding CanUserSearchInHiddenColumns}"
                                                            AutoGenerateColumns="{Binding AutoGenerateColumns}"
                                                            AlternateRowStyle="{Binding AlternateRowStyle}"
                                                            AlternateRowStyleSelector="{Binding AlternateRowStyleSelector}"
                                                            RowStyle="{Binding RowStyle}"
                                                            RowStyleSelector="{Binding RowStyleSelector}"
                                                            AlternationCount="{Binding AlternationCount}"
                                                            ShowColumnFooters="{Binding ShowColumnFooters}"
                                                            ShowColumnHeaders="{Binding ShowColumnHeaders}"
                                                            ColumnWidth="{Binding ColumnWidth}"
                                                            ContextMenu="{Binding ContextMenu}"
                                                            IsReadOnly="{Binding IsReadOnly}"
                                                            RowIndicatorVisibility="{Binding RowIndicatorVisibility}"
                                                            ShowSearchPanel="False"
                                                            ShowGroupPanel="False"
                                                            CanUserGroupColumns="False"
                                                            CanUserSearch="False"
                                                            DataContext="{Binding}"
                                                            SelectionMode="Extended"
                                                            EnableLostFocusSelectedState="False"
                                                            SearchStateManager="{Binding SearchStateManager}"
                                                            ItemsSource="{Binding CollectionView}"
                                                            IsSynchronizedWithCurrentItem="False"
                                                            BorderThickness="0"/>
                                                </DataTemplate>
                                            </ContentControl.ContentTemplate>
                                        </ContentControl>
                                        <Border
                                                Grid.Row="1"
                                                Grid.ColumnSpan="2"
                                                BorderThickness="0 1 0 0"
                                                BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimarySolidBorderBrush}"
                                                Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                                                Visibility="{TemplateBinding FooterVisibility}"
                                                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                                            <ContentPresenter x:Name="Footer" Margin="5" Content="{TemplateBinding FooterContent}" ContentTemplate="{TemplateBinding FooterContentTemplate}" VerticalAlignment="Center">
                                                <ContentPresenter.Resources>
                                                    <Style TargetType="TextBlock">
                                                        <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                                                        <Setter Property="Margin" Value="{Binding ElementName=CornerResizerIcon, Path=ActualWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Right}"/>
                                                    </Style>
                                                </ContentPresenter.Resources>
                                            </ContentPresenter>
                                        </Border>
                                        <Path x:Name="CornerResizerIcon"
                                                Grid.Row="1"
                                                Grid.ColumnSpan="2"
                                                Data="M8.0,0 L12,0 12,8 12,12 8.0,12 0,12 0,8 8.0,8 z"
                                                Fill="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                                                Stretch="Fill"
                                                StrokeThickness="0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Bottom"
                                                Width="8"
                                                Height="8"
                                                Margin="2 3"
                                                Visibility="{TemplateBinding FooterVisibility}"/>
                                        <Thumb x:Name="PART_HorizontalResizer"
                                                Grid.Column="1"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Width="3"
                                                Cursor="SizeWE"
                                                HorizontalAlignment="Right"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        <Thumb x:Name="PART_CornerResizer"
                                                Grid.Column="1"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Height="3"
                                                Width="3"
                                                Cursor="SizeNWSE"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Right"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        <Thumb x:Name="PART_VerticalResizer"
                                                Grid.Column="0"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Height="3"
                                                Cursor="SizeNS"
                                                VerticalAlignment="Bottom"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                    </Grid>
                                </telerik:RadDropDownButton.DropDownContent>
                            </telerik:RadDropDownButton>
                            <Border x:Name="FocusVisual" Grid.ColumnSpan="2" Style="{StaticResource InputFocusVisualBorderStyle}" CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush)}"/>
                        </Trigger>
                        <Trigger SourceName="PART_DropDownButton" Property="IsPressed" Value="True">
                            <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}"/>
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBackgroundBrush)}"/>
                            <Setter TargetName="GlyphClose" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                        </Trigger>
                        <Trigger SourceName="PART_DropDownButton" Property="IsOpen" Value="True">
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBackgroundBrush)}"/>
                            <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}"/>
                            <Setter TargetName="GlyphClose" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBackgroundBrush)}"/>
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsReadOnly" Value="True">
                            <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.ReadOnlyBrush)}"/>
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.ReadOnlyBackgroundBrush)}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBackgroundBrush)}"/>
                            <Setter TargetName="BackgroundVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                            <Setter TargetName="PART_DropDownButton" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                        </Trigger>
                        <Trigger Property="Validation.HasError" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="DropDownWidth" Value="NaN">
                            <Setter TargetName="PART_DropDownButton" Property="DropDownWidth" Value="{Binding RelativeSource={RelativeSource AncestorType=controls:RadMultiColumnComboBox}, Path=ActualWidth}"/>
                        </Trigger>
                        <Trigger Property="FooterVisibility" Value="Visible">
                            <Setter TargetName="PART_CornerResizer" Property="Width" Value="{Binding ElementName=CornerResizerIcon, Path=Width}"/>
                            <Setter TargetName="PART_CornerResizer" Property="Height" Value="{Binding ElementName=CornerResizerIcon, Path=Height}"/>
                            <Setter TargetName="PART_CornerResizer" Property="Grid.Row" Value="1"/>
                            <Setter TargetName="PART_VerticalResizer" Property="Grid.Row" Value="1"/>
                        </Trigger>
                        <Trigger Property="CanUserResizeDropDown" Value="False">
                            <Setter TargetName="CornerResizerIcon" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="controls:RadMultiColumnComboBox" BasedOn="{StaticResource RadMultiColumnComboBoxStyle}"/>
</ResourceDictionary>

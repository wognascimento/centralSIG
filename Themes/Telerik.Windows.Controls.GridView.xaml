                                HorizontalAlignment="Right"
                                VerticalAlignment="Top">
                            <telerik:RadGlyph Glyph="{StaticResource GlyphClose}" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </telerik:RadButton>
                    </Grid>
                </Border>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="PART_FilterCloseButton" Property="IsPressed" Value="True">
                <Setter TargetName="PART_FilterCloseButton" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_ClearButton" Property="Visibility" Value="Visible">

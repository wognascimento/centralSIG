                    VerticalAlignment="Stretch"
                    LayoutTransform="{TemplateBinding ZoomScaleTransform}"/>
            <Border x:Name="PART_FrozenColumnsPreviewRight"
                    Grid.Row="2"
                    Grid.RowSpan="3"
                    Width="4"
                    Opacity="0.5"
                    Background="{telerik1:Windows11Resource ResourceKey=AccentSelectedBrush}"
                    Visibility="Collapsed"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Stretch"
                    LayoutTransform="{TemplateBinding ZoomScaleTransform}"/>
            <grid:GridViewLoadingIndicator x:Name="PART_GridViewLoadingIndicator"
                    Grid.RowSpan="4"
                    Grid.ColumnSpan="3"
                    Visibility="Collapsed"
                    IsBusy="{TemplateBinding IsBusy}"
                    ProgressValue="{Binding BusyIndicatorProgressValue, RelativeSource={RelativeSource TemplatedParent}}"
                    IsIndeterminate="{Binding BusyIndicatorIsIndeterminate, RelativeSource={RelativeSource TemplatedParent}}"
                    BusyContent="{Binding BusyIndicatorContent, RelativeSource={RelativeSource TemplatedParent}}"
                    BusyContentTemplate="{Binding BusyIndicatorContentTemplate, RelativeSource={RelativeSource TemplatedParent}}"/>
            <Grid x:Name="DragBetweenItemsFeedback"
                    Grid.Row="2"
                    Grid.ColumnSpan="3"
                    Height="8"
                    Visibility="Collapsed"
                    HorizontalAlignment="Left"
                    IsHitTestVisible="False"
                    VerticalAlignment="Top"
                    Margin="{TemplateBinding Padding}"
                    LayoutTransform="{TemplateBinding ZoomScaleTransform}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Ellipse Stroke="{telerik1:Windows11Resource ResourceKey=AccentSelectedBrush}" Width="8" Height="8" StrokeThickness="2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                <Rectangle Grid.Column="1" Height="2" Fill="{telerik1:Windows11Resource ResourceKey=AccentSelectedBrush}" RadiusX="2" RadiusY="2" Margin="-2 3 0 0" VerticalAlignment="Top"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsBusy" Value="True">
                <Setter TargetName="PART_GridViewLoadingIndicator" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="ShowColumnFooters" Value="false">
                <Setter Property="Visibility" Value="Collapsed" TargetName="PART_FooterRow"/>
            </Trigger>
            <Trigger Property="ShowColumnHeaders" Value="false">
                <Setter Property="Visibility" Value="Collapsed" TargetName="PART_HeaderRow"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadTreeListViewStyle" TargetType="controls:RadTreeListView">

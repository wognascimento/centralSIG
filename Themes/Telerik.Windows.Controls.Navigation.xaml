    <ControlTemplate x:Key="RadNavigationViewTemplate" TargetType="telerikNavigation:RadNavigationView">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="PART_DismissOverlay" Grid.RowSpan="2" Grid.Column="1" Panel.ZIndex="2" Fill="Transparent" Visibility="Collapsed"/>
            <Border x:Name="HeaderBackground"
                    Grid.ColumnSpan="2"
                    Visibility="Collapsed"
                    Panel.ZIndex="1"
                    Height="{TemplateBinding PaneHeaderHeight}"
                    Padding="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}"
                    Background="{TemplateBinding PaneHeaderBackground}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopLeftRight}"
                    CornerRadius="{Binding Path=(helpers:ThemeHelper.CornerRadius), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopLeftRight}">
                <ContentPresenter x:Name="HeaderPresenter" ContentSource="PaneHeader" TextBlock.Foreground="{TemplateBinding PaneHeaderForeground}" VerticalAlignment="Center"/>
            </Border>
            <telerik1:RadToggleButton x:Name="PaneToggleButton"
                    Panel.ZIndex="2"
                    HorizontalAlignment="Left"
                    Content="{TemplateBinding PaneToggleButtonContent}"
                    ContentTemplate="{TemplateBinding PaneToggleButtonContentTemplate}"
                    IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsPaneOpen, Mode=TwoWay}"
                    Visibility="{TemplateBinding PaneToggleButtonVisibility}"
                    AutomationProperties.Name="PaneToggleButton"
                    Style="{TemplateBinding PaneToggleButtonStyle}"
                    Margin="5 5 0 0"/>
            <Border x:Name="ContentBorder"
                    Grid.Column="1"
                    Grid.RowSpan="2"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopRightBottom}"
                    CornerRadius="{Binding (helpers:ThemeHelper.CornerRadius), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopRightBottom}">
                <ContentPresenter x:Name="PART_Content" ContentSource="Content" TextBlock.Foreground="{TemplateBinding Foreground}" Margin="{TemplateBinding Padding}"/>
            </Border>
            <ScrollViewer x:Name="PART_FlyoutOverlay"
                    Grid.Column="1"
                    Grid.RowSpan="2"
                    Panel.ZIndex="3"
                    Background="{TemplateBinding PaneBackground}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="1"
                    helpers:ThemeHelper.CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"
                    Visibility="Collapsed"/>
            <Grid x:Name="PART_PaneRoot" Grid.Column="0" Grid.RowSpan="2" Panel.ZIndex="1" Width="{TemplateBinding PaneWidth}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Border x:Name="PaneRootBorder"
                        Grid.RowSpan="3"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        BorderBrush="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                        Background="{TemplateBinding PaneBackground}"
                        CornerRadius="{Binding (helpers:ThemeHelper.CornerRadius), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomLeft}"/>
                <Grid Grid.Row="0" Height="{TemplateBinding PaneHeaderHeight}" Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}">
                    <Border Background="{TemplateBinding PaneHeaderBackground}" BorderThickness="{TemplateBinding BorderThickness}"/>
                    <ContentPresenter x:Name="RootPresenter" ContentSource="PaneHeader" Margin="8 0" TextBlock.Foreground="{TemplateBinding PaneHeaderForeground}" VerticalAlignment="Center"/>
                </Grid>
                <ScrollViewer x:Name="PART_ScrollViewer"
                        Grid.Row="1"
                        HorizontalScrollBarVisibility="Disabled"
                        VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}"
                        CanContentScroll="{TemplateBinding ScrollViewer.CanContentScroll}"
                        Background="{x:Null}"
                        Padding="4"
                        BorderThickness="0">
                    <ItemsPresenter/>
                </ScrollViewer>
                <Border x:Name="PaneFooterContentBorder"
                        Grid.Row="2"
                        BorderBrush="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                        Margin="0 0 0 1"
                        CornerRadius="{Binding (helpers:ThemeHelper.CornerRadius), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}, ConverterParameter=LeftRightBottom}">
                    <ContentPresenter ContentSource="PaneFooter"/>
                </Border>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPaneOpen" Value="True"/>
                    <Condition Property="DisplayMode" Value="Compact"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPaneOpen" Value="True"/>
                    <Condition Property="DisplayMode" Value="Minimal"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible"/>
            </MultiTrigger>
            <Trigger Property="DisplayMode" Value="Compact">
                <Setter TargetName="ContentBorder" Property="Grid.Column" Value="0"/>
                <Setter TargetName="ContentBorder" Property="Grid.ColumnSpan" Value="2"/>
                <Setter TargetName="ContentBorder" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CompactPaneWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}"/>
            </Trigger>
            <Trigger Property="DisplayMode" Value="Minimal">
                <Setter TargetName="ContentBorder" Property="Grid.Row" Value="1"/>
                <Setter TargetName="ContentBorder" Property="Grid.Column" Value="0"/>
                <Setter TargetName="ContentBorder" Property="Grid.ColumnSpan" Value="2"/>
                <Setter TargetName="ContentBorder" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}"/>
                <Setter TargetName="ContentBorder" Property="CornerRadius" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=BottomLeftRight}"/>
                <Setter TargetName="HeaderBackground" Property="Visibility" Value="Visible"/>
                <Setter TargetName="PART_PaneRoot" Property="Panel.ZIndex" Value="0"/>
                <Setter TargetName="PaneToggleButton" Property="Margin" Value="5 5 0 5"/>
            </Trigger>
            <Trigger SourceName="PART_FlyoutOverlay" Property="Visibility" Value="Visible">
                <Setter TargetName="PART_DismissOverlay" Property="Grid.Column" Value="0"/>
                <Setter TargetName="PART_DismissOverlay" Property="Grid.ColumnSpan" Value="2"/>
                <Setter TargetName="PART_DismissOverlay" Property="Panel.ZIndex" Value="3"/>
                <Setter TargetName="PART_DismissOverlay" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="RootPresenter" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="ContentBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                <Setter TargetName="ContentBorder" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBackgroundBrush)}"/>
                <Setter TargetName="PaneRootBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                <Setter TargetName="RootPresenter" Property="TextBlock.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
            </Trigger>
            <Trigger Property="PaneHeaderHeight" Value="0">
                <Setter TargetName="PaneToggleButton" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style TargetType="telerikNavigation:RadNavigationView" x:Key="RadNavigationViewStyle">
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBackgroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledForegroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="PaneBackground" Value="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"/>
        <Setter Property="PaneHeaderBackground" Value="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"/>
        <Setter Property="PaneHeaderForeground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="PaneHeaderHeight" Value="48"/>
        <Setter Property="CompactPaneWidth" Value="48"/>
        <Setter Property="SubItemsIndentation" Value="40"/>
        <Setter Property="ExpandedPaneWidth" Value="320"/>
        <Setter Property="PaneToggleButtonStyle" Value="{StaticResource PaneToggleButtonBaseStyle}"/>
        <Setter Property="PaneToggleButtonContent" Value="{StaticResource GlyphMenu}"/>
        <Setter Property="PaneToggleButtonContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <telerik1:RadGlyph Glyph="{Binding}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource RadNavigationViewTemplate}"/>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ResizeAnimation AnimationName="ResizePaneAnimation" TargetElementName="PART_PaneRoot" Duration="0:0:0.2" ResizeMode="Horizontal">
                        <animation:ResizeAnimation.Easing>
                            <ExponentialEase EasingMode="EaseOut"/>
                        </animation:ResizeAnimation.Easing>
                    </animation:ResizeAnimation>
                    <animation:SlideAnimation AnimationName="MinimalPaneOpenAnimation" TargetElementName="PART_PaneRoot" Duration="0:0:0.2" PixelsToAnimate="250" Orientation="Horizontal" Direction="In"/>
                    <animation:SlideAnimation AnimationName="MinimalPaneCloseAnimation" TargetElementName="PART_PaneRoot" Duration="0:0:0.2" PixelsToAnimate="250" Orientation="Horizontal" Direction="Out"/>
                    <animation:ExpandCollapseAnimation AnimationName="ExpandItemAnimation" Direction="In" SpeedRatio="2" TargetElementName="PART_NavigationViewSubItemsHost"/>
                    <animation:ExpandCollapseAnimation AnimationName="CollapseItemAnimation" Direction="Out" SpeedRatio="2" TargetElementName="PART_NavigationViewSubItemsHost"/>
                    <animation:ExpandCollapseAnimation AnimationName="FlyoutExpandItemAnimation" Direction="In" Orientation="Horizontal" SpeedRatio="2" TargetElementName="PART_NavigationViewSubItemsHost"/>
                    <animation:ExpandCollapseAnimation AnimationName="FlyoutCollapseItemAnimation" Direction="Out" Orientation="Horizontal" SpeedRatio="2" TargetElementName="PART_NavigationViewSubItemsHost"/>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadNavigationViewItemTemplate" TargetType="telerikNavigation:RadNavigationViewItem">
        <Grid x:Name="PART_LayoutRoot">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid x:Name="PART_ContainerGrid">
                <Border x:Name="BackgroundVisual"
                        SnapsToDevicePixels="True"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        Background="{TemplateBinding Background}"
                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                <Border x:Name="SelectedVisual"
                        Background="{TemplateBinding helpers:ThemeHelper.CheckedBackgroundBrush}"
                        BorderBrush="{TemplateBinding helpers:ThemeHelper.CheckedBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                        Opacity="0"/>
                <Border x:Name="HighlightedVisual"
                        Background="{TemplateBinding helpers:ThemeHelper.MouseOverBackgroundBrush}"
                        BorderBrush="{TemplateBinding helpers:ThemeHelper.MouseOverBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                        Opacity="0"/>
                <Border x:Name="PressedVisual"
                        Background="{TemplateBinding helpers:ThemeHelper.PressedBackgroundBrush}"
                        BorderBrush="{TemplateBinding helpers:ThemeHelper.PressedBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                        Opacity="0"/>
                <Rectangle x:Name="Pill" Margin="{Binding BorderThickness, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Left}" Style="{StaticResource DefaultItemPillStyle}">
                    <Rectangle.RenderTransform>
                        <ScaleTransform ScaleY="0.4"/>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Grid x:Name="ItemContentGrid" MinHeight="{TemplateBinding MinHeight}" Background="Transparent" Margin="-4 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="IconColumn" Width="{Binding RelativeSource={RelativeSource AncestorType=telerikNavigation:RadNavigationView}, Path=CompactPaneWidth}"/>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ContentPresenter x:Name="IconContent" ContentSource="Icon" TextElement.Foreground="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}" Grid.Column="0"/>
                    <ContentPresenter x:Name="Content"
                            Grid.Column="1"
                            RecognizesAccessKey="True"
                            TextElement.Foreground="{TemplateBinding Foreground}"
                            Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    <Grid x:Name="PART_ExpandCollapseIconHost" Grid.Column="2" Background="Transparent" MinWidth="{TemplateBinding MinHeight}">
                        <ContentPresenter x:Name="ExpandedIcon"
                                Content="{TemplateBinding ExpandedIcon}"
                                ContentTemplate="{TemplateBinding ExpandedIconTemplate}"
                                TextElement.Foreground="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"
                                Opacity="0"/>
                        <ContentPresenter x:Name="CollapsedIcon"
                                Content="{TemplateBinding CollapsedIcon}"
                                ContentTemplate="{TemplateBinding CollapsedIconTemplate}"
                                TextElement.Foreground="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"
                                Opacity="0"/>
                    </Grid>
                </Grid>
            </Grid>
            <telerikNavigation:NavigationViewSubItemsHost x:Name="PART_NavigationViewSubItemsHost"
                    Grid.Row="1"
                    Background="Transparent"
                    ItemsSource="{TemplateBinding ItemsSource}"
                    DisplayMemberPath="{TemplateBinding DisplayMemberPath}"
                    ItemTemplate="{TemplateBinding ItemTemplate}"
                    ItemContainerStyle="{TemplateBinding ItemContainerStyle}"
                    UseLayoutRounding="True"
                    Focusable="False"
                    Visibility="Collapsed"/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsTopLevel}" Value="False">
                <Setter TargetName="ItemContentGrid" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CalculatedItemIndentation, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Left}"/>
            </DataTrigger>
            <Trigger Property="IsExpanded" Value="True">
                <Setter TargetName="ExpandedIcon" Property="Opacity" Value="1"/>
            </Trigger>
            <Trigger Property="IsExpanded" Value="False">
                <Setter TargetName="CollapsedIcon" Property="Opacity" Value="1"/>
            </Trigger>
            <Trigger Property="IconVisibility" Value="Collapsed">
                <Setter TargetName="IconColumn" Property="Width" Value="0"/>
            </Trigger>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter TargetName="HighlightedVisual" Property="Opacity" Value="1"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True"/>
                    <Condition Property="IsHighlighted" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1"/>
            </MultiTrigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="PressedVisual" Property="Opacity" Value="1"/>
            </Trigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="False"/>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1"/>
                <Setter TargetName="Pill" Property="Opacity" Value="1"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsExpanded}" Value="True"/>
                    <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=HasSelectedSubItem}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="0"/>
            </MultiDataTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True"/>
                    <Condition Property="IsHighlighted" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="HighlightedVisual" Property="Background" Value="{telerik:Windows11Resource ResourceKey=SelectedMouseOverBrush}"/>
            </MultiTrigger>

            <!-- Selected and animation enabled -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="True"/>
                    <Condition Property="IsSelected" Value="True"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Pill" Storyboard.TargetProperty="RenderTransform.ScaleY" From="0" To="0.4" Duration="0:0:.3">
                                <DoubleAnimation.EasingFunction>
                                    <SineEase EasingMode="EaseOut"/>
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Storyboard.TargetName="Pill" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:.3"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.EnterActions>
                <MultiTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Pill" Storyboard.TargetProperty="RenderTransform.ScaleY" From="0.4" To="0" Duration="0:0:.3">
                                <DoubleAnimation.EasingFunction>
                                    <SineEase EasingMode="EaseIn"/>
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Storyboard.TargetName="Pill" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:.3"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.ExitActions>
            </MultiTrigger>

            <!-- Selected and animation disabled -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
                    <Condition Property="IsSelected" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="Pill" Property="Opacity" Value="1"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="PART_LayoutRoot" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="IconContent" Property="TextElement.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                <Setter TargetName="ExpandedIcon" Property="TextElement.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                <Setter TargetName="CollapsedIcon" Property="TextElement.Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

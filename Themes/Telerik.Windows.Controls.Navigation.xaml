                            <telerik:RadGlyph x:Name="GripOrnamentH" Visibility="Visible" Glyph="{StaticResource GlyphMoreVertical}" FontSize="16" Foreground="{telerik1:Windows11Resource ResourceKey=IconBrush}"/>
                            <telerik:RadGlyph x:Name="GripOrnamentV"
                                    Visibility="Collapsed"
                                    Glyph="{StaticResource GlyphMoreHorizontal}"
                                    FontSize="16"
                                    Foreground="{telerik1:Windows11Resource ResourceKey=IconBrush}"/>
                        </Grid>
                        <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel" Grid.Row="0" Grid.Column="1" Orientation="Horizontal" Margin="{TemplateBinding Padding}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Orientation" Value="Horizontal"/>
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*"/>
                            <Setter TargetName="c1" Property="Width" Value="Auto"/>
                            <Setter TargetName="r0" Property="Height" Value="Auto"/>
                            <Setter TargetName="r1" Property="Height" Value="*"/>
                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_Overflow" Property="Placement" Value="Right"/>
                            <Setter TargetName="PART_Overflow" Property="HorizontalOffset" Value="0"/>
                            <Setter TargetName="PART_Overflow" Property="VerticalOffset" Value="0"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                            <Setter TargetName="GripOrnamentH" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="GripOrnamentH" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="GripOrnamentV" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="GripOrnamentV" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

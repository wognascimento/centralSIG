    <Style x:Key="BreadCrumbHistoryToggleButtonStyle" TargetType="telerik:RadToggleButton" BasedOn="{StaticResource RadToggleButtonStyle}">
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=MouseOverBackgroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=PressedBackgroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBackgroundBrush" Value="Transparent"/>
        <Setter Property="IsBackgroundVisible" Value="False"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Margin" Value="0 0 5 0"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Style.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPressed" Value="True"/>
                    <Condition Property="IsChecked" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </MultiTrigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="BreadCrumbButtonStyle" TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{x:Null}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1 0 0 0"/>
        <Setter Property="CornerRadius" Value="0"/>
    </Style>
    <Style x:Key="BreadCrumbToggleButtonStyle" TargetType="telerik:RadToggleButton" BasedOn="{StaticResource RadToggleButtonStyle}">
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{x:Null}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBackgroundBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="MinWidth" Value="22"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
                <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPressed" Value="True"/>
                    <Condition Property="IsChecked" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </MultiTrigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="BreadcrumbSplitButtonStyle" TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledForegroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
        <Setter Property="ButtonPartStyle" Value="{StaticResource BreadCrumbButtonStyle}"/>
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{x:Null}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                IsHitTestVisible="False"/>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <telerik:RadButton x:Name="ButtonPart"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Style="{TemplateBinding ButtonPartStyle}"
                                    ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                    ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                    Content="{TemplateBinding Content}"
                                    IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            <Border x:Name="Divider" Grid.Row="1" Grid.Column="2" BorderThickness="1 0 0 0" BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}" Canvas.ZIndex="2"/>
                            <telerik:RadToggleButton x:Name="DropDownPart"
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                    Style="{TemplateBinding TogglePartStyle}"
                                    IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                    Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                <telerik:RadToggleButton.Content>
                                    <telerik:RadGlyph x:Name="arrow"
                                            IsHitTestVisible="False"
                                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                            Glyph="{StaticResource GlyphArrowChevronRight}"/>
                                </telerik:RadToggleButton.Content>
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                PopupAnimation="{TemplateBinding PopupAnimation}"
                                HorizontalOffset="0"
                                VerticalOffset="{telerik1:Windows11Resource ResourceKey=PopupOffset}">
                            <Grid x:Name="PopupGrid" Margin="4">
                                <mat:Shadow Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}" ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}" CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
                                <Border x:Name="DropDownPopupBorder"
                                        MinWidth="3"
                                        MinHeight="3"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"
                                        Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                                        BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                        BorderThickness="1">
                                    <ContentControl x:Name="DropDownPopupContent"
                                            Content="{TemplateBinding DropDownContent}"
                                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!-- enabled & opened -->
                        <Trigger SourceName="DropDownPart" Property="IsChecked" Value="True">
                            <Setter TargetName="arrow" Property="Glyph" Value="{StaticResource GlyphArrowChevronDown}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="ButtonPart" Property="IsMouseOver" Value="False"/>
                                <Condition SourceName="ButtonPart" Property="IsPressed" Value="False"/>
                                <Condition SourceName="DropDownPart" Property="IsMouseOver" Value="False"/>
                                <Condition SourceName="DropDownPart" Property="IsPressed" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Divider" Property="BorderBrush" Value="Transparent"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="ButtonPart" Property="IsMouseOver" Value="True"/>
                                <Condition Property="IsOpen" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}"/>
                            <Setter TargetName="DropDownPart" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}"/>
                        </MultiTrigger>
                        <Trigger SourceName="DropDownPart" Property="IsMouseOver" Value="True">
                            <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
                            <Setter TargetName="DropDownPart" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="arrow" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                            <Setter TargetName="ButtonPart" Property="helpers:ThemeHelper.DisabledBrush" Value="Transparent"/>
                            <Setter TargetName="DropDownPart" Property="helpers:ThemeHelper.DisabledBrush" Value="Transparent"/>
                            <Setter TargetName="Divider" Property="BorderBrush" Value="Transparent"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="BreadcrumbOverflowSplitButtonStyle" TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{x:Null}"/>
        <Setter Property="IsBackgroundVisible" Value="False"/>
        <Setter Property="IsButtonPartVisible" Value="False"/>
        <Setter Property="MinWidth" Value="32"/>
        <Setter Property="MaxWidth" Value="150"/>
        <Setter Property="DropDownMaxHeight" Value="700"/>
        <Setter Property="DropDownMaxWidth" Value="400"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}"/>
        <Setter Property="IsTabStop" Value="False"/>
    </Style>
    <Style x:Key="BreadcrumbTextSplitButton" TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.DisabledForegroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbHistoryToggleButtonStyle}"/>
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentControl x:Name="Content"
                                    Content="{TemplateBinding Content}"
                                    Foreground="{TemplateBinding Foreground}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalContentAlignment="Stretch"
                                    IsTabStop="False"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            <telerik:RadToggleButton x:Name="DropDownPart"
                                    Grid.Column="1"
                                    Style="{TemplateBinding TogglePartStyle}"
                                    IsTabStop="False"
                                    Focusable="False"
                                    VerticalAlignment="Stretch"
                                    Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                    IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                                <telerik:RadGlyph x:Name="DropDownIndicator" IsHitTestVisible="False" Glyph="{StaticResource GlyphArrowOverflowDown}"/>
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                IsOpen="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                HorizontalOffset="0"
                                VerticalOffset="{telerik1:Windows11Resource ResourceKey=PopupOffset}">
                            <Grid Margin="4">
                                <mat:Shadow Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}" ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}" CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
                                <Border x:Name="DropDownPopupBorder"
                                        Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                                        BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                        BorderThickness="1"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}">
                                    <ContentPresenter x:Name="DropDownPopupContent"
                                            Content="{TemplateBinding DropDownContent}"
                                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible"/>
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="headerContainer" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <telerik:RadSplitButton x:Name="SplitButton"
                        Foreground="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}"
                        Content="{TemplateBinding Header}"
                        FontSize="{TemplateBinding FontSize}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                        Style="{TemplateBinding SplitButtonStyle}"
                        Command="{x:Static telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem}"
                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" MaxWidth="270" MinWidth="148" MaxHeight="250" Padding="0 5" Margin="0" BorderThickness="0" VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter/>
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
            <Border x:Name="FocusVisual"
                    IsHitTestVisible="False"
                    BorderBrush="{TemplateBinding helpers:ThemeHelper.FocusBrush}"
                    BorderThickness="{telerik1:Windows11Resource ResourceKey=FocusThickness}"
                    Background="{x:Null}"
                    Visibility="Collapsed"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="HasItems" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsLinearMode" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsButtonPartHidden" Value="True">
                <Setter TargetName="SplitButton" Property="IsButtonPartVisible" Value="False"/>
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid MinHeight="40">
            <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}"/>
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="32"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Image Style="{StaticResource BreadcrumbImageStyle}" Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                <ContentControl x:Name="contentControl"
                        Grid.Column="1"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        FontSize="{TemplateBinding FontSize}"
                        Foreground="{TemplateBinding Foreground}"
                        Content="{TemplateBinding DropDownHeader}"
                        ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                        Margin="{TemplateBinding Padding}"
                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="helpers:ThemeHelper.FocusBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBackgroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Windows11Resource ResourceKey=AccentBrush}"/>
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"/>
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=ButtonBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="MinHeight" Value="32"/>
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}"/>
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}"/>
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}"/>
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid Margin="{TemplateBinding helpers:ThemeHelper.FocusVisualMargin}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid x:Name="outerGrid" MinHeight="{TemplateBinding MinHeight}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="32"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="NormalVisual"
                                    Grid.ColumnSpan="3"
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                            <Image x:Name="image"
                                    Style="{StaticResource BreadcrumbImageStyle}"
                                    Margin="5 0"
                                    Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                            <telerik:RadSplitButton x:Name="historySplitButton"
                                    Grid.Column="1"
                                    Grid.ColumnSpan="2"
                                    MinHeight="{TemplateBinding MinHeight}"
                                    IsTabStop="False"
                                    Style="{StaticResource BreadcrumbTextSplitButton}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}"
                                    IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}">
                                <telerik:RadSplitButton.Content>
                                    <TextBox x:Name="pathTextBox"
                                            AutomationProperties.Name="history"
                                            Background="{x:Null}"
                                            BorderBrush="{x:Null}"
                                            BorderThickness="0"
                                            helpers:ThemeHelper.MouseOverBackgroundBrush="{x:Null}"
                                            helpers:ThemeHelper.FocusBackgroundBrush="{x:Null}"
                                            helpers:ThemeHelper.FocusBrush="{x:Null}"
                                            helpers:ThemeHelper.FocusVisualMargin="0"
                                            IsTabStop="False"
                                            Opacity="0"
                                            VerticalAlignment="Stretch"
                                            FontSize="{TemplateBinding FontSize}"/>
                                </telerik:RadSplitButton.Content>
                                <telerik:RadSplitButton.DropDownContent>
                                    <ListBox x:Name="HistoryListBox"
                                            helpers:ThemeHelper.CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"
                                            BorderThickness="0"
                                            Foreground="{TemplateBinding Foreground}"
                                            ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                            ItemsSource="{TemplateBinding HistoryItems}"
                                            FontSize="{TemplateBinding FontSize}"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}"/>
                                </telerik:RadSplitButton.DropDownContent>
                            </telerik:RadSplitButton>
                            <telerikBreadcrumb:RadBreadcrumbBar x:Name="BreadcrumbBar"
                                    AutomationProperties.Name="Address bar"
                                    Grid.Column="1"
                                    Background="Transparent"
                                    Foreground="{TemplateBinding Foreground}"
                                    FontSize="{TemplateBinding FontSize}"
                                    ItemTemplate="{TemplateBinding ItemTemplate}"
                                    ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>
                            <Border x:Name="FocusVisual" Grid.ColumnSpan="3" Style="{StaticResource InputFocusVisualBorderStyle}" CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                            <Popup x:Name="AutocompletePopup"
                                    AllowsTransparency="True"
                                    StaysOpen="False"
                                    Focusable="False"
                                    Placement="Bottom"
                                    VerticalOffset="{telerik1:Windows11Resource ResourceKey=PopupOffset}">
                                <Grid Margin="4">
                                    <mat:Shadow Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}" ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}" CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
                                    <ListBox x:Name="AutoCompleteListBox"
                                            ItemsSource="{TemplateBinding AutocompleteItems}"
                                            FontSize="{TemplateBinding FontSize}"
                                            Foreground="{TemplateBinding Foreground}"
                                            MaxWidth="650"
                                            MaxHeight="500"
                                            MinWidth="150"
                                            ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                            SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}"/>
                                </Grid>
                            </Popup>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="NormalVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBackgroundBrush)}"/>
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible"/>
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBar">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikBreadcrumb:RadBreadcrumbBar">
                    <Grid>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ItemsPresenter x:Name="ItemsPresented" ClipToBounds="True"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarItemStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Windows11Resource ResourceKey=FocusBrush}"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="5 0"/>

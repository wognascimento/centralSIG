                            <Border
                                    BorderThickness="1"
                                    BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                    Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                            <StackPanel Orientation="Horizontal" Margin="{TemplateBinding Padding}">
                                <telerik1:RadGlyph
                                        Glyph="{StaticResource GlyphInsertUp}"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Margin="0 0 8 0"
                                        Foreground="{telerik1:Windows11Resource ResourceKey=IconBrush}"
                                        Visibility="{Binding DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Before}"/>
                                <telerik1:RadGlyph
                                        Glyph="{StaticResource GlyphInsertDown}"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Foreground="{telerik1:Windows11Resource ResourceKey=IconBrush}"
                                        Margin="0 0 8 0"
                                        Visibility="{Binding DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=After}"/>
                                <TextBlock Text="{Binding DropPosition}" Foreground="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                                <TextBlock Text=":" Margin="1 0 3 0" Foreground="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                                <TextBlock Text="{Binding DropItemString}" Foreground="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                            </StackPanel>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Effects" Value="None">
                            <Setter TargetName="NoneEffect" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="lowerGrid" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Move">
                            <Setter TargetName="NoneEffect" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadListBoxStyle" TargetType="telerik1:RadListBox">
        <Setter Property="telerik1:ScrollingSettingsBehavior.IsEnabled" Value="True"/>
        <Setter Property="telerik1:ScrollingSettingsBehavior.ScrollAreaPadding" Value="30"/>

        <!-- ScrollStep should be at least the ActualHeight of the ListBoxItem when scrolling is item based. -->
        <Setter Property="telerik1:ScrollingSettingsBehavior.ScrollStep" Value="24"/>
        <Setter Property="telerik1:ScrollingSettingsBehavior.ScrollStepTime" Value="00:00:00.05"/>
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBackgroundBrush}"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.CanContentScroll" Value="True"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Once"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <VirtualizingStackPanel/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadListBox">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    IsTabStop="False"
                                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}"
                                    helpers:ThemeHelper.CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                                    Padding="{TemplateBinding Padding}"
                                    Background="Transparent"
                                    BorderBrush="{x:Null}"
                                    Margin="0"
                                    BorderThickness="0">
                                <ScrollViewer.InputBindings>
                                    <KeyBinding Command="telerikPrimitives:ListControl.SelectAllCommand" Key="A" Modifiers="Control"/>
                                </ScrollViewer.InputBindings>
                                <ItemsPresenter/>
                            </ScrollViewer>
                        </Border>
                        <ContentPresenter x:Name="PART_DropVisualPlaceholder" Visibility="Collapsed" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBackgroundBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerik1:RadListBox" BasedOn="{StaticResource RadListBoxStyle}"/>
    <Style TargetType="telerik1:RadListBoxItem" BasedOn="{StaticResource RadListBoxItemStyle}"/>
    <Style TargetType="telerikDragDrop:ListBoxDragVisual" BasedOn="{StaticResource ListBoxDragVisualStyle}"/>
    <Style x:Key="ActiveItemsBaseRepeatButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource RepeatButtonStyle}">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="helpers:ThemeHelper.FocusVisualMargin" Value="0"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="Transparent"/>
        <Setter Property="Padding" Value="0"/>
    </Style>
    <Style x:Key="TopPlacementPeekPopupCalloutStyle" TargetType="telerik:RadCallout" BasedOn="{StaticResource RadCalloutStyle}">
        <Setter Property="MinHeight" Value="60"/>
        <Setter Property="MinWidth" Value="100"/>
        <Setter Property="ArrowAnchorPoint" Value="0.5, 1.02"/>
        <Setter Property="ArrowBasePoint1" Value="0, 0.7"/>
        <Setter Property="ArrowBasePoint2" Value="1, 0.7"/>
    </Style>
    <Style x:Key="RightPlacementPeekPopupCalloutStyle" TargetType="telerik:RadCallout" BasedOn="{StaticResource RadCalloutStyle}">
        <Setter Property="ArrowAnchorPoint" Value="-0.02, 0.5"/>
        <Setter Property="ArrowBasePoint1" Value="0.3,0"/>
        <Setter Property="ArrowBasePoint2" Value="0.3,1"/>
    </Style>
    <ControlTemplate x:Key="RadOfficeNavigationBarItemTemplate" TargetType="telerikNavigation:RadOfficeNavigationBarItem">
        <Border x:Name="BorderVisual"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                <ContentPresenter x:Name="PART_CompactContent"
                        ContentSource="CompactContent"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        Visibility="{Binding IsInCompactMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadOfficeNavigationBar}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                <ContentPresenter x:Name="Content"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        Visibility="{Binding IsInCompactMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadOfficeNavigationBar}, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
                        Margin="{TemplateBinding Padding}"/>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=AccentSelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush)}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="PART_CompactContent" Property="Opacity" Value="{telerik:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="Content" Property="Opacity" Value="{telerik:Windows11Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadOfficeNavigationBarItemStyle" TargetType="telerikNavigation:RadOfficeNavigationBarItem">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik:Windows11Resource ResourceKey=ButtonBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik:Windows11Resource ResourceKey=MouseOverBackgroundBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="14 0"/>
        <Setter Property="Foreground" Value="{telerik:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinHeight" Value="42"/>
        <Setter Property="MinWidth" Value="42"/>
        <Setter Property="PeekPopupStyle" Value="{StaticResource TopPlacementPeekPopupCalloutStyle}"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Template" Value="{StaticResource RadOfficeNavigationBarItemTemplate}"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding Orientation, RelativeSource={RelativeSource AncestorType=telerikNavigation:RadOfficeNavigationBar}}" Value="Vertical">
                <Setter Property="PeekPopupStyle" Value="{StaticResource RightPlacementPeekPopupCalloutStyle}"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <ControlTemplate x:Key="RadOfficeNavigationBarTemplate" TargetType="telerikNavigation:RadOfficeNavigationBar">
        <Border
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <ItemsPresenter x:Name="ItemsPresenter"/>
                <telerik:RadButton x:Name="OptionsButton"
                        Grid.Column="1"
                        IsBackgroundVisible="False"
                        Padding="0"
                        FocusVisualStyle="{x:Null}"
                        mat:MaterialAssist.IsShadowDisabled="True"
                        helpers:ThemeHelper.FocusBrush="Transparent"
                        Width="{Binding ActualHeight, ElementName=ItemsPresenter}"
                        helpers:ThemeHelper.FocusVisualMargin="0"
                        helpers:ThemeHelper.FocusBackgroundBrush="Transparent">
                    <telerik:RadGlyph Glyph="{StaticResource GlyphMoreHorizontal}"/>
                    <telerikNavigation:RadContextMenu.ContextMenu>
                        <officeNavigationBarPrimitives:OverflowMenu x:Name="PART_OptionsContextMenu" EventName="Click"/>
                    </telerikNavigation:RadContextMenu.ContextMenu>
                </telerik:RadButton>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="Orientation" Value="Vertical">
                <Setter TargetName="OptionsButton" Property="Grid.Row" Value="1"/>
                <Setter TargetName="OptionsButton" Property="Grid.Column" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style TargetType="telerikNavigation:RadOfficeNavigationBar" x:Key="RadOfficeNavigationBarStyle">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Background" Value="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <officeNavigationBarPrimitives:OfficeNavigationBarPanel Orientation="{Binding Orientation, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadOfficeNavigationBar}, Mode=TwoWay}" ItemsMaxCount="{Binding ItemsMaxCount, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadOfficeNavigationBar}, Mode=TwoWay}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource RadOfficeNavigationBarTemplate}"/>
    </Style>
    <ControlTemplate x:Key="OfficeNavigationBarDialogTemplate" TargetType="officeNavigationBarPrimitives:OfficeNavigationBarDialog">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal">
                    <TextBlock VerticalAlignment="Center" Text="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_MaximumVisibleItems}" Margin="0 0 8 0"/>
                    <telerik:RadWatermarkTextBox
                            AutomationProperties.Name="Active items count"
                            SelectionOnFocus="SelectAll"
                            IsReadOnly="True"
                            Text="{Binding ItemsMaxCount}"
                            VerticalAlignment="Top"
                            MinWidth="70"
                            BorderThickness="1"
                            helpers:ThemeHelper.ReadOnlyBrush="{telerik:Windows11Resource ResourceKey=InputBorderBrush}"
                            ReadOnlyBackground="Transparent">
                        <telerik:RadWatermarkTextBox.AdditionalContent>
                            <Grid Margin="5 0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <RepeatButton
                                        AutomationProperties.Name="Decrease active items count"
                                        Command="{Binding DecreaseItemsMaxCountCommand}"
                                        ContentTemplate="{StaticResource ActiveItemsDownButton}"
                                        Style="{StaticResource ActiveItemsBaseRepeatButtonStyle}"/>
                                <RepeatButton
                                        AutomationProperties.Name="Increase active items count"
                                        Command="{Binding IncreaseItemsMaxCountCommand}"
                                        ContentTemplate="{StaticResource ActiveItemsUpButton}"
                                        Style="{StaticResource ActiveItemsBaseRepeatButtonStyle}"
                                        Grid.Column="1"/>
                            </Grid>
                        </telerik:RadWatermarkTextBox.AdditionalContent>
                    </telerik:RadWatermarkTextBox>
                </StackPanel>
                <CheckBox
                        Grid.Row="1"
                        IsChecked="{Binding IsInCompactMode}"
                        Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_CompactNavigation}"
                        Margin="0 5"
                        Visibility="{Binding IsCompactModeCheckBoxVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                <Rectangle Grid.Row="2" Fill="{TemplateBinding BorderBrush}" Height="1" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Margin="0 5"/>
                <Grid Grid.Row="3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.ColumnSpan="2" Text="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_ChangeItemsOrder}" Margin="0 5"/>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <officeNavigationBarPrimitives:OfficeNavigationBarDialogListBox x:Name="ReorderItemsListbox"
                                AutomationProperties.Name="Reorder items"
                                SelectedItem="{Binding SelectedItem, Mode=TwoWay}"
                                ItemsSource="{Binding Items}"
                                MaxHeight="250"
                                BorderThickness="1"/>
                        <StackPanel Grid.Column="1" Margin="8 0 0 0">
                            <telerik:RadButton Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_MoveItemUp}" Command="{Binding MoveItemUpCommand}" MinWidth="112"/>
                            <telerik:RadButton Margin="0 8 0 0" Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_MoveItemDown}" Command="{Binding MoveItemDownCommand}" MinWidth="112"/>
                        </StackPanel>
                    </Grid>
                </Grid>
            </Grid>
            <Border
                    Grid.Row="1"
                    BorderThickness="0 1 0 0"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{telerik:Windows11Resource ResourceKey=AlternativeBrush}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <Grid Margin="13 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_Reset}" Command="{Binding ResetCommand}" Margin="2 0" MinWidth="112" HorizontalAlignment="Left"/>
                    <telerik:RadButton
                            Grid.Column="1"
                            Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_Ok}"
                            Command="{x:Static telerik:WindowCommands.Confirm}"
                            telerikNavigation:RadWindow.ResponseButton="Accept"
                            Margin="2 0"
                            MinWidth="112"/>
                    <telerik:RadButton
                            Grid.Column="2"
                            Content="{telerik:LocalizableResource Key=OfficeNavigationBarDialog_Cancel}"
                            Command="{x:Static telerik:WindowCommands.Cancel}"
                            telerikNavigation:RadWindow.ResponseButton="Cancel"
                            Margin="2 0"
                            MinWidth="112"/>
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="OfficeNavigationBarDialogStyle" TargetType="officeNavigationBarPrimitives:OfficeNavigationBarDialog">
        <Setter Property="Template" Value="{StaticResource OfficeNavigationBarDialogTemplate}"/>
        <Setter Property="Padding" Value="16"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="MinWidth" Value="355"/>
    </Style>
    <Style TargetType="telerikNavigation:RadOfficeNavigationBar" BasedOn="{StaticResource RadOfficeNavigationBarStyle}"/>
    <Style TargetType="telerikNavigation:RadOfficeNavigationBarItem" BasedOn="{StaticResource RadOfficeNavigationBarItemStyle}"/>
    <Style TargetType="officeNavigationBarPrimitives:OfficeNavigationBarDialog" BasedOn="{StaticResource OfficeNavigationBarDialogStyle}"/>
    <Style TargetType="officeNavigationBarPrimitives:OverflowMenu" BasedOn="{StaticResource RadContextMenuStyle}"/>
    <Style TargetType="officeNavigationBarPrimitives:OfficeNavigationBarDialogListBox" BasedOn="{StaticResource RadListBoxStyle}"/>
</ResourceDictionary>

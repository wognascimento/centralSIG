                                helpers:ThemeHelper.CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=BottomLeftRight}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                            <RepeatButton.Content>
                                <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronDown}" Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}" FontSize="12"/>

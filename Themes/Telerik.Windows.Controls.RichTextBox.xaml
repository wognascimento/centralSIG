    <Style x:Key="PasteOptionsPickerStyle" TargetType="telerikControlsRTBUI:PasteOptionsPicker">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:PasteOptionsPicker">
                    <Grid Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.RowSpan="2"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                        <Border x:Name="HeaderBorder"
                                Background="Transparent"
                                BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}"
                                BorderBrush="Transparent">
                            <telerik:Label Content="Paste Options:" FontWeight="SemiBold"/>
                        </Border>
                        <ItemsControl x:Name="PART_CommandsItemsControl"
                                DataContext="{x:Null}"
                                Grid.Row="1"
                                BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                BorderBrush="Transparent"
                                ItemsSource="{Binding AvailablePasteOptions}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Grid IsHitTestVisible="True">
                                        <ContentControl ToolTip="{Binding Tooltip}" Content="{Binding PasteOption}">
                                            <ContentControl.ContentTemplate>
                                                <DataTemplate>
                                                    <telerik:RadButton IsBackgroundVisible="False" Command="{Binding RelativeSource={RelativeSource AncestorType=telerikControlsRTBUI:PasteOptionsPicker}, Path=PasteCommand}" CommandParameter="{Binding}">
                                                        <Image
                                                                Width="24"
                                                                Height="24"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Source="{telerik:IconResource IconRelativePathStringFormat='{}{0}.png', IconSources={StaticResource IconSourcesPaste}}"/>
                                                    </telerik:RadButton>
                                                </DataTemplate>
                                            </ContentControl.ContentTemplate>
                                        </ContentControl>
                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PasteOptionsDropDownPickerStyle" TargetType="telerikControlsRTBUI:PasteOptionsDropDownPicker">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:PasteOptionsDropDownPicker">
                    <Border Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                        <telerik:RadDropDownButton x:Name="PART_DropDownButton" helpers:ThemeHelper.FocusVisualMargin="0">
                            <telerik:RadDropDownButton.Content>
                                <StackPanel Orientation="Horizontal">
                                    <Image
                                            Width="16"
                                            Height="16"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Source="{telerik:IconResource IconRelativePath='16/paste.png', IconSources={StaticResource IconSources}}"/>
                                    <Label Content="(Ctrl)" Padding="0" Margin="5 0 0 0" VerticalAlignment="Center" Foreground="{Binding ElementName=PART_DropDownButton, Path=Foreground}"/>
                                </StackPanel>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <Grid Margin="{TemplateBinding Padding}" MinWidth="150">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Border x:Name="BorderVisual"
                                            Grid.RowSpan="2"
                                            Background="{TemplateBinding Background}"
                                            BorderBrush="{TemplateBinding BorderBrush}"
                                            BorderThickness="{TemplateBinding BorderThickness}"
                                            CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                                    <Border x:Name="HeaderBorder"
                                            Background="Transparent"
                                            BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}"
                                            BorderBrush="Transparent">
                                        <telerik:Label telerik:LocalizationManager.ResourceKey="Documents_PasteOptions_Name" FontWeight="Bold"/>
                                    </Border>
                                    <telerik:RadListBox x:Name="PART_CommandsListBox"
                                            Grid.Row="1"
                                            BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                            BorderBrush="Transparent"
                                            SelectedValuePath="PasteOption"
                                            SelectedValue="{Binding SelectedPasteOption, Mode=OneWay}"
                                            ItemsSource="{Binding AvailablePasteOptions}"
                                            SelectionMode="Single"
                                            CanKeyboardNavigationSelectItems="False"
                                            KeyboardNavigation.TabNavigation="Cycle">
                                        <telerik:RadListBox.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <StackPanel Orientation="Horizontal"/>
                                            </ItemsPanelTemplate>
                                        </telerik:RadListBox.ItemsPanel>
                                        <telerik:RadListBox.ItemTemplate>
                                            <DataTemplate>
                                                <Grid IsHitTestVisible="True">
                                                    <ContentControl ToolTip="{Binding Tooltip}" Content="{Binding PasteOption}" IsTabStop="False">
                                                        <ContentControl.ContentTemplate>
                                                            <DataTemplate>
                                                                <Image
                                                                        Width="24"
                                                                        Height="24"
                                                                        HorizontalAlignment="Center"
                                                                        VerticalAlignment="Center"
                                                                        Source="{telerik:IconResource IconRelativePathStringFormat='{}{0}.png', IconSources={StaticResource IconSourcesPaste}}"/>
                                                            </DataTemplate>
                                                        </ContentControl.ContentTemplate>
                                                    </ContentControl>
                                                </Grid>
                                            </DataTemplate>
                                        </telerik:RadListBox.ItemTemplate>
                                    </telerik:RadListBox>
                                </Grid>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ShapesGallerySeparatorStyle" TargetType="telerik:RadListBoxItem">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Width" Value="{Binding PopupContentWidth}"/>
        <Setter Property="BorderThickness" Value="0 1 0 0"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>

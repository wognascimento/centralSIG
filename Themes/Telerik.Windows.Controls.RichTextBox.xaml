            <Setter.Value>
                <ItemsPanelTemplate>
                    <slider:ThumbsPanel x:Name="PART_ThumbPanel" IsItemsHost="True"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">

        <Setter Property="GalleryContentItemStyle" Value="{StaticResource ShapesGalleryListBoxItemStyle}"/>
        <Setter Property="GallerySeparatorStyle" Value="{StaticResource ShapesGallerySeparatorStyle}"/>

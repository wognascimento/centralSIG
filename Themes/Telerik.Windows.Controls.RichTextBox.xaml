                                                    <telerik:RadButton.Content>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_MoreLines}" VerticalAlignment="Center" Margin="8 0 0 0"/>
                                                        </StackPanel>
                                                    </telerik:RadButton.Content>
                                                    <telerik:EventToCommandBehavior.EventBindings>
                                                        <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                                                    </telerik:EventToCommandBehavior.EventBindings>
                                                </telerik:RadButton>
                                            </telerik:RadMenuGroupItem>
                                        </telerik:RadMenuItem>
                                        <telerik:RadMenuItem
                                                SubmenuHeaderTemplateKey="{StaticResource ShapesPickerMenuItemTemplate}"
                                                SubmenuItemTemplateKey="{StaticResource ShapesPickerMenuItemTemplate}"
                                                Margin="0 0 0 2"
                                                Header="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_Dashes}"
                                                DropDownPlacement="Right">
                                            <telerik:RadMenuItem.Icon>
                                                <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                                            </telerik:RadMenuItem.Icon>
                                            <telerik:RadMenuGroupItem BorderThickness="0" Padding="0">
                                                <telerik:RadListBox x:Name="PART_OutlineDashTypeListBox"
                                                        ItemContainerStyle="{StaticResource ShapesPopupGalleryListBoxItemStyle}"
                                                        BorderThickness="0"
                                                        ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                        ItemsSource="{Binding OutlineDashTypeItems, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type telerikControlsRTBUI:ShapesOutlinePicker}}}"
                                                        SelectedValuePath="LineDashType"
                                                        SelectedValue="{Binding SelectedOutlineDashType, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type telerikControlsRTBUI:ShapesOutlinePicker}}, Mode=TwoWay}">
                                                    <telerik:EventToCommandBehavior.EventBindings>
                                                        <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                                                    </telerik:EventToCommandBehavior.EventBindings>
                                                    <telerik:RadListBox.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <VirtualizingStackPanel/>
                                                        </ItemsPanelTemplate>
                                                    </telerik:RadListBox.ItemsPanel>
                                                    <telerik:RadListBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <Line
                                                                    Style="{StaticResource ShapesOutlinePickerLineStyle}"
                                                                    Stroke="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadListBoxItem}}}"
                                                                    StrokeDashArray="{Binding LineDashType, Converter={StaticResource LineDashTypeToDoubleCollectionConverter}}"
                                                                    StrokeThickness="2"
                                                                    ToolTipService.ToolTip="{Binding Path=TooltipText, Mode=OneTime}"/>
                                                        </DataTemplate>
                                                    </telerik:RadListBox.ItemTemplate>
                                                </telerik:RadListBox>
                                                <telerik:RadMenuSeparatorItem IconColumnWidth="0"/>
                                                <telerik:RadButton Style="{StaticResource ShapesPickerMoreLinesButtonStyle}" Command="{Binding AssociatedRichTextBox.Commands.ShowTaskPaneCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type telerikControlsRTBUI:ShapesOutlinePicker}}}" CommandParameter="MoreLines">
                                                    <telerik:RadButton.Content>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_MoreLines}" VerticalAlignment="Center" Margin="8 0 0 0"/>
                                                        </StackPanel>
                                                    </telerik:RadButton.Content>
                                                    <telerik:EventToCommandBehavior.EventBindings>
                                                        <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                                                    </telerik:EventToCommandBehavior.EventBindings>
                                                </telerik:RadButton>
                                            </telerik:RadMenuGroupItem>
                                        </telerik:RadMenuItem>
                                    </telerik:RadMenuGroupItem>
                                </telerik:RadMenu>
                            </DataTemplate>
                        </telerik:RadColorPicker.AdditionalContentTemplate>
                    </telerik:RadColorPicker>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TaskPaneTextBlockStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSizeM}"/>
        <Setter Property="Margin" Value="10"/>
    </Style>
    <Style x:Key="TaskPaneHeaderTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Margin" Value="0 8"/>
    </Style>
    <Style x:Key="FormatTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Margin" Value="0 4 0 12"/>
    </Style>
    <Style x:Key="TaskPaneStyle" TargetType="telerikControlsRTBUI:TaskPane">
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBorderBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Padding" Value="10 8"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinWidth" Value="310"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:TaskPane">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FormatShape}" Style="{StaticResource TaskPaneTextBlockStyle}"/>
                            <telerik:RadButton
                                    Grid.Column="1"
                                    x:Name="closeButton"
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Padding="4"
                                    Margin="0 0 5 0"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center">
                                <telerik:RadGlyph Foreground="{TemplateBinding Foreground}" HorizontalAlignment="Center" VerticalAlignment="Center" Glyph="{StaticResource GlyphClose}"/>
                            </telerik:RadButton>
                            <telerik:RadTabControl x:Name="taskPane" Grid.ColumnSpan="2" Grid.Row="1" BorderThickness="0" Padding="{TemplateBinding Padding}">
                                <telerik:RadTabControl.Items>
                                    <telerik:RadTabItem Margin="10 0 0 0">
                                        <telerik:RadTabItem.Header>
                                            <Image Source="{telerik:IconResource IconRelativePath='FormatShapesBucket.png', IconSources={StaticResource IconSourcesShapes}}" ToolTipService.ToolTip="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FillAndLine}" Stretch="None"/>
                                        </telerik:RadTabItem.Header>
                                        <telerik:RadTabItem.Content>
                                            <ScrollViewer BorderThickness="0">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <telerik:RadExpander x:Name="fillExpander">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Fill}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:FillSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                    <telerik:RadExpander x:Name="lineExpander" Grid.Row="1">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Line}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:LineSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                </Grid>
                                            </ScrollViewer>
                                        </telerik:RadTabItem.Content>
                                    </telerik:RadTabItem>
                                    <telerik:RadTabItem Margin="3 0">
                                        <telerik:RadTabItem.Header>
                                            <Image
                                                    Source="{telerik:IconResource IconRelativePath='LayoutProperties.png', IconSources={StaticResource IconSourcesShapes}}"
                                                    ToolTipService.ToolTip="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FillAndLine}"
                                                    Stretch="None"
                                                    Margin="0 2 0 0"/>
                                        </telerik:RadTabItem.Header>
                                        <telerik:RadTabItem.Content>
                                            <ScrollViewer BorderThickness="0">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <telerik:RadExpander x:Name="textFillExpander" IsExpanded="True">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Fill}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:TextFillSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                    <telerik:RadExpander x:Name="textLineExpander" Grid.Row="1">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Line}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:TextLineSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                </Grid>
                                            </ScrollViewer>
                                        </telerik:RadTabItem.Content>
                                    </telerik:RadTabItem>
                                    <telerik:RadTabItem>
                                        <telerik:RadTabItem.Header>
                                            <Image
                                                    Source="{telerik:IconResource IconRelativePath='TextFillOutline.png', IconSources={StaticResource IconSourcesShapes}}"
                                                    ToolTipService.ToolTip="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FillAndLine}"
                                                    Stretch="None"
                                                    Margin="0 2 0 0"/>
                                        </telerik:RadTabItem.Header>
                                        <telerik:RadTabItem.Content>
                                            <telerik:RadExpander x:Name="textBoxExpander" VerticalAlignment="Top" IsExpanded="True">
                                                <telerik:RadExpander.Header>
                                                    <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                </telerik:RadExpander.Header>
                                                <telerik:RadExpander.Content>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition/>
                                                            <ColumnDefinition/>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock
                                                                Grid.Row="0"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_VerticalAlignment}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadComboBox
                                                                Grid.Row="0"
                                                                Grid.Column="1"
                                                                ItemsSource="{Binding VerticalAlignments}"
                                                                SelectedValue="{Binding CurrentVerticalAlignment}"
                                                                DisplayMemberPath="Name"
                                                                SelectedValuePath="Value"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <TextBlock
                                                                Grid.Row="1"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_TextDirection}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadComboBox
                                                                Grid.Row="1"
                                                                Grid.Column="1"
                                                                ItemsSource="{Binding Rotations}"
                                                                SelectedValue="{Binding CurrentRotation}"
                                                                DisplayMemberPath="Name"
                                                                SelectedValuePath="Value"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <CheckBox
                                                                Grid.Row="2"
                                                                Grid.ColumnSpan="2"
                                                                Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_DoNotRotateText}"
                                                                IsChecked="{Binding ShouldNotRotate, Mode=TwoWay}"
                                                                Margin="0 6"/>
                                                        <CheckBox
                                                                Grid.Row="3"
                                                                Grid.ColumnSpan="2"
                                                                Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_ResizeShapeToFitText}"
                                                                IsChecked="{Binding ShouldResize, Mode=TwoWay}"
                                                                Margin="0 0 0 15"/>
                                                        <TextBlock
                                                                Grid.Row="4"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_LeftMargin}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadNumericUpDown
                                                                Grid.Row="4"
                                                                Grid.Column="1"
                                                                Minimum="0.0"
                                                                Value="{Binding LeftMargin, Mode=TwoWay}"
                                                                SmallChange="0.1"
                                                                LargeChange="0.1"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <TextBlock
                                                                Grid.Row="5"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_RightMargin}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadNumericUpDown
                                                                Grid.Row="5"
                                                                Grid.Column="1"
                                                                Minimum="0.0"
                                                                Value="{Binding RightMargin, Mode=TwoWay}"
                                                                SmallChange="0.1"
                                                                LargeChange="0.1"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <TextBlock
                                                                Grid.Row="6"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_TopMargin}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadNumericUpDown
                                                                Grid.Row="6"
                                                                Grid.Column="1"
                                                                Minimum="0.0"
                                                                Value="{Binding TopMargin, Mode=TwoWay}"
                                                                SmallChange="0.05"
                                                                LargeChange="0.05"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <TextBlock
                                                                Grid.Row="7"
                                                                Grid.Column="0"
                                                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_BottomMargin}"
                                                                Style="{StaticResource FormatTextBlockStyle}"/>
                                                        <telerik:RadNumericUpDown
                                                                Grid.Row="7"
                                                                Grid.Column="1"
                                                                Minimum="0.0"
                                                                Value="{Binding BottomMargin, Mode=TwoWay}"
                                                                SmallChange="0.05"
                                                                LargeChange="0.05"
                                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                                        <CheckBox
                                                                Grid.Row="8"
                                                                Grid.ColumnSpan="2"
                                                                Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_TextBox_WrapTextInShape}"
                                                                IsChecked="{Binding ShouldWrap, Mode=TwoWay}"
                                                                Margin="0 6 0 0"/>
                                                    </Grid>
                                                </telerik:RadExpander.Content>
                                            </telerik:RadExpander>
                                        </telerik:RadTabItem.Content>
                                    </telerik:RadTabItem>
                                </telerik:RadTabControl.Items>
                            </telerik:RadTabControl>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FillSettingsControlBaseStyle" TargetType="taskPaneControls:FillSettingsControlBase">
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBorderBrush}"/>
        <Setter Property="Padding" Value="8"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:FillSettingsControlBase">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Vertical" Margin="29 0 0 0">
                            <RadioButton x:Name="noFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_NoFill}" Margin="0 0 0 6"/>
                            <RadioButton x:Name="solidFillButton"
                                    Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_SolidFill}"
                                    Margin="0 0 0 6"
                                    IsChecked="True"/>
                            <RadioButton x:Name="gradientFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_GradientFill}" Margin="0 0 0 6"/>
                            <RadioButton x:Name="patternFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_PatternFill}" Margin="0 0 0 6"/>
                        </StackPanel>
                        <Separator HorizontalAlignment="Stretch" Margin="0 15 0 8" Height="1" Background="{TemplateBinding BorderBrush}" Grid.Row="1"/>
                        <taskPaneControls:SolidFillSettingsControl x:Name="solidFillSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2"/>
                        <taskPaneControls:GradientSettingsControl x:Name="gradientFillSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2" Visibility="Collapsed"/>
                        <taskPaneControls:PatternFillSettingsControl x:Name="patternFillSettingsControl"
                                Margin="{Binding Padding, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                Grid.Row="2"
                                Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SolidFillSettingsControlStyle" TargetType="taskPaneControls:SolidFillSettingsControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:SolidFillSettingsControl">
                    <Grid>
                        <taskPaneControls:ColorAndTransparencyPicker SelectedColor="{Binding SelectedColor, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ColorAndTransparencyPickerStyle" TargetType="taskPaneControls:ColorAndTransparencyPicker">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:ColorAndTransparencyPicker">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Color}" HorizontalAlignment="Left" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadColorPicker x:Name="colorPicker"
                                SelectedColor="{Binding SelectedColor, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=taskPaneControls:ColorAndTransparencyPicker}, Mode=TwoWay}"
                                Grid.Column="2"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadColorPicker.ContentTemplate>

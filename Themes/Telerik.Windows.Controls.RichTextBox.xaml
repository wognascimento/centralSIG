    <ControlTemplate x:Key="ThumbControlTemplate" TargetType="Thumb">
        <Grid>
            <Ellipse x:Name="OuterVisual" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="1"/>
            <Ellipse x:Name="InnerVisual" Width="12" Height="12" RenderTransformOrigin="0.5 0.5" Fill="{TemplateBinding helpers:ThemeHelper.CheckedBackgroundBrush}">
                <Ellipse.RenderTransform>
                    <ScaleTransform/>
                </Ellipse.RenderTransform>
            </Ellipse>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard Name="ExpandInnerThumb">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="InnerVisual" Storyboard.TargetProperty="RenderTransform.ScaleX">
                                <EasingDoubleKeyFrame KeyTime="0:0:0" Value="1.16"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="InnerVisual" Storyboard.TargetProperty="RenderTransform.ScaleY">
                                <EasingDoubleKeyFrame KeyTime="0:0:0" Value="1.16"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <RemoveStoryboard BeginStoryboardName="ExpandInnerThumb"/>
                </Trigger.ExitActions>
                <Setter TargetName="InnerVisual" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush)}"/>
            </Trigger>
            <Trigger Property="IsDragging" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard Name="ShrinkInnerThumb">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="InnerVisual" Storyboard.TargetProperty="RenderTransform.ScaleX">
                                <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0.83"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="InnerVisual" Storyboard.TargetProperty="RenderTransform.ScaleY">
                                <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0.83"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <RemoveStoryboard BeginStoryboardName="ShrinkInnerThumb"/>
                </Trigger.ExitActions>
                <Setter TargetName="InnerVisual" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBackgroundBrush)}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="InnerVisual" Property="Fill" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
                <Setter TargetName="InnerVisual" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="OuterVisual" Property="Stroke" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

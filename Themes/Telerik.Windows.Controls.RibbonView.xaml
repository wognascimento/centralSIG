                            <Border x:Name="RecentColorsPaletteBorder" Grid.Row="6" Visibility="Collapsed" Background="{telerik1:Windows11Resource ResourceKey=AlternativeBrush}" MinHeight="32">
                                <TextBlock x:Name="RecentColorsPaletteHeader"

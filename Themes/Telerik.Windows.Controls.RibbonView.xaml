                            Command="{TemplateBinding HelpCommand}"
                            CommandParameter="{TemplateBinding HelpCommandParameter}"
                            CommandTarget="{TemplateBinding HelpCommandTarget}"
                            x:Name="HelpButton"
                            Visibility="{TemplateBinding HelpButtonVisibility}"
                            SmallImage="{TemplateBinding HelpButtonImageSource}"
                            Style="{TemplateBinding HelpButtonStyle}"
                            telerikRibbonView:ScreenTip.Title="{telerik:LocalizableResource Key=RibbonViewHelp}"/>
                    <ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}"/>
                </StackPanel>
            </Grid>
            <ContentPresenter x:Name="BelowToolBarHost" Grid.Row="2"/>
            <Popup x:Name="TabContentPopup" PlacementTarget="{Binding ElementName=TopGrid}" AllowsTransparency="True">
                <Grid x:Name="PopupGrid" Background="{TemplateBinding Background}">
                    <Grid.Resources>
                        <Style x:Key="SelectedTabContentPopupStyle" TargetType="ContentPresenter">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Default">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Resources>
                    <Grid Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Padding, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}">
                        <mat:Shadow x:Name="PopupShadow"
                                SnapsToDevicePixels="True"
                                ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"
                                Background="{TemplateBinding BorderBrush}"
                                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
                        <ContentPresenter x:Name="SelectedTabContentPopup" Style="{StaticResource SelectedTabContentPopupStyle}" MaxWidth="{Binding ElementName=SelectedTabContent, Path=ActualWidth}"/>
                    </Grid>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsHostedInRibbonWindow" Value="false">
                <Setter TargetName="RootPanel" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"/>
            </Trigger>
            <Trigger Property="IsHostedInRibbonWindow" Value="true">
                <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="RibbonTitleBarPanel" Property="MinHeight" Value="32"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ContextualGroupsHeaders" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
            </Trigger>
            <Trigger Property="IsBackstageOpen" Value="True">
                <Setter TargetName="Shadow" Property="Background" Value="{x:Null}"/>
            </Trigger>
            <Trigger Property="IsMinimized" Value="True">
                <Setter TargetName="SelectedTabContentGrid" Property="Margin" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

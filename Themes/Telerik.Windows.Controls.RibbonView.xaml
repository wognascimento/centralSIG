                <Setter Property="Canvas.ZIndex" Value="1"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Canvas.ZIndex" Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=DisabledBackgroundBrush}"/>
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RibbonGalleryDropDownButtonStyle" TargetType="telerikRibbonView:RadRibbonDropDownButton" BasedOn="{StaticResource RadRibbonDropDownButtonStyle}">
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=SecondaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed"/>
        <Setter Property="MinWidth" Value="22"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="0 0 0 -1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Canvas.ZIndex" Value="1"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Canvas.ZIndex" Value="1"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter Property="Canvas.ZIndex" Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=DisabledBackgroundBrush}"/>
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <ControlTemplate x:Key="RadRibbonGalleryTemplate" TargetType="telerikRibbonView:RadRibbonGallery">
        <Grid>
            <Border x:Name="ExpandedBorder"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <ScrollViewer x:Name="ScrollViewer"
                            Grid.RowSpan="3"
                            VerticalScrollBarVisibility="Hidden"
                            IsTabStop="False"
                            Padding="{TemplateBinding Padding}"
                            BorderBrush="Transparent"
                            BorderThickness="0"
                            VerticalAlignment="Stretch">
                        <ContentPresenter x:Name="InRibbonHost" Width="{TemplateBinding ViewportWidth}" HorizontalAlignment="Left">
                            <ContentPresenter.Content>
                                <ItemsPresenter x:Name="ItemsPresenter"/>
                            </ContentPresenter.Content>
                        </ContentPresenter>
                    </ScrollViewer>
                    <Grid Grid.Column="1" Grid.RowSpan="3" Margin="0 -1 -1 0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <telerikRibbonView:RadRibbonButton x:Name="ScrollUpButton"
                                Style="{StaticResource RibbonGalleryButtonStyle}"
                                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=RightTop}"
                                telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}">
                            <telerik:RadGlyph x:Name="ArrowUp" FontSize="12" Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}" Glyph="{StaticResource GlyphArrowChevronUp}"/>
                        </telerikRibbonView:RadRibbonButton>
                        <telerikRibbonView:RadRibbonButton x:Name="ScrollDownButton"
                                Grid.Row="1"
                                Style="{StaticResource RibbonGalleryButtonStyle}"
                                CornerRadius="0"
                                Margin="0 -1"
                                telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}">
                            <telerik:RadGlyph x:Name="ArrowDown" FontSize="12" Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}" Glyph="{StaticResource GlyphArrowChevronDown}"/>
                        </telerikRibbonView:RadRibbonButton>
                        <telerikRibbonView:RadRibbonDropDownButton x:Name="PopupButton"
                                Grid.Row="2"
                                Style="{StaticResource RibbonGalleryDropDownButtonStyle}"
                                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=RightBottom}"
                                telerikRibbonView:ScreenTip.Title="{TemplateBinding MoreButtonTitle}"
                                Visibility="{TemplateBinding PopupLaunchButtonVisibility}">
                            <telerik:RadGlyph x:Name="ArrowOverflow" FontSize="12" Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}" Glyph="{StaticResource GlyphArrowOverflowDown}"/>
                        </telerikRibbonView:RadRibbonDropDownButton>
                    </Grid>
                </Grid>
            </Border>
            <telerikRibbonView:RadRibbonDropDownButton x:Name="CollapsedButton"
                    HorizontalAlignment="Right"
                    VerticalContentAlignment="Center"
                    Size="Large"
                    LargeImage="{TemplateBinding Icon}"
                    Text="{TemplateBinding Title}"
                    Visibility="Collapsed">
                <telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer Padding="0" HorizontalScrollBarVisibility="Hidden" IsTabStop="False" BorderBrush="Transparent" Height="{TemplateBinding PopupViewportHeight}">
                            <ContentPresenter x:Name="PopupHost" Width="{TemplateBinding PopupViewportWidth}"/>
                        </ScrollViewer>
                        <ItemsControl
                                HorizontalContentAlignment="Stretch"
                                BorderThickness="0"
                                Margin="0 0 0 5"
                                Grid.Row="1"
                                ItemsSource="{TemplateBinding PopupMenuItems}"
                                Foreground="{TemplateBinding Foreground}"/>
                    </Grid>
                </telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
            </telerikRibbonView:RadRibbonDropDownButton>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsPressed" SourceName="ScrollUpButton" Value="True">
                <Setter Property="Foreground" TargetName="ArrowUp" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" SourceName="ScrollDownButton" Value="True">
                <Setter Property="Foreground" TargetName="ArrowDown" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" SourceName="PopupButton" Value="True">
                <Setter Property="Foreground" TargetName="ArrowOverflow" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="LayoutMode" Value="Simplified">
                <Setter TargetName="CollapsedButton" Property="Size" Value="Medium"/>
                <Setter TargetName="ScrollUpButton" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="ScrollDownButton" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="PopupButton" Property="Grid.Row" Value="0"/>
                <Setter TargetName="PopupButton" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="PopupButton" Property="CornerRadius" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomRight}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
                <Setter TargetName="ArrowDown" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="ArrowUp" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="ArrowOverflow" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonGalleryStyle" TargetType="telerikRibbonView:RadRibbonGallery">
        <Setter Property="Template" Value="{StaticResource RadRibbonGalleryTemplate}"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=CornerRadius}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimarySolidBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Hidden"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Hidden"/>
        <Setter Property="ViewportWidth" Value="210"/>
        <Setter Property="PopupViewportHeight" Value="{x:Static sys:Double.NaN}"/>
        <Setter Property="PopupViewportWidth" Value="210"/>
        <Setter Property="ItemWidth" Value="70"/>
        <Setter Property="ItemHeight" Value="50"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="PopupLaunchButtonVisibility" Value="Visible"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="Icon">
            <Setter.Value>
                <BitmapImage UriSource="pack://application:,,,/Telerik.Windows.Controls.RibbonView;component/RibbonGalleryCollapsedIcon.png"/>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:RadWrapPanel ItemWidth="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikRibbonView:RadRibbonGallery}}, Path=ItemWidth}" ItemHeight="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikRibbonView:RadRibbonGallery}}, Path=ItemHeight}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikRibbonView:RadRibbonGallery" BasedOn="{StaticResource RadRibbonGalleryStyle}"/>
    <ItemsPanelTemplate x:Key="ButtonGroupPanel">
        <telerikRibbonViewPrimitives:ButtonGroupPanel/>
    </ItemsPanelTemplate>
    <Style x:Key="RadButtonGroupStyle" TargetType="telerikRibbonView:RadButtonGroup">
        <Style.Resources>
            <Style TargetType="telerikInput:RadColorPicker" BasedOn="{StaticResource RadRibbonColorPickerStyle}">
                <Setter Property="Background" Value="Transparent"/>
            </Style>
        </Style.Resources>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=SecondaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="Padding" Value="0 0 5 0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ItemsPanel" Value="{StaticResource ButtonGroupPanel}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonView:RadButtonGroup">
                    <Grid>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
                        <ItemsPresenter Margin="{TemplateBinding Padding}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadButtonGroupItemTemplate" TargetType="telerikRibbonView:RadButtonGroupItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CollectionOrderStates">
                    <VisualState x:Name="FirstContainer"/>
                    <VisualState x:Name="DefaultContainer">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonItemsSeparator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <StackPanel Orientation="Horizontal">
                <Border x:Name="ButtonItemsSeparator" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Visibility="Collapsed"/>
                <ContentControl
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                        Margin="{TemplateBinding Padding}"
                        IsTabStop="{TemplateBinding IsTabStop}"/>
            </StackPanel>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadButtonGroupItemStyle" TargetType="telerikRibbonView:RadButtonGroupItem">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=SecondaryBorderBrush}"/>
        <Setter Property="Template" Value="{StaticResource RadButtonGroupItemTemplate}"/>
    </Style>
    <Style TargetType="telerikRibbonView:RadButtonGroup" BasedOn="{StaticResource RadButtonGroupStyle}"/>
    <Style TargetType="telerikRibbonView:RadButtonGroupItem" BasedOn="{StaticResource RadButtonGroupItemStyle}"/>
    <ControlTemplate x:Key="GroupChromeTemplate" TargetType="telerikRibbonViewPrimitives:GroupChrome">
        <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
            <Grid Background="Transparent" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid x:Name="Tray" Grid.Row="1" Visibility="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode, Converter={StaticResource VisibilityToRibbonLayoutConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ContentControl x:Name="Content"
                            Content="{TemplateBinding Header}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            IsTabStop="False"
                            Margin="2 0"
                            Foreground="{TemplateBinding Foreground}"
                            ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                            Opacity="0.8"/>
                    <telerikRibbonView:RadRibbonButton x:Name="DialogLauncher"
                            MinHeight="16"
                            MinWidth="16"
                            Visibility="{TemplateBinding DialogLauncherVisibility}"
                            Grid.Column="1"
                            IsAutoSize="False"
                            Padding="0"
                            TabIndex="1"
                            IsTabStop="False"
                            BorderBrush="Transparent"
                            BorderThickness="0"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Center"
                            Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                            Command="{TemplateBinding DialogLauncherCommand}"
                            CommandParameter="{TemplateBinding DialogLauncherCommandParameter}"
                            CommandTarget="{TemplateBinding DialogLauncherCommandTarget}">
                        <telerikRibbonView:RadRibbonButton.Content>
                            <telerik:RadGlyph Glyph="{StaticResource GlyphLaunch}"/>
                        </telerikRibbonView:RadRibbonButton.Content>
                    </telerikRibbonView:RadRibbonButton>
                </Grid>
                <ContentPresenter Margin="{TemplateBinding Padding}"/>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsPressed" SourceName="DialogLauncher" Value="True">
                <Setter TargetName="DialogLauncher" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="DialogLauncher" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="GroupChromeStyle" TargetType="telerikRibbonViewPrimitives:GroupChrome">
        <Setter Property="Template" Value="{StaticResource GroupChromeTemplate}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSizeS}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="Margin" Value="0 2 -1 2"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Padding" Value="4 2"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
    <Style TargetType="telerikRibbonViewPrimitives:GroupChrome" BasedOn="{StaticResource GroupChromeStyle}"/>
    <ControlTemplate x:Key="RadGroupHeaderTemplate" TargetType="telerikRibbonView:RadGroupHeader">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <ContentControl
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Margin="{TemplateBinding Padding}"
                    Foreground="{TemplateBinding Foreground}"
                    IsTabStop="False"/>
        </Border>
    </ControlTemplate>
    <Style x:Key="RadGroupHeaderStyle" TargetType="telerikRibbonView:RadGroupHeader">
        <Setter Property="Template" Value="{StaticResource RadGroupHeaderTemplate}"/>
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=SecondaryForegroundBrush}"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
    <Style TargetType="telerikRibbonView:RadGroupHeader" BasedOn="{StaticResource RadGroupHeaderStyle}"/>
    <ControlTemplate x:Key="RadRibbonGroupDropDownButtonTemplate" TargetType="telerikRibbonView:RadRibbonDropDownButton">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="KeyboardNavigationVisual" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
            <Border x:Name="KeyboardNavigationVisual" Opacity="0" BorderBrush="{TemplateBinding helpers:ThemeHelper.CornerRadius}" BorderThickness="{TemplateBinding BorderThickness}"/>
            <Grid x:Name="ContentGrid" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter x:Name="ContentPresenter"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        RecognizesAccessKey="True"/>
                <Border x:Name="Image"
                        Margin="{TemplateBinding Padding}"
                        Width="20"
                        Height="20"
                        Background="{telerik1:Windows11Resource ResourceKey=PrimaryBackgroundBrush}"
                        BorderBrush="{telerik1:Windows11Resource ResourceKey=ButtonBorderBrush}"
                        BorderThickness="1">
                    <Image VerticalAlignment="Center" HorizontalAlignment="Center" Width="16" Height="16" Source="{TemplateBinding SmallImage}" Stretch="None"/>
                </Border>
                <TextBlock Name="textRow1" Grid.Row="1" Text="{TemplateBinding TextRow1}" TextAlignment="Center" Foreground="{TemplateBinding Foreground}"/>
                <StackPanel Name="DropDownPanel" Grid.Row="2" HorizontalAlignment="Center" Orientation="Horizontal">
                    <TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" Foreground="{TemplateBinding Foreground}"/>
                    <telerik:RadGlyph x:Name="DropDownIndicator"
                            Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                            Glyph="{StaticResource GlyphArrowChevronDown}"
                            FontSize="12"
                            Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                            Margin="{TemplateBinding Padding}"/>
                </StackPanel>
            </Grid>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}"
                    HorizontalOffset="0"
                    VerticalOffset="{telerik1:Windows11Resource ResourceKey=PopupOffset}">
                <Grid>
                    <Border x:Name="DropDownPopupBorder"
                            Width="{TemplateBinding DropDownWidth}"
                            Height="{TemplateBinding DropDownHeight}"
                            MaxWidth="{TemplateBinding DropDownMaxWidth}"
                            MaxHeight="{TemplateBinding DropDownMaxHeight}"
                            Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"
                            BorderBrush="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                            BorderThickness="1"
                            CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}">
                        <ContentControl x:Name="DropDownPopupContent"
                                Content="{TemplateBinding DropDownContent}"
                                ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"
                                FocusVisualStyle="{x:Null}"/>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush), Mode=OneWay}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBackgroundBrush), Mode=OneWay}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="ContentPresenter" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow2" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="Image" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledBorderBrush}"/>
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=TertiaryForegroundBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronUp}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBackgroundBrush), Mode=OneWay}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="DropDownButtonPosition" Value="Right">
                <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
                <Setter TargetName="DropDownPanel" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
                <Setter TargetName="textRow1" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="Image" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="ContentPresenter" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="DropDownPopup" Property="HorizontalOffset" Value="-4"/>
                <Setter TargetName="DropDownPopup" Property="VerticalOffset" Value="0"/>
            </Trigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                <Setter TargetName="DropDownIndicator" Property="VerticalAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter TargetName="ContentGrid" Property="Margin" Value="5 0"/>
            </DataTrigger>
        </ControlTemplate.Triggers>

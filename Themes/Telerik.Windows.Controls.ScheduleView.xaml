                                telerik:LocalizationManager.ResourceKey="EndColon"/>
                        <telerik:RadDateTimePicker x:Name="EndTimePicker"
                                Grid.Row="1"
                                Grid.Column="1"
                                AutomationProperties.Name="EndTimePicker"
                                Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                Width="120"
                                AutomationProperties.AutomationId="EndTimePicker"

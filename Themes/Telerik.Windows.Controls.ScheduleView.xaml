                                            Maximum="999"
                                            Value="{Binding Interval, Mode=TwoWay}"/>
                                    <telerik:Label
                                            Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"

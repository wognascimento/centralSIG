                                            IsInteger="True"
                                            Maximum="31"
                                            Value="{Binding DaysOfMonth, Mode=TwoWay, Converter={StaticResource MultipleToSingleConverter}, ValidatesOnDataErrors=True, NotifyOnValidationError=True}"/>
                                    <telerik:Label
                                            Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                            Target="{Binding ElementName=MonthlyDetailsUpDown1}"
                                            telerik:LocalizationManager.ResourceKey="OfEvery"/>
                                    <telerik:RadNumericUpDown x:Name="MonthlyDetailsUpDown2"
                                            Width="120"
                                            AutomationProperties.AutomationId="MonthlyDetailsUpDown2"
                                            Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                            IsInteger="True"
                                            Maximum="99"
                                            Value="{Binding Interval, Mode=TwoWay}"/>
                                    <telerik:Label
                                            Margin="{StaticResource ScheduleViewDialogElementOffset}"
                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                            Target="{Binding ElementName=MonthlyDetailsUpDown2}"
                                            telerik:LocalizationManager.ResourceKey="Months"/>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Orientation="Horizontal">
                                    <RadioButton x:Name="MonthlyNthRB"
                                            AutomationProperties.AutomationId="MonthlyNthRB"

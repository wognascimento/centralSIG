    <ext:DynamicResourceProxy x:Key="ValidationBrush230AlphaProxy" ProxyValue="{Binding Source={x:Static telerik1:Windows11Palette.Palette}, Path=ValidationColor, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=230}"/>
    <ControlTemplate x:Key="ErrorTooltipTemplate" TargetType="ToolTip">
        <Grid x:Name="RootVisual" HorizontalAlignment="Right" RenderTransformOrigin="0 0" Opacity="0" SnapsToDevicePixels="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="OpenStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0"/>
                        <VisualTransition To="Open" GeneratedDuration="0:0:0.3">
                            <Storyboard>
                                <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0:0:0.3">
                                    <DoubleAnimation.EasingFunction>
                                        <BackEase Amplitude=".4" EasingMode="EaseOut"/>
                                    </DoubleAnimation.EasingFunction>
                                </DoubleAnimation>
                                <DoubleAnimation Storyboard.TargetName="RootVisual" Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0.3"/>
                            </Storyboard>
                        </VisualTransition>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Closed">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="RootVisual" Storyboard.TargetProperty="Opacity" To="0" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Open">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0"/>
                            <DoubleAnimation Storyboard.TargetName="RootVisual" Storyboard.TargetProperty="Opacity" To="1" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25"/>
            </Grid.RenderTransform>
            <Border
                    Background="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                    Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"
                    CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}"
                    MinHeight="32">
                <ContentPresenter
                        Content="{Binding ErrorContent}"
                        Margin="12 6"
                        TextBlock.Foreground="{telerik1:Windows11Resource ResourceKey=AccentControlForegroundBrush}"
                        TextBlock.FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
            </Border>
            <ContentControl/>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="StaticValidationTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <Grid SnapsToDevicePixels="True" VerticalAlignment="Top">
                <Border Background="Transparent" HorizontalAlignment="Right" VerticalAlignment="Top" Width="3" Height="3"/>
                <AdornedElementPlaceholder x:Name="Holder"/>
                <Border
                        BorderBrush="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                        BorderThickness="0 0 0 2"
                        CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}"
                        Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"/>
            </Grid>
            <Grid Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}">
                <Border
                        Background="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                        Margin="1 0 0 0"
                        SnapsToDevicePixels="True"
                        CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}">
                    <TextBlock x:Name="PART_ToolTip"
                            FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"
                            FontFamily="{telerik1:Windows11Resource ResourceKey=FontFamily}"
                            Foreground="{telerik1:Windows11Resource ResourceKey=AccentControlForegroundBrush}"
                            VerticalAlignment="Center"
                            Padding="12 6"
                            Text="{Binding ElementName=Holder, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}"/>
                </Border>
            </Grid>
        </StackPanel>
    </ControlTemplate>
    <ControlTemplate x:Key="ValidationTooltipTemplate">
        <Grid SnapsToDevicePixels="True" VerticalAlignment="Top">
            <Border Background="Transparent" HorizontalAlignment="Right" VerticalAlignment="Top" Width="3" Height="3"/>
            <AdornedElementPlaceholder x:Name="Holder"/>
            <Border
                    BorderBrush="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                    BorderThickness="0 0 0 2"
                    CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}"
                    Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"/>
            <Path
                    Data="{StaticResource ArrowTopRight8x8}"
                    Fill="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                    Width="8"
                    Height="8"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"/>
            <ToolTipService.ToolTip>
                <ToolTip x:Name="PART_ToolTip"
                        DataContext="{Binding RelativeSource={RelativeSource Mode=Self}, Path=PlacementTarget.DataContext}"
                        Template="{StaticResource ErrorTooltipTemplate}"
                        Placement="Right"/>
            </ToolTipService.ToolTip>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="StaticValidationOnFocusTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <Grid SnapsToDevicePixels="True" VerticalAlignment="Top">
                <AdornedElementPlaceholder x:Name="Holder"/>
                <Border
                        BorderBrush="{telerik1:Windows11Resource ResourceKey=ValidationBrush}"
                        BorderThickness="0 0 0 2"
                        CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}"
                        Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}"/>
            </Grid>
            <Grid Margin="1 0 0 0" Visibility="{Binding ElementName=Holder, Path=AdornedElement.IsKeyboardFocusWithin, Converter={StaticResource BooleanToVisibilityConverter}}">
                <Grid Margin="{telerik1:Windows11Resource ResourceKey=FocusVisualMargin}">
                    <Border Background="{telerik1:Windows11Resource ResourceKey=ValidationBrush}" SnapsToDevicePixels="True" CornerRadius="{telerik1:Windows11Resource ResourceKey=CornerRadius}">
                        <TextBlock x:Name="PART_ToolTip"
                                FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"
                                FontFamily="{telerik1:Windows11Resource ResourceKey=FontFamily}"
                                Foreground="{telerik1:Windows11Resource ResourceKey=AccentControlForegroundBrush}"
                                Padding="12 6"
                                VerticalAlignment="Center"
                                Text="{Binding ElementName=Holder, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}"/>
                    </Border>
                </Grid>
            </Grid>
        </StackPanel>
    </ControlTemplate>
    <ControlTemplate x:Key="NonEditableComboToggleButtonControlTemplate" TargetType="telerik:RadToggleButton">
        <Grid Background="#01FFFFFF">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto" MinWidth="25"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="ToggleButtonBackground"
                    Grid.ColumnSpan="2"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <ContentControl x:Name="DropDownIcon"
                    Grid.Column="1"
                    Margin="0 0 5 0"
                    IsTabStop="False"
                    Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="{Binding RelativeSource={RelativeSource AncestorType=telerikInput:RadComboBox}, Path=DropDownButtonIconContent}"
                    ContentTemplate="{Binding RelativeSource={RelativeSource AncestorType=telerikInput:RadComboBox}, Path=DropDownButtonIconTemplate}"/>
            <Grid Grid.Column="0">
                <ContentPresenter x:Name="Content"
                        TextBlock.Foreground="{TemplateBinding Foreground}"
                        Margin="{TemplateBinding Padding}"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="DropDownIcon" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledForegroundBrush)}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="NonEditableComboToggleButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="helpers:ThemeHelper.DisabledForegroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ClickMode" Value="Press"/>
        <Setter Property="Template" Value="{StaticResource NonEditableComboToggleButtonControlTemplate}"/>
    </Style>
    <Style x:Key="EditableComboToggleButtonStyle" TargetType="telerik:RadToggleButton" BasedOn="{StaticResource RadToggleButtonStyle}">
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
        <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBackgroundBrush" Value="Transparent"/>
        <Setter Property="helpers:ThemeHelper.DisabledBackgroundBrush" Value="Transparent"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{x:Null}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="MinWidth" Value="22"/>
        <Setter Property="MinHeight" Value="22"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Style.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleBrush}"/>
                <Setter Property="helpers:ThemeHelper.PressedBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=SubtleSecondaryBrush}"/>
                <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{x:Null}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="ComboBoxTextBoxStyle" TargetType="TextBox">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerikInput:RadComboBox}}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomLeft}"/>
        <Setter Property="helpers:ThemeHelper.MouseOverBackgroundBrush" Value="Transparent"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik1:Windows11Resource ResourceKey=AccentBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledForegroundBrush" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
        <Setter Property="SelectionBrush" Value="{telerik1:Windows11Resource ResourceKey=AccentBrush}"/>
        <Setter Property="SelectionOpacity" Value="0.4"/>
        <Setter Property="Foreground" Value="{Binding Foreground,RelativeSource={RelativeSource AncestorType={x:Type telerikInput:RadComboBox}}}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="CaretBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopBottom}"/>
    </Style>
    <ControlTemplate x:Key="EditableComboBox" TargetType="telerikInput:RadComboBox">
        <Grid x:Name="VisualRoot" Margin="{TemplateBinding helpers:ThemeHelper.FocusVisualMargin}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid MinHeight="{TemplateBinding MinHeight}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Border x:Name="BackgroundVisual"
                        Grid.ColumnSpan="2"
                        IsHitTestVisible="False"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                <TextBox x:Name="PART_EditableTextBox"
                        AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                        AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                        AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                        TabIndex="{TemplateBinding TabIndex}"
                        Grid.Column="0"
                        Padding="{TemplateBinding Padding}"
                        HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                        IsReadOnly="{TemplateBinding IsReadOnly}"
                        Focusable="{TemplateBinding Focusable}"
                        Style="{StaticResource ComboBoxTextBoxStyle}">
                    <TextBox.Template>
                        <ControlTemplate TargetType="TextBox">
                            <Grid>
                                <Border x:Name="BorderVisual"
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                                <ScrollViewer x:Name="PART_ContentHost"
                                        Background="Transparent"
                                        BorderBrush="Transparent"
                                        BorderThickness="0"
                                        IsTabStop="False"
                                        VerticalScrollBarVisibility="Auto"
                                        VerticalContentAlignment="Stretch"
                                        Padding="{TemplateBinding Padding}"/>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsEnabled" Value="False">
                                    <Setter TargetName="PART_ContentHost" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBackgroundBrush), Mode=OneWay}"/>
                                </Trigger>
                                <Trigger Property="IsKeyboardFocused" Value="True">
                                    <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </TextBox.Template>
                </TextBox>
                <telerik:RadToggleButton x:Name="PART_DropDownButton"
                        Grid.Column="1"
                        IsTabStop="False"
                        helpers:ThemeHelper.FocusVisualMargin="0"
                        helpers:ThemeHelper.CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomRight}"
                        Margin="0 0 5 0"
                        VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        Style="{StaticResource EditableComboToggleButtonStyle}"
                        ClickMode="Press">
                    <ContentControl x:Name="DropDownIcon"
                            IsTabStop="False"
                            Foreground="{telerik1:Windows11Resource ResourceKey=IconSecondaryBrush}"
                            Margin="5 0"
                            Content="{TemplateBinding DropDownButtonIconContent}"
                            ContentTemplate="{TemplateBinding DropDownButtonIconTemplate}"/>
                </telerik:RadToggleButton>
                <Border x:Name="FocusVisual" Grid.ColumnSpan="2" Style="{StaticResource InputFocusVisualBorderStyle}" CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
                <TextBlock x:Name="Watermark"
                        Text="{TemplateBinding EmptyText}"
                        Margin="2 0 0 0"
                        IsHitTestVisible="False"
                        Padding="{TemplateBinding Padding}"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Visibility="Collapsed"
                        FontStyle="Italic"
                        Foreground="{Binding ElementName=PART_EditableTextBox, Path=Foreground, Mode=OneWay}"/>
                <Popup x:Name="PART_Popup" VerticalOffset="{telerik1:Windows11Resource ResourceKey=PopupOffset}">
                    <Grid x:Name="PopupRoot" Margin="4 0 4 4">
                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}" CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
                        <Border x:Name="PART_ResizeBorder"
                                CornerRadius="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"
                                Background="{telerik1:Windows11Resource ResourceKey=TertiaryBackgroundBrush}"

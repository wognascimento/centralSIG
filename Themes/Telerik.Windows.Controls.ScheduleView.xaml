                                Grid.Column="0"
                                MinWidth="50"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                IsEnabled="{Binding IsReadOnly, Converter={StaticResource InvertedBooleanConverter}}"

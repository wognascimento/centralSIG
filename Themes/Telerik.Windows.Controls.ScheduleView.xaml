                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="16 0 8 0">
                            <TextBlock x:Name="DateTextBlock" FontSize="30" Margin="0 0 7 0" Text="{Binding Name, StringFormat='{}{0:dd}'}"/>

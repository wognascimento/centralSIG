        <Setter Property="helpers:ThemeHelper.ReadOnlyBrush" Value="{telerik1:Windows11Resource ResourceKey=ReadOnlyBorderBrush}"/>
        <Setter Property="helpers:ThemeHelper.ReadOnlyBackgroundBrush" Value="{telerik1:Windows11Resource ResourceKey=ReadOnlyBackgroundBrush}"/>

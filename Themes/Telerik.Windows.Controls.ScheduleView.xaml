                        </StackPanel>
                    </ContentPresenter.Content>
                </ContentPresenter>
                <DataTemplate.Triggers>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikScheduleView:GroupHeaderButton}}, Path=IsPressed}" Value="True">
                        <Setter TargetName="DateTextBlock" Property="Foreground" Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikScheduleView:GroupHeaderButton}}}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikScheduleView:GroupHeaderButton}}, Path=IsMouseOver}" Value="True">
                        <Setter TargetName="DateTextBlock" Property="Foreground" Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikScheduleView:GroupHeaderButton}}}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikScheduleView:GroupHeaderButton}}, Path=IsToday}" Value="True">

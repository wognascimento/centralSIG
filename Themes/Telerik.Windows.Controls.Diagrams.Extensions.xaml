                            <Setter TargetName="OuterCircle" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush), Mode=OneWay}"/>
                        </Trigger>

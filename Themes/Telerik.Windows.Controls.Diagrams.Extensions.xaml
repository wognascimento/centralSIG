        <Setter Property="ExpandButtonStyle" Value="{StaticResource DiagramExpandToggleButtonStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:RadDiagramNavigationPane">
                    <Grid>
                        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid Visibility="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <extensions:RadDiagramThumbnail x:Name="Thumbnail" Style="{TemplateBinding ThumbnailStyle}" Diagram="{TemplateBinding Diagram}" BorderThickness="0"/>
                                </Grid>
                                <Grid Grid.Row="1" Background="{TemplateBinding Background}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <telerik:RadButton
                                            Visibility="{Binding IsAutofitButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            CommandTarget="{TemplateBinding Diagram}"
                                            Command="{x:Static telerik:DiagramCommands.AutoFit}"
                                            Margin="1 4"
                                            Style="{TemplateBinding AutofitButtonStyle}">
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{telerik:LocalizableResource Key=Auto_fit}"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadToggleButton x:Name="expandCollapseThumbnailButton"
                                            Grid.Column="1"
                                            Margin="1 4"
                                            IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                            Style="{TemplateBinding ExpandButtonStyle}"/>
                                    <Rectangle x:Name="Separator"
                                            Grid.Column="2"
                                            Width="1"
                                            Fill="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"
                                            Visibility="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            Margin="4"/>
                                    <TextBox
                                            Grid.Column="3"
                                            Background="Transparent"
                                            BorderBrush="Transparent"
                                            Visibility="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            FontSize="{telerik1:Windows11Resource ResourceKey=FontSize}"
                                            FontFamily="{telerik1:Windows11Resource ResourceKey=FontFamily}"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Padding="0"
                                            IsHitTestVisible="False"
                                            Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Diagram.Zoom, Converter={StaticResource percentageConverter}}"/>
                                    <telerik:RadSlider x:Name="ZoomSlider"
                                            Grid.Column="4"
                                            Width="170"
                                            Visibility="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Diagram.Zoom, Mode=TwoWay}"
                                            Style="{TemplateBinding SliderStyle}"/>
                                </Grid>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

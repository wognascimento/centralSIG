        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" x:Key="RadDiagramPrintPreviewStyle">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik1:Windows11Resource ResourceKey=OverlayCornerRadius}"/>
        <Setter Property="Width" Value="800"/>
        <Setter Property="Height" Value="740"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows11Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows11Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=PrimaryForegroundBrush}"/>
        <Setter Property="Background" Value="{telerik1:Windows11Resource ResourceKey=OverlayBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Windows11Resource ResourceKey=PrimaryBorderBrush}"/>
        <Setter Property="ItemStyle" Value="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
        <Setter Property="PrintScaleFactor" Value="{Binding ScaleFactor, Mode=TwoWay}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition MaxWidth="280"/>
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <ScrollViewer x:Name="PART_ScrollViewer" Background="Transparent" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" BorderThickness="0">
                                <telerik:RadUniformGrid x:Name="PART_PagesPanel" VerticalAlignment="Top" UseLayoutRounding="True" HorizontalAlignment="Left" Margin="8"/>
                            </ScrollViewer>
                            <StackPanel Grid.Row="1" MinHeight="36" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="5">
                                <TextBlock VerticalAlignment="Center" Text="{Binding Value, ElementName=ZoomSlider, Converter={StaticResource percentageConverter}}" Margin="0 0 8 0"/>
                                <telerik:RadSlider x:Name="ZoomSlider"
                                        Style="{StaticResource ThumbnailSliderStyle}"
                                        MinWidth="200"
                                        HandlesVisibility="Collapsed"
                                        Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ScaleDownFactor, Mode=TwoWay, Converter={StaticResource ReciprocalDoubleConverter}}"/>
                            </StackPanel>
                        </Grid>
                        <Border
                                Grid.Column="1"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="1 0 0 0"
                                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=BottomRight}">
                            <Grid Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <StackPanel Margin="15 15 15 0">
                                    <TextBlock HorizontalAlignment="Left" Text="{telerik:LocalizableResource Key=PrintPreview_Printer}"/>
                                    <telerik:RadComboBox
                                            Margin="0 10 0 0"
                                            ItemsSource="{Binding PrintQueues, Mode=OneWay}"
                                            SelectedItem="{Binding SelectedPrintQueue, Mode=TwoWay}"
                                            DisplayMemberPath="Name"
                                            Style="{StaticResource PrintPreviewComboStyle}"/>
                                </StackPanel>
                                <telerik:GroupBox Grid.Row="1" Header="{telerik:LocalizableResource Key=PrintPreview_Settings}" Margin="15 10 15 0">
                                    <StackPanel Margin="10">
                                        <TextBlock HorizontalAlignment="Left" Text="{telerik:LocalizableResource Key=PrintPreview_Orientation}"/>
                                        <telerik:RadComboBox
                                                Margin="0 10 0 0"
                                                ItemsSource="{Binding PageOrientations, Mode=OneWay}"
                                                SelectedItem="{Binding SelectedPageOrientation, Mode=TwoWay}"
                                                DisplayMemberPath=""
                                                Style="{StaticResource PrintPreviewComboStyle}"/>
                                        <TextBlock HorizontalAlignment="Left" Margin="0 10 0 0" Text="{telerik:LocalizableResource Key=PrintPreview_PaperSize}"/>
                                        <telerik:RadComboBox
                                                Margin="0 10 0 0"
                                                ItemsSource="{Binding PageSizes, Mode=OneWay}"
                                                SelectedItem="{Binding SelectedPageSize, Mode=TwoWay}"
                                                ItemTemplate="{StaticResource PageSizesItemTemplate}"
                                                SelectionBoxTemplate="{StaticResource PageSizesSelectedItemTemplate}"
                                                Style="{StaticResource PrintPreviewComboStyle}"/>
                                        <TextBlock Text="{telerik:LocalizableResource Key=PrintPreview_DiagramPosition}" Margin="0 10 0 0"/>
                                        <telerik:RadComboBox
                                                Margin="0 10 0 0"
                                                ItemsSource="{Binding DiagramPrintPositions}"
                                                SelectedItem="{Binding SelectedDiagramPrintPosition, Mode=TwoWay}"
                                                Style="{StaticResource PrintPreviewComboStyle}"/>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton x:Name="AdjustButton" Content="{telerik:LocalizableResource Key=PrintPreview_AdjustTo}" GroupName="FitGroup" VerticalAlignment="Center" IsChecked="True"/>
                                            <telerik:RadComboBox x:Name="PART_ZoomComboBox"
                                                    Grid.Column="1"
                                                    Margin="15 0 0 0"
                                                    IsEnabled="{Binding IsChecked, ElementName=AdjustButton}"
                                                    IsEditable="True"
                                                    UpdateSelectionOnLostFocus="True"
                                                    Text="100 %"
                                                    AutomationProperties.Name="ZoomComboBox"
                                                    VerticalAlignment="Center">
                                                <telerik:RadComboBoxItem Content="25 %"/>
                                                <telerik:RadComboBoxItem Content="50 %"/>
                                                <telerik:RadComboBoxItem Content="100 %"/>
                                                <telerik:RadComboBoxItem Content="200 %"/>
                                                <telerik:RadComboBoxItem Content="400 %"/>
                                            </telerik:RadComboBox>
                                        </Grid>
                                        <RadioButton x:Name="FitToRadioButton"
                                                Margin="0 10 0 0"
                                                GroupName="FitGroup"
                                                IsChecked="{Binding IsFitToEnabled, Mode=TwoWay}"
                                                Content="{telerik:LocalizableResource Key=PrintPreview_FitTo}"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"/>
                                        <StackPanel Orientation="Horizontal" Margin="0 10 0 0">
                                            <telerik:RadNumericUpDown x:Name="PagesHorizontalNumeric"
                                                    Minimum="1"
                                                    Maximum="50"
                                                    MinWidth="75"
                                                    Value="{Binding PagesHorizontalCount, Mode=TwoWay}"
                                                    IsInteger="True"
                                                    UpdateValueEvent="PropertyChanged"
                                                    IsEnabled="{Binding IsFitToEnabled}"/>
                                            <TextBlock Text="{telerik:LocalizableResource Key=PrintPreview_PagesHorizontally}" VerticalAlignment="Center" Margin="10 0 0 0"/>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Margin="0 10 0 0">
                                            <telerik:RadNumericUpDown x:Name="PagesVerticalNumeric"
                                                    Minimum="1"
                                                    Maximum="50"
                                                    MinWidth="75"
                                                    Value="{Binding PagesVerticalCount, Mode=TwoWay}"
                                                    IsInteger="True"
                                                    UpdateValueEvent="PropertyChanged"
                                                    IsEnabled="{Binding IsFitToEnabled}"/>
                                            <TextBlock Text="{telerik:LocalizableResource Key=PrintPreview_PagesVertically}" VerticalAlignment="Center" Margin="10 0 0 0"/>
                                        </StackPanel>
                                    </StackPanel>
                                </telerik:GroupBox>
                                <telerik:RadButton x:Name="PART_PrintButton"
                                        Grid.Row="3"
                                        HorizontalContentAlignment="Center"
                                        VerticalAlignment="Bottom"
                                        Margin="15 10 15 15"
                                        Content="{telerik:LocalizableResource Key=PrintPreview_Print}"/>
                            </Grid>
                        </Border>
                        <Border Grid.ColumnSpan="2" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0 1 0 0" VerticalAlignment="Top"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" BasedOn="{StaticResource RadDiagramPrintPreviewStyle}"/>
    <Style TargetType="extensions:RadDiagramPrintPreviewItem" BasedOn="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
</ResourceDictionary>

                                                    VerticalAlignment="Center"/>
                                        </StackPanel>
                                    </Border>
                                </Grid>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{telerik1:Windows11Resource ResourceKey=DisabledForegroundBrush}"/>
                            <Setter TargetName="ToolBoxHideablePartTitle" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="ToolBoxTitle" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="ViewGalleriesTb" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="SelectedTabContent" Property="Opacity" Value="{telerik1:Windows11Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:MoveAnimation AnimationName="ShowAnimation" TargetElementName="hideablePart" Duration="00:00:00.32">
                        <animation:MoveAnimation.Easing>
                            <CircleEase EasingMode="EaseOut"/>
                        </animation:MoveAnimation.Easing>
                    </animation:MoveAnimation>
                    <animation:MoveAnimation AnimationName="HideAnimation" TargetElementName="hideablePart" Duration="00:00:00.4">
                        <animation:MoveAnimation.Easing>
                            <CircleEase EasingMode="EaseOut"/>
                        </animation:MoveAnimation.Easing>
                    </animation:MoveAnimation>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
    </Style>
